312
00:18:52,281 --> 00:18:52,631


313
00:18:52,631 --> 00:19:07,296


314
00:19:07,296 --> 00:19:07,646


315
00:19:07,646 --> 00:19:22,277


316
00:19:22,278 --> 00:19:22,628


317
00:19:22,628 --> 00:19:37,276


318
00:19:37,276 --> 00:19:37,626


319
00:19:37,626 --> 00:19:52,274


320
00:19:52,274 --> 00:19:52,624


321
00:19:52,625 --> 00:20:07,273


322
00:20:07,273 --> 00:20:07,623


323
00:20:07,623 --> 00:20:22,271


324
00:20:22,271 --> 00:20:22,621


325
00:20:22,621 --> 00:20:37,286


326
00:20:37,286 --> 00:20:37,636


327
00:20:37,636 --> 00:20:52,284


328
00:20:52,284 --> 00:20:52,634


329
