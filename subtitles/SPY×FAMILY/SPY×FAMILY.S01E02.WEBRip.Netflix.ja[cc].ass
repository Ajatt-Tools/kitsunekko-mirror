Dialogue: 0,0:00:00.41,0:00:02.41,Default#3,,0,0,0,,♪～
Dialogue: 0,0:01:26.37,0:01:28.37,Default#3,,0,0,0,,～♪
Dialogue: 0,0:01:34.47,0:01:36.71,Default#3,,0,0,0,,（ロイド）ハア ダメだな
Dialogue: 0,0:01:36.80,0:01:38.04,Default#3,,0,0,0,,（フランキー）何！
Dialogue: 0,0:01:38.14,0:01:39.59,Default#3,,0,0,0,,（アーニャ）\Nアーニャ このはは やだ
Dialogue: 0,0:01:39.68,0:01:42.59,Default#3,,0,0,0,,お前に母親役は無理だったようだ\Nフランキー
Dialogue: 0,0:01:42.68,0:01:43.76,Default#3,,0,0,0,,（フランキー）お前の技術なら
Dialogue: 0,0:01:43.85,0:01:46.55,Default#3,,0,0,0,,もっとうまくできたんじゃないの\Nこれ？　ねえ
Dialogue: 0,0:01:46.64,0:01:48.76,Default#3,,0,0,0,,身長や体型には限度がある
Dialogue: 0,0:01:49.98,0:01:52.43,Default#3,,0,0,0,,せっかく協力してやったのに！
Dialogue: 0,0:01:52.53,0:01:54.73,Default#3,,0,0,0,,（アーニャ）ピーナツあげる\N（ロイド）オペレーション〈梟(ストリクス)〉
Dialogue: 0,0:01:54.82,0:01:58.06,Default#3,,0,0,0,,（ロイド）東国(オスタニア)の要人\Nデズモンドの戦争計画を止めるため
Dialogue: 0,0:01:58.16,0:02:02.19,Default#3,,0,0,0,,西国(ウェスタリス)情報部諜報員の\Nこの俺に下った極秘任務
Dialogue: 0,0:02:02.28,0:02:06.82,Default#3,,0,0,0,,名門イーデン校へ潜り込むべく\N筆記試験を突破したが
Dialogue: 0,0:02:07.50,0:02:08.78,Default#3,,0,0,0,,まさか次の面接で
Dialogue: 0,0:02:08.87,0:02:11.41,Default#3,,0,0,0,,父母そろっての出席が\N必須だったとは
Dialogue: 0,0:02:13.30,0:02:14.41,Default#3,,0,0,0,,しかたない
Dialogue: 0,0:02:15.13,0:02:16.41,Default#3,,0,0,0,,婚活するか
Dialogue: 0,0:02:17.51,0:02:18.42,Default#3,,0,0,0,,（カミラ）聞いた？
Dialogue: 0,0:02:18.51,0:02:20.67,Default#3,,0,0,0,,うちに\N泥棒入ったかもって
Dialogue: 0,0:02:20.76,0:02:23.63,Default#3,,0,0,0,,なんか\N女の個人情報入った棚ばっか
Dialogue: 0,0:02:23.72,0:02:25.30,Default#3,,0,0,0,,荒らされてたらしいよ
Dialogue: 0,0:02:25.39,0:02:27.93,Default#3,,0,0,0,,ヤバくない？　犯人キモくない？
Dialogue: 0,0:02:28.02,0:02:30.64,Default#3,,0,0,0,,（ミリー）\Nそれより課長のほうがキモいって
Dialogue: 0,0:02:30.73,0:02:33.26,Default#3,,0,0,0,,今朝もエロい目でジト～ってさ
Dialogue: 0,0:02:33.36,0:02:35.85,Default#3,,0,0,0,,（シャロン）\Nそんな短いスカートはいてるからよ
Dialogue: 0,0:02:35.94,0:02:38.56,Default#3,,0,0,0,,（ミリー）\Nえ～ だって彼氏が喜ぶし
Dialogue: 0,0:02:38.65,0:02:39.60,Default#3,,0,0,0,,フ～ッ
Dialogue: 0,0:02:39.70,0:02:41.06,Default#3,,0,0,0,,（ミリー）\Nシャロンもスタイルいいんだし
Dialogue: 0,0:02:41.16,0:02:42.65,Default#3,,0,0,0,,もっと出しなよ～
Dialogue: 0,0:02:42.74,0:02:44.73,Default#3,,0,0,0,,子供いると そんなことしない
Dialogue: 0,0:02:44.83,0:02:48.07,Default#3,,0,0,0,,ねえ ちょっと\Nヨル先輩はどう思います？
Dialogue: 0,0:02:48.16,0:02:49.86,Default#3,,0,0,0,,キモくないです？
Dialogue: 0,0:02:50.37,0:02:50.87,Default#3,,0,0,0,,（ヨル）はい？
Dialogue: 0,0:02:51.67,0:02:54.58,Default#3,,0,0,0,,（ミリー）それ 課長のコーヒー？\N鼻クソ入れちゃう？
Dialogue: 0,0:02:54.67,0:02:55.37,Default#3,,0,0,0,,えっ
Dialogue: 0,0:02:55.46,0:02:58.16,Default#3,,0,0,0,,鼻クソを入れると\Nおいしくなるのですか？
Dialogue: 0,0:02:59.80,0:03:02.46,Default#3,,0,0,0,,ヨル先輩ってなんていうか…
Dialogue: 0,0:03:02.55,0:03:04.17,Default#3,,0,0,0,,個性的ですよね
Dialogue: 0,0:03:04.26,0:03:05.09,Default#3,,0,0,0,,えっ
Dialogue: 0,0:03:05.18,0:03:06.30,Default#3,,0,0,0,,（ミリー）分かる～
Dialogue: 0,0:03:06.39,0:03:08.97,Default#3,,0,0,0,,異次元すぎて男 寄ってこなそう
Dialogue: 0,0:03:09.06,0:03:10.09,Default#3,,0,0,0,,ちょっと ミリー
Dialogue: 0,0:03:10.19,0:03:11.89,Default#3,,0,0,0,,（カミラ）\Nヨル先輩 元がいいんだから
Dialogue: 0,0:03:11.98,0:03:14.93,Default#3,,0,0,0,,おしゃれしたら\N絶対モテると思うんですよ
Dialogue: 0,0:03:15.02,0:03:15.68,Default#3,,0,0,0,,はあ…
Dialogue: 0,0:03:15.77,0:03:18.85,Default#3,,0,0,0,,（ミリー）今度 おすすめのコスメ\N教えてあげますよ～
Dialogue: 0,0:03:19.45,0:03:23.11,Default#3,,0,0,0,,（ヨル）私は仕事を続けられれば\Nそれで十分です
Dialogue: 0,0:03:23.20,0:03:26.07,Default#3,,0,0,0,,でも 先輩って27でしたっけ？
Dialogue: 0,0:03:26.16,0:03:28.15,Default#3,,0,0,0,,気をつけないとダメですよ
Dialogue: 0,0:03:28.25,0:03:29.74,Default#3,,0,0,0,,最近 スパイとか
Dialogue: 0,0:03:29.83,0:03:31.70,Default#3,,0,0,0,,多いみたいじゃ\Nないですか
Dialogue: 0,0:03:31.79,0:03:32.95,Default#3,,0,0,0,,だから\Nちょっとしたことで
Dialogue: 0,0:03:33.04,0:03:35.16,Default#3,,0,0,0,,通報とかも\N増えたらしくて
Dialogue: 0,0:03:35.25,0:03:38.50,Default#3,,0,0,0,,こないだ 30手前くらいの\N１人暮らしの女が
Dialogue: 0,0:03:38.59,0:03:42.63,Default#3,,0,0,0,,近所の人から怪しいって\N通報されちゃったらしいですよ？
Dialogue: 0,0:03:42.72,0:03:45.25,Default#3,,0,0,0,,（ミリー）え～ 何それ ウケる～
Dialogue: 0,0:03:45.35,0:03:47.42,Default#3,,0,0,0,,てか カミラ 超情報通じゃん
Dialogue: 0,0:03:47.51,0:03:50.05,Default#3,,0,0,0,,確かにその歳(とし)で\N独り身はないわね
Dialogue: 0,0:03:50.14,0:03:51.34,Default#3,,0,0,0,,怪しい 怪しい
Dialogue: 0,0:03:51.44,0:03:52.84,Default#3,,0,0,0,,そうなのですね
Dialogue: 0,0:03:53.35,0:03:55.22,Default#3,,0,0,0,,ご忠告 感謝します
Dialogue: 0,0:03:57.44,0:03:58.39,Default#3,,0,0,0,,そうだ！
Dialogue: 0,0:03:58.48,0:04:01.14,Default#3,,0,0,0,,今週末 うちで\Nパーティーやるんですよ
Dialogue: 0,0:04:01.24,0:04:03.48,Default#3,,0,0,0,,ヨル先輩も\Nよかったら来てください
Dialogue: 0,0:04:03.57,0:04:05.44,Default#3,,0,0,0,,ぜひ パートナーとご一緒に
Dialogue: 0,0:04:08.70,0:04:10.99,Default#3,,0,0,0,,（シャロン）\Nちょっとハードル高すぎじゃん
Dialogue: 0,0:04:11.08,0:04:12.70,Default#3,,0,0,0,,（カミラ）\Nうちに侵入したキモ犯人なら
Dialogue: 0,0:04:12.79,0:04:14.28,Default#3,,0,0,0,,誘われてくれるかもよ
Dialogue: 0,0:04:14.79,0:04:16.12,Default#3,,0,0,0,,（ミリー）ひどい～
Dialogue: 0,0:04:17.42,0:04:20.41,Default#3,,0,0,0,,（電話のベル）
Dialogue: 0,0:04:21.26,0:04:22.71,Default#3,,0,0,0,,（ヨル）はい ブライアです
Dialogue: 0,0:04:22.80,0:04:24.33,Default#3,,0,0,0,,（ユーリ）姉さん 元気？
Dialogue: 0,0:04:24.43,0:04:25.92,Default#3,,0,0,0,,（ヨル）ああ ユーリ
Dialogue: 0,0:04:26.01,0:04:27.46,Default#3,,0,0,0,,（ユーリ）どう？　仕事のほうは
Dialogue: 0,0:04:27.55,0:04:30.71,Default#3,,0,0,0,,大丈夫\Nちゃんと続けてますって
Dialogue: 0,0:04:30.81,0:04:33.18,Default#3,,0,0,0,,（ユーリ）姉さん\N変わってるから 心配なんだよ
Dialogue: 0,0:04:33.27,0:04:35.80,Default#3,,0,0,0,,（ヨル）ふ… 普通ですよ 失礼ね
Dialogue: 0,0:04:35.90,0:04:38.10,Default#3,,0,0,0,,（ユーリ）\Nそろそろ結婚とかどうなの？
Dialogue: 0,0:04:38.19,0:04:39.26,Default#3,,0,0,0,,いい人 いないの？
Dialogue: 0,0:04:39.36,0:04:40.85,Default#3,,0,0,0,,（ヨル）また その話…
Dialogue: 0,0:04:40.94,0:04:43.69,Default#3,,0,0,0,,（ユーリ）\N実は僕 昇進の話が来ててさ
Dialogue: 0,0:04:44.53,0:04:47.61,Default#3,,0,0,0,,これまで以上に 忙しく\N飛び回ることになりそうなんだ
Dialogue: 0,0:04:48.16,0:04:51.11,Default#3,,0,0,0,,でも 今のままじゃ\N姉さんのことほっとけないし
Dialogue: 0,0:04:51.20,0:04:52.70,Default#3,,0,0,0,,受けるかどうか迷ってる
Dialogue: 0,0:04:53.33,0:04:54.36,Default#3,,0,0,0,,今の僕があるのは
Dialogue: 0,0:04:54.46,0:04:56.95,Default#3,,0,0,0,,姉さんのおかげだから\N感謝してるんだ
Dialogue: 0,0:04:57.04,0:04:58.95,Default#3,,0,0,0,,だから 幸せになってほしいんだよ
Dialogue: 0,0:04:59.88,0:05:00.83,Default#3,,0,0,0,,分かってる
Dialogue: 0,0:05:01.51,0:05:02.96,Default#3,,0,0,0,,ありがとう ユーリ
Dialogue: 0,0:05:03.05,0:05:05.08,Default#3,,0,0,0,,（ユーリ）\Nよかったら誰か紹介するよ？
Dialogue: 0,0:05:05.18,0:05:07.63,Default#3,,0,0,0,,え！　だ… 大丈夫ですよ
Dialogue: 0,0:05:07.72,0:05:10.96,Default#3,,0,0,0,,じ… 実はね\N週末 パーティーに行くのです
Dialogue: 0,0:05:11.06,0:05:12.51,Default#3,,0,0,0,,もちろんパートナーと
Dialogue: 0,0:05:12.60,0:05:14.01,Default#3,,0,0,0,,（ユーリ）えっ 彼氏いたの？
Dialogue: 0,0:05:14.10,0:05:15.63,Default#3,,0,0,0,,（ヨル）そ… そうなの アハハ…
Dialogue: 0,0:05:15.73,0:05:17.30,Default#3,,0,0,0,,だから 安心して
Dialogue: 0,0:05:17.40,0:05:19.60,Default#3,,0,0,0,,（ユーリ）そうか よかった
Dialogue: 0,0:05:19.69,0:05:21.43,Default#3,,0,0,0,,それって\N職場のパーティーだよね？
Dialogue: 0,0:05:21.53,0:05:22.89,Default#3,,0,0,0,,そう カミラさんの
Dialogue: 0,0:05:22.99,0:05:23.52,Default#3,,0,0,0,,（ユーリ）ああ！
Dialogue: 0,0:05:24.36,0:05:26.06,Default#3,,0,0,0,,じゃあ あとで\Nドミニクさんに聞いて
Dialogue: 0,0:05:26.16,0:05:28.19,Default#3,,0,0,0,,どんな人だったか\N教えてもらおうっと
Dialogue: 0,0:05:28.28,0:05:28.90,Default#3,,0,0,0,,え？
Dialogue: 0,0:05:28.99,0:05:31.11,Default#3,,0,0,0,,（ユーリ）\N姉さん だまされやすいからね
Dialogue: 0,0:05:31.62,0:05:33.99,Default#3,,0,0,0,,ろくでもないヤツだったら\N追っ払わないと
Dialogue: 0,0:05:34.08,0:05:35.40,Default#3,,0,0,0,,（ヨル）えっ あの…
Dialogue: 0,0:05:35.50,0:05:36.86,Default#3,,0,0,0,,（ユーリ）\Nいいヤツだって分かるまで
Dialogue: 0,0:05:36.96,0:05:38.70,Default#3,,0,0,0,,昇進は保留にしとくから
Dialogue: 0,0:05:38.79,0:05:40.12,Default#3,,0,0,0,,（ヨル）\Nそんなことしなくても大丈…
Dialogue: 0,0:05:40.21,0:05:41.33,Default#3,,0,0,0,,（ユーリ）楽しみにしてるよ
Dialogue: 0,0:05:41.42,0:05:42.83,Default#3,,0,0,0,,じゃあね おやすみ
Dialogue: 0,0:05:43.55,0:05:44.46,Default#3,,0,0,0,,（通話が切れる音）
Dialogue: 0,0:05:44.55,0:05:46.58,Default#3,,0,0,0,,（不通音）
Dialogue: 0,0:05:46.68,0:05:48.67,Default#3,,0,0,0,,（ヨル）どうしましょう
Dialogue: 0,0:05:48.76,0:05:51.34,Default#3,,0,0,0,,パーティーまでに\N誰か探さなくては…
Dialogue: 0,0:05:51.43,0:05:52.92,Default#3,,0,0,0,,ウソだとバレたら
Dialogue: 0,0:05:53.02,0:05:55.42,Default#3,,0,0,0,,“姉さんは変人”に\N“虚言癖”まで加わって
Dialogue: 0,0:05:55.52,0:05:57.38,Default#3,,0,0,0,,更に信用を失う
Dialogue: 0,0:05:57.98,0:06:01.06,Default#3,,0,0,0,,弟の昇進のためにも\Nなんとかして いい人を…
Dialogue: 0,0:06:01.15,0:06:02.89,Default#3,,0,0,0,,（電話のベル）\N（ヨル）あっ！
Dialogue: 0,0:06:03.94,0:06:05.14,Default#3,,0,0,0,,ユーリ 違うんです！
Dialogue: 0,0:06:05.24,0:06:06.35,Default#3,,0,0,0,,さっきのは冗談で…
Dialogue: 0,0:06:06.45,0:06:09.56,Default#3,,0,0,0,,（店長）\Nおや 珍しく姉弟(きょうだい)ゲンカですか？
Dialogue: 0,0:06:10.24,0:06:12.23,Default#3,,0,0,0,,（ヨル）あっ 店長？
Dialogue: 0,0:06:12.33,0:06:13.90,Default#3,,0,0,0,,すみません 間違えました
Dialogue: 0,0:06:14.50,0:06:15.49,Default#3,,0,0,0,,（店長）こんばんは
Dialogue: 0,0:06:16.08,0:06:18.91,Default#3,,0,0,0,,“お客様”が入りましたよ\Nいばら姫
Dialogue: 0,0:06:20.46,0:06:24.70,Default#3,,0,0,0,,ロイヤルホテル 1307号室です
Dialogue: 0,0:06:24.80,0:06:26.50,Default#3,,0,0,0,,（到着音）
Dialogue: 0,0:06:28.47,0:06:29.92,Default#3,,0,0,0,,（黒服）すみません レディ
Dialogue: 0,0:06:30.01,0:06:33.75,Default#3,,0,0,0,,こちらのフロアは\N現在 貸し切りとなってまして
Dialogue: 0,0:06:33.85,0:06:35.51,Default#3,,0,0,0,,（黒服）誰か 女頼んだか？
Dialogue: 0,0:06:35.60,0:06:36.97,Default#3,,0,0,0,,（黒服）さあ
Dialogue: 0,0:06:37.06,0:06:39.43,Default#3,,0,0,0,,（ヨル）あの でも こちらに…
Dialogue: 0,0:06:39.52,0:06:43.14,Default#3,,0,0,0,,売国クソ野郎殿が\Nいらっしゃると聞きまして
Dialogue: 0,0:06:44.98,0:06:45.85,Default#3,,0,0,0,,（衝撃音）\N（黒服）うわあ！
Dialogue: 0,0:06:45.94,0:06:47.14,Default#3,,0,0,0,,（ブレナン）何事？
Dialogue: 0,0:06:47.24,0:06:49.40,Default#3,,0,0,0,,（黒服）\N賊です！　女が１人で… うわっ
Dialogue: 0,0:06:52.49,0:06:53.73,Default#3,,0,0,0,,（おびえる声）
Dialogue: 0,0:06:53.83,0:06:55.15,Default#3,,0,0,0,,（ヨル）あの…
Dialogue: 0,0:06:55.24,0:06:58.32,Default#3,,0,0,0,,監査局のブレナン次官ですよね？
Dialogue: 0,0:06:59.25,0:07:01.16,Default#3,,0,0,0,,大変恐縮なのですが…
Dialogue: 0,0:07:01.25,0:07:05.45,Default#3,,0,0,0,,息の根 止めさせていただいても\Nよろしいでしょうか？
Dialogue: 0,0:07:06.26,0:07:09.04,Default#3,,0,0,0,,（始末する音）
Dialogue: 0,0:07:09.13,0:07:11.17,Default#3,,0,0,0,,（ナレーション）\Nコードネーム“いばら姫”
Dialogue: 0,0:07:11.80,0:07:13.54,Default#3,,0,0,0,,女は殺し屋だった
Dialogue: 0,0:07:14.64,0:07:17.26,Default#3,,0,0,0,,幼少から殺人術をたたき込まれ
Dialogue: 0,0:07:17.35,0:07:21.47,Default#3,,0,0,0,,雇用主の命じるまま\N汚れ仕事を請け負い続けてきた
Dialogue: 0,0:07:22.10,0:07:23.93,Default#3,,0,0,0,,う～ん 落ちません
Dialogue: 0,0:07:25.61,0:07:28.60,Default#3,,0,0,0,,えっ ウソ！\Nどうしましょう
Dialogue: 0,0:07:28.69,0:07:31.19,Default#3,,0,0,0,,よそ行きは\Nこれしか持ってないのに
Dialogue: 0,0:07:31.28,0:07:33.23,Default#3,,0,0,0,,パーティーに行けない
Dialogue: 0,0:07:34.45,0:07:35.36,Default#3,,0,0,0,,（ため息）
Dialogue: 0,0:07:36.58,0:07:38.32,Default#3,,0,0,0,,（ユーリ）いい人 いないの？
Dialogue: 0,0:07:38.41,0:07:40.86,Default#3,,0,0,0,,（シャロン）\Nその歳で独り身はないわね
Dialogue: 0,0:07:40.96,0:07:43.49,Default#3,,0,0,0,,（カミラ）\Nぜひ パートナーとご一緒に
Dialogue: 0,0:07:44.63,0:07:46.08,Default#3,,0,0,0,,無理ですよ
Dialogue: 0,0:07:46.17,0:07:50.33,Default#3,,0,0,0,,私 家事は…\Nお片づけしかできませんもの
Dialogue: 0,0:07:52.09,0:07:53.21,Default#3,,0,0,0,,（ドアが開く音）
Dialogue: 0,0:07:53.30,0:07:54.25,Default#3,,0,0,0,,（アーニャ）モジャモジャ！
Dialogue: 0,0:07:55.80,0:07:58.76,Default#3,,0,0,0,,役所から独身女のリスト\Nコピってきたよ
Dialogue: 0,0:07:58.85,0:07:59.67,Default#3,,0,0,0,,助かる
Dialogue: 0,0:08:00.23,0:08:01.63,Default#3,,0,0,0,,（フランキー）\N子持ちバツイチＯＫで
Dialogue: 0,0:08:01.73,0:08:03.76,Default#3,,0,0,0,,名門にふさわしい\N気品があり
Dialogue: 0,0:08:03.85,0:08:06.89,Default#3,,0,0,0,,48時間以内に\N手続き可能な女？
Dialogue: 0,0:08:07.52,0:08:10.02,Default#3,,0,0,0,,そんな女神がいたら\N拝んでみたいね
Dialogue: 0,0:08:10.11,0:08:13.40,Default#3,,0,0,0,,俺なんて えり好みしてないのに\Nデートすら叶(かな)わんての
Dialogue: 0,0:08:13.95,0:08:14.81,Default#3,,0,0,0,,かわいそうに
Dialogue: 0,0:08:14.91,0:08:16.19,Default#3,,0,0,0,,かわいそうって言うな！
Dialogue: 0,0:08:17.03,0:08:18.86,Default#3,,0,0,0,,（アーニャ）子連れ 人気ない？
Dialogue: 0,0:08:19.37,0:08:21.03,Default#3,,0,0,0,,アーニャ 邪魔な子供？
Dialogue: 0,0:08:22.29,0:08:23.49,Default#3,,0,0,0,,（ロイド）邪魔じゃないよ
Dialogue: 0,0:08:24.00,0:08:25.99,Default#3,,0,0,0,,心配せずにテレビでも見てろ
Dialogue: 0,0:08:26.08,0:08:26.70,Default#3,,0,0,0,,（アーニャ）うい
Dialogue: 0,0:08:32.59,0:08:34.21,Default#3,,0,0,0,,（「SPY(スパイ) WARS(ウォーズ)」のテーマ）
Dialogue: 0,0:08:34.30,0:08:34.92,Default#3,,0,0,0,,（フランキー）つうかさ
Dialogue: 0,0:08:35.01,0:08:38.46,Default#3,,0,0,0,,なんで お前のとこの\N女諜報員とか使わねえの？
Dialogue: 0,0:08:38.56,0:08:41.38,Default#3,,0,0,0,,（ロイド）先のスパイ狩りで\Nだいぶ やられたからな
Dialogue: 0,0:08:41.48,0:08:42.63,Default#3,,0,0,0,,適任がいない
Dialogue: 0,0:08:42.73,0:08:45.26,Default#3,,0,0,0,,（フランキー）\Nああ 今 密告ブームだからな
Dialogue: 0,0:08:45.35,0:08:46.51,Default#3,,0,0,0,,人手不足すぎて
Dialogue: 0,0:08:46.61,0:08:49.77,Default#3,,0,0,0,,新たに別の任務が\N俺の所に回ってきた
Dialogue: 0,0:08:49.86,0:08:52.14,Default#3,,0,0,0,,とある密輸組織の壊滅だとさ
Dialogue: 0,0:08:52.65,0:08:54.60,Default#3,,0,0,0,,どんだけブラックだよ
Dialogue: 0,0:08:54.70,0:08:56.69,Default#3,,0,0,0,,ならもう\Nちゃっちゃと済ませようぜ
Dialogue: 0,0:08:56.78,0:09:00.69,Default#3,,0,0,0,,とりあえず手っとり早いのは\N利害が一致するワケアリさんだ
Dialogue: 0,0:09:00.79,0:09:04.28,Default#3,,0,0,0,,弱みを握るのもアリだな\Nないなら捏造(ねつぞう)したっていい
Dialogue: 0,0:09:04.37,0:09:06.70,Default#3,,0,0,0,,なるべくリスクは避けたい
Dialogue: 0,0:09:06.79,0:09:09.79,Default#3,,0,0,0,,（フランキー）何言ってんだ\Nあれだって相当リスキーだろ？
Dialogue: 0,0:09:10.46,0:09:13.08,Default#3,,0,0,0,,とてもじゃないが\N良家のご令嬢には見えないぞ
Dialogue: 0,0:09:14.47,0:09:15.83,Default#3,,0,0,0,,そうだな
Dialogue: 0,0:09:15.93,0:09:18.29,Default#3,,0,0,0,,外見だけでも なんとかせんと
Dialogue: 0,0:09:20.97,0:09:21.59,Default#3,,0,0,0,,おお
Dialogue: 0,0:09:21.68,0:09:23.72,Default#3,,0,0,0,,（女主人）\Nでは 採寸いたしますので
Dialogue: 0,0:09:23.81,0:09:25.30,Default#3,,0,0,0,,お嬢様はこちらへ
Dialogue: 0,0:09:25.39,0:09:27.68,Default#3,,0,0,0,,アーニャ 売り飛ばされる？
Dialogue: 0,0:09:27.77,0:09:29.76,Default#3,,0,0,0,,（ロイド）\Nいい子にしてたら売らない
Dialogue: 0,0:09:29.86,0:09:33.64,Default#3,,0,0,0,,ったく どこでそんな言葉\N覚えてくるんだか
Dialogue: 0,0:09:36.95,0:09:39.57,Default#3,,0,0,0,,（ロイド）針子(はりこ)は既婚者か
Dialogue: 0,0:09:39.66,0:09:43.44,Default#3,,0,0,0,,女主人は\Nさっきの独身者リストにあったが
Dialogue: 0,0:09:43.54,0:09:46.78,Default#3,,0,0,0,,過去に一度\N政治運動での逮捕歴があり
Dialogue: 0,0:09:46.87,0:09:48.28,Default#3,,0,0,0,,危険度が高い
Dialogue: 0,0:09:48.96,0:09:51.83,Default#3,,0,0,0,,好条件となると\Nなかなかうまくいかな…
Dialogue: 0,0:09:54.38,0:09:55.66,Default#3,,0,0,0,,すみませ～ん
Dialogue: 0,0:09:56.30,0:09:59.00,Default#3,,0,0,0,,（店員）あら ヨルちゃん 久しぶり
Dialogue: 0,0:09:59.09,0:10:00.17,Default#3,,0,0,0,,（ヨル）こんにちは
Dialogue: 0,0:10:00.26,0:10:02.42,Default#3,,0,0,0,,ドレスのお直し\N頼みたいのですが
Dialogue: 0,0:10:02.93,0:10:04.55,Default#3,,0,0,0,,急ぎでお願いできます？
Dialogue: 0,0:10:04.64,0:10:07.59,Default#3,,0,0,0,,（店員）\Nもちろん お得意様ですもの
Dialogue: 0,0:10:07.69,0:10:09.72,Default#3,,0,0,0,,（ロイド）たやすく俺の背後を？
Dialogue: 0,0:10:09.81,0:10:10.85,Default#3,,0,0,0,,何者だ？
Dialogue: 0,0:10:10.94,0:10:12.76,Default#3,,0,0,0,,ヨル… ヨル…
Dialogue: 0,0:10:13.44,0:10:14.56,Default#3,,0,0,0,,あった
Dialogue: 0,0:10:14.65,0:10:16.48,Default#3,,0,0,0,,ヨル･ブライア 27歳
Dialogue: 0,0:10:17.03,0:10:18.48,Default#3,,0,0,0,,結婚 離婚歴なし
Dialogue: 0,0:10:18.99,0:10:22.48,Default#3,,0,0,0,,両親は共に他界\N年の離れた弟が１人
Dialogue: 0,0:10:23.04,0:10:25.94,Default#3,,0,0,0,,２人とも公務員で\N経歴に怪しい点はない
Dialogue: 0,0:10:26.75,0:10:28.36,Default#3,,0,0,0,,ただの油断か
Dialogue: 0,0:10:28.46,0:10:30.12,Default#3,,0,0,0,,最近 気が緩んで…
Dialogue: 0,0:10:30.21,0:10:30.82,Default#3,,0,0,0,,（ヨル）あの…
Dialogue: 0,0:10:30.92,0:10:33.33,Default#3,,0,0,0,,先ほどからジロジロと…
Dialogue: 0,0:10:33.42,0:10:35.04,Default#3,,0,0,0,,何かご用ですか？
Dialogue: 0,0:10:35.13,0:10:38.33,Default#3,,0,0,0,,（ロイド）バカな！\N視線まで気取られただと？
Dialogue: 0,0:10:38.43,0:10:40.46,Default#3,,0,0,0,,（ロイド）きれいな方だなと思って
Dialogue: 0,0:10:41.55,0:10:42.59,Default#3,,0,0,0,,それは
Dialogue: 0,0:10:42.68,0:10:45.76,Default#3,,0,0,0,,私の容姿に\N好感をお持ちということで？
Dialogue: 0,0:10:45.85,0:10:47.63,Default#3,,0,0,0,,えっ まあ はい
Dialogue: 0,0:10:47.73,0:10:50.14,Default#3,,0,0,0,,（カミラ）\Nヨル先輩 元がいいんだから
Dialogue: 0,0:10:50.23,0:10:52.85,Default#3,,0,0,0,,絶対モテると思うんですよ
Dialogue: 0,0:10:52.94,0:10:53.97,Default#3,,0,0,0,,（ヨル）あの…
Dialogue: 0,0:10:54.07,0:10:57.48,Default#3,,0,0,0,,（アーニャ）ちち～\Nアーニャの長さ 判明した！
Dialogue: 0,0:10:57.57,0:10:59.27,Default#3,,0,0,0,,あっ 誰？
Dialogue: 0,0:10:59.36,0:11:00.44,Default#3,,0,0,0,,（ロイド）ほかのお客さん
Dialogue: 0,0:11:00.53,0:11:01.86,Default#3,,0,0,0,,（ヨル）子連れだった
Dialogue: 0,0:11:02.49,0:11:06.36,Default#3,,0,0,0,,危うく人様の配偶者を\Nお誘いしてしまうところでした
Dialogue: 0,0:11:06.45,0:11:07.24,Default#3,,0,0,0,,そういった行為は
Dialogue: 0,0:11:07.33,0:11:10.24,Default#3,,0,0,0,,奥様に殺されると\N聞いたことがあります
Dialogue: 0,0:11:10.33,0:11:12.49,Default#3,,0,0,0,,まあ 私なら殺し返しますけれど
Dialogue: 0,0:11:13.00,0:11:13.91,Default#3,,0,0,0,,えっ
Dialogue: 0,0:11:14.00,0:11:14.99,Default#3,,0,0,0,,（ヨル）いけません
Dialogue: 0,0:11:15.09,0:11:19.41,Default#3,,0,0,0,,こんな考えでは いずれ誰かに\N殺し屋だと見抜かれてしまいます
Dialogue: 0,0:11:19.51,0:11:21.83,Default#3,,0,0,0,,もっと普通の人にならなくては
Dialogue: 0,0:11:21.93,0:11:23.59,Default#3,,0,0,0,,（アーニャ）こ… 殺し屋！
Dialogue: 0,0:11:24.43,0:11:26.17,Default#3,,0,0,0,,スパイ 殺し屋
Dialogue: 0,0:11:26.68,0:11:27.63,Default#3,,0,0,0,,アーニャ…
Dialogue: 0,0:11:28.14,0:11:29.22,Default#3,,0,0,0,,わくわく！
Dialogue: 0,0:11:29.31,0:11:31.55,Default#3,,0,0,0,,（ナレーション）\N少女は娯楽に飢えていた
Dialogue: 0,0:11:31.65,0:11:32.93,Default#3,,0,0,0,,（ロイド）う～ん
Dialogue: 0,0:11:33.02,0:11:36.01,Default#3,,0,0,0,,あわよくば嫁役にと思ったが\Nやめておこう
Dialogue: 0,0:11:36.11,0:11:36.72,Default#3,,0,0,0,,（アーニャ）ハッ！
Dialogue: 0,0:11:36.82,0:11:40.39,Default#3,,0,0,0,,（ヨル）あわよくば パーティーで\N恋人役をと思いましたが
Dialogue: 0,0:11:40.49,0:11:42.60,Default#3,,0,0,0,,シュラバとやらは回避せねば
Dialogue: 0,0:11:42.70,0:11:43.31,Default#3,,0,0,0,,（アーニャ）ハッ！
Dialogue: 0,0:11:44.32,0:11:45.48,Default#3,,0,0,0,,（アーニャ）あ～
Dialogue: 0,0:11:45.58,0:11:49.11,Default#3,,0,0,0,,アーニャ ははいなくて寂しい～
Dialogue: 0,0:11:49.71,0:11:50.86,Default#3,,0,0,0,,（ロイド）どうした？　急に
Dialogue: 0,0:11:50.96,0:11:53.95,Default#3,,0,0,0,,ははの存在 恋し～
Dialogue: 0,0:11:54.04,0:11:55.95,Default#3,,0,0,0,,奥様はご一緒では？
Dialogue: 0,0:11:56.04,0:11:57.12,Default#3,,0,0,0,,ああ いや…
Dialogue: 0,0:11:57.71,0:12:00.00,Default#3,,0,0,0,,妻とは２年前 死別しまして
Dialogue: 0,0:12:00.09,0:12:02.79,Default#3,,0,0,0,,今は男手だけで\Nこいつを育ててます
Dialogue: 0,0:12:02.88,0:12:03.92,Default#3,,0,0,0,,あっ
Dialogue: 0,0:12:04.01,0:12:06.00,Default#3,,0,0,0,,（ヨル）誘っても殺されない？
Dialogue: 0,0:12:07.26,0:12:08.59,Default#3,,0,0,0,,あの…
Dialogue: 0,0:12:11.39,0:12:12.97,Default#3,,0,0,0,,（ロイド）恋人のフリ？
Dialogue: 0,0:12:13.48,0:12:15.01,Default#3,,0,0,0,,そうなんです
Dialogue: 0,0:12:15.11,0:12:18.39,Default#3,,0,0,0,,弟に恋人がいると偽ってしまって
Dialogue: 0,0:12:18.48,0:12:19.89,Default#3,,0,0,0,,ご迷惑でなければ
Dialogue: 0,0:12:19.99,0:12:22.39,Default#3,,0,0,0,,ご一緒にパーティーに\N出ていただけないかと
Dialogue: 0,0:12:23.03,0:12:26.61,Default#3,,0,0,0,,あの 変な下心とか\N全くありませんので
Dialogue: 0,0:12:26.70,0:12:28.48,Default#3,,0,0,0,,もちろん お礼もいたします
Dialogue: 0,0:12:29.41,0:12:32.82,Default#3,,0,0,0,,私はただ 弟を安心させたくて
Dialogue: 0,0:12:34.25,0:12:36.95,Default#3,,0,0,0,,分かりました\N引き受けましょう
Dialogue: 0,0:12:37.84,0:12:38.99,Default#3,,0,0,0,,ホントですか？
Dialogue: 0,0:12:39.09,0:12:41.16,Default#3,,0,0,0,,（ロイド）ただし 交換条件が
Dialogue: 0,0:12:44.55,0:12:46.17,Default#3,,0,0,0,,というわけでして
Dialogue: 0,0:12:46.26,0:12:49.71,Default#3,,0,0,0,,代わりにこちらも\N面接時の母親役を頼みたい
Dialogue: 0,0:12:49.81,0:12:51.38,Default#3,,0,0,0,,（ヨル）母親ですか？
Dialogue: 0,0:12:52.06,0:12:54.05,Default#3,,0,0,0,,先の見えないこの時代
Dialogue: 0,0:12:54.14,0:12:57.22,Default#3,,0,0,0,,娘には どうしても\Nいい学校に入ってもらいたくて
Dialogue: 0,0:12:57.90,0:13:01.35,Default#3,,0,0,0,,それが亡き妻の\N遺志でもあるのです
Dialogue: 0,0:13:01.94,0:13:03.31,Default#3,,0,0,0,,（アーニャ）ちち ウソつき
Dialogue: 0,0:13:03.40,0:13:05.19,Default#3,,0,0,0,,（ヨル）なんていい人！
Dialogue: 0,0:13:06.32,0:13:08.44,Default#3,,0,0,0,,（ロイド）\Nまずは小さな要求をのませ
Dialogue: 0,0:13:08.53,0:13:11.15,Default#3,,0,0,0,,最終的に正式な婚姻を\N承諾させてやる
Dialogue: 0,0:13:11.24,0:13:12.94,Default#3,,0,0,0,,私に務まることであれば
Dialogue: 0,0:13:13.54,0:13:14.40,Default#3,,0,0,0,,ありがとう
Dialogue: 0,0:13:14.50,0:13:16.74,Default#3,,0,0,0,,では まず土曜のパーティーで
Dialogue: 0,0:13:16.83,0:13:17.62,Default#3,,0,0,0,,はい！
Dialogue: 0,0:13:20.17,0:13:22.45,Default#3,,0,0,0,,（ロイド）土曜は\Nシッターさん 頼んでおいたから
Dialogue: 0,0:13:22.55,0:13:23.87,Default#3,,0,0,0,,おとなしくしてろよ
Dialogue: 0,0:13:24.42,0:13:25.96,Default#3,,0,0,0,,ピーナツ買い込んどけ
Dialogue: 0,0:13:26.05,0:13:27.63,Default#3,,0,0,0,,（ロイド）分かった 分かった
Dialogue: 0,0:13:27.72,0:13:29.59,Default#3,,0,0,0,,（店員）\Nはい 10ペントのお釣りケロ
Dialogue: 0,0:13:29.68,0:13:30.38,Default#3,,0,0,0,,（ロイド）あっ
Dialogue: 0,0:13:31.22,0:13:32.30,Default#3,,0,0,0,,（店員）まいどケロ
Dialogue: 0,0:13:32.39,0:13:33.17,Default#3,,0,0,0,,（アーニャ）ケロ？
Dialogue: 0,0:13:34.73,0:13:38.14,Default#3,,0,0,0,,（ロイド）Ｔ… いや Ｆ暗号か
Dialogue: 0,0:13:43.74,0:13:45.64,Default#3,,0,0,0,,追加任務の詳細か
Dialogue: 0,0:13:46.66,0:13:49.31,Default#3,,0,0,0,,“西側から盗まれた\N美術品の回収⸺”
Dialogue: 0,0:13:49.41,0:13:51.73,Default#3,,0,0,0,,“および\N密輸組織の壊滅”
Dialogue: 0,0:13:51.83,0:13:53.03,Default#3,,0,0,0,,“実行は標的が⸺”
Dialogue: 0,0:13:53.12,0:13:55.65,Default#3,,0,0,0,,“バイヤーに\N接触する土曜18時”
Dialogue: 0,0:13:56.67,0:13:57.49,Default#3,,0,0,0,,土曜？
Dialogue: 0,0:14:00.04,0:14:01.16,Default#3,,0,0,0,,（ロイド）というわけで
Dialogue: 0,0:14:01.75,0:14:04.70,Default#3,,0,0,0,,パーティーに間に合わせるために\N速攻で片づける
Dialogue: 0,0:14:04.80,0:14:05.66,Default#3,,0,0,0,,手伝え
Dialogue: 0,0:14:05.76,0:14:06.58,Default#3,,0,0,0,,待て 待て！
Dialogue: 0,0:14:06.68,0:14:09.88,Default#3,,0,0,0,,俺はただの情報屋であって\N戦闘力はゴミだって！
Dialogue: 0,0:14:09.97,0:14:12.21,Default#3,,0,0,0,,しかも お前\Nいろんな代金を踏み倒して…
Dialogue: 0,0:14:12.31,0:14:15.84,Default#3,,0,0,0,,（ロイド）\N総額３百万ダルク相当の美術品78点
Dialogue: 0,0:14:16.43,0:14:19.93,Default#3,,0,0,0,,１つや２つ なくなっても\N見つからんだろうな
Dialogue: 0,0:14:21.31,0:14:22.31,Default#3,,0,0,0,,俺に任せろ
Dialogue: 0,0:14:22.90,0:14:24.72,Default#3,,0,0,0,,こんな日のため\N新たなスパイグッズを
Dialogue: 0,0:14:24.82,0:14:26.94,Default#3,,0,0,0,,発明したこの俺
Dialogue: 0,0:14:27.03,0:14:28.02,Default#3,,0,0,0,,（ロイド）便利なヤツ
Dialogue: 0,0:14:33.74,0:14:36.03,Default#3,,0,0,0,,ロイドさん 遅いですね
Dialogue: 0,0:14:37.29,0:14:39.91,Default#3,,0,0,0,,事故など起こしていないと\Nよいですが…
Dialogue: 0,0:14:40.79,0:14:42.03,Default#3,,0,0,0,,（爆発音）
Dialogue: 0,0:14:42.13,0:14:44.12,Default#3,,0,0,0,,（フランキー）ひええええ！\N（手下）待て こら！
Dialogue: 0,0:14:44.21,0:14:45.50,Default#3,,0,0,0,,（手下）どこのもんじゃあ！
Dialogue: 0,0:14:46.13,0:14:48.54,Default#3,,0,0,0,,（フランキー）\Nこんな大群なんて聞いてないぞ！
Dialogue: 0,0:14:49.09,0:14:51.88,Default#3,,0,0,0,,（ロイド）２ ４ ６… 38人か
Dialogue: 0,0:14:51.97,0:14:54.21,Default#3,,0,0,0,,１人当たり10秒もかけてられん！
Dialogue: 0,0:14:54.31,0:14:55.76,Default#3,,0,0,0,,（手下たち）ぐあっ うわあ！
Dialogue: 0,0:14:56.39,0:14:58.59,Default#3,,0,0,0,,（発砲音）\N（手下たち）うわ！　ぐあっ…
Dialogue: 0,0:14:59.77,0:15:01.80,Default#3,,0,0,0,,（手下たち）がっ！　ぐああっ！
Dialogue: 0,0:15:02.65,0:15:04.39,Default#3,,0,0,0,,（ロイド）増援か キリがない！
Dialogue: 0,0:15:04.48,0:15:06.02,Default#3,,0,0,0,,（フランキー）\N積み込み終わったぞ 乗れ！
Dialogue: 0,0:15:06.65,0:15:08.10,Default#3,,0,0,0,,（エンジン音）
Dialogue: 0,0:15:11.16,0:15:12.69,Default#3,,0,0,0,,（フランキー）うっひょ～！
Dialogue: 0,0:15:12.78,0:15:15.69,Default#3,,0,0,0,,さて どれを頂戴しようかな
Dialogue: 0,0:15:16.33,0:15:17.07,Default#3,,0,0,0,,あっ
Dialogue: 0,0:15:18.62,0:15:19.82,Default#3,,0,0,0,,こいつは俺がもらう
Dialogue: 0,0:15:19.91,0:15:22.66,Default#3,,0,0,0,,（フランキー）あっ ダイヤ！\Nズルいぞ 高そうなやつ！
Dialogue: 0,0:15:22.75,0:15:24.12,Default#3,,0,0,0,,（ロイド）偽装のときに使うんだ
Dialogue: 0,0:15:24.21,0:15:25.62,Default#3,,0,0,0,,（フランキー）うるさい 返せ！
Dialogue: 0,0:15:27.55,0:15:28.12,Default#3,,0,0,0,,（銃声）
Dialogue: 0,0:15:29.80,0:15:30.54,Default#3,,0,0,0,,（衝撃音）
Dialogue: 0,0:15:37.68,0:15:41.93,Default#3,,0,0,0,,（ヨル）なるほど\Nこれが弄ばれたというやつですね
Dialogue: 0,0:15:43.15,0:15:47.56,Default#3,,0,0,0,,弟のためにも\N職場の人間関係は良好に保たねば
Dialogue: 0,0:15:48.40,0:15:51.35,Default#3,,0,0,0,,故にパーティーを\N欠席するわけにはいきません
Dialogue: 0,0:15:54.66,0:15:57.07,Default#3,,0,0,0,,遅いです ヨル先輩！
Dialogue: 0,0:15:57.70,0:16:00.61,Default#3,,0,0,0,,すみません\Nこれ 差し入れです
Dialogue: 0,0:16:00.71,0:16:01.61,Default#3,,0,0,0,,あれ～？
Dialogue: 0,0:16:01.71,0:16:04.87,Default#3,,0,0,0,,彼氏と一緒に来るって\N聞いてましたけど？
Dialogue: 0,0:16:04.96,0:16:07.49,Default#3,,0,0,0,,（ヨル）なんだか\N急用が入ってしまわれたようです
Dialogue: 0,0:16:07.59,0:16:10.62,Default#3,,0,0,0,,（カミラ）\Nえ～ 残念！　見たかった～！
Dialogue: 0,0:16:10.72,0:16:12.62,Default#3,,0,0,0,,何 その言い訳
Dialogue: 0,0:16:12.72,0:16:14.04,Default#3,,0,0,0,,（ミリー）苦しすぎ～
Dialogue: 0,0:16:14.14,0:16:17.38,Default#3,,0,0,0,,（シャロン）あの人が男とか\N絶対ハッタリだと思ったわ
Dialogue: 0,0:16:17.47,0:16:19.09,Default#3,,0,0,0,,（ミリー）\Nウソつきは通報しちゃう？
Dialogue: 0,0:16:19.18,0:16:20.13,Default#3,,0,0,0,,（カミラ）そうよ
Dialogue: 0,0:16:20.23,0:16:23.09,Default#3,,0,0,0,,あれは きっと我が国の\N出生率を下げるために
Dialogue: 0,0:16:23.19,0:16:25.26,Default#3,,0,0,0,,遣わされたスパイなのよ
Dialogue: 0,0:16:25.36,0:16:27.22,Default#3,,0,0,0,,（シャロン）何それ しょうもな
Dialogue: 0,0:16:27.32,0:16:30.14,Default#3,,0,0,0,,ただのモテない女なんでしょ\Nほっときなさい
Dialogue: 0,0:16:30.24,0:16:33.31,Default#3,,0,0,0,,（ドミニク）いや マジで\N楽しみにしてたんだけどな
Dialogue: 0,0:16:33.41,0:16:34.69,Default#3,,0,0,0,,ヨルさんの彼
Dialogue: 0,0:16:35.24,0:16:36.61,Default#3,,0,0,0,,ド… ドミニクさん
Dialogue: 0,0:16:36.70,0:16:40.07,Default#3,,0,0,0,,ユーリ君 いつも\Nヨルさんのこと心配してたからさ
Dialogue: 0,0:16:41.04,0:16:43.11,Default#3,,0,0,0,,あの… 弟にはその…
Dialogue: 0,0:16:43.67,0:16:46.66,Default#3,,0,0,0,,いい人 連れてきたって\N伝えといてくれませんか？
Dialogue: 0,0:16:46.75,0:16:48.66,Default#3,,0,0,0,,え～ ないない！
Dialogue: 0,0:16:48.75,0:16:51.75,Default#3,,0,0,0,,そこまでして\N見栄(みえ)張りたいんですか 先輩
Dialogue: 0,0:16:51.84,0:16:54.04,Default#3,,0,0,0,,余計 惨めになりますよ
Dialogue: 0,0:16:54.13,0:16:55.46,Default#3,,0,0,0,,（ドミニク）おいよせ カミラ
Dialogue: 0,0:16:55.55,0:16:57.59,Default#3,,0,0,0,,（カミラ）\N人の男と勝手にしゃべんな
Dialogue: 0,0:16:57.68,0:17:02.13,Default#3,,0,0,0,,弟君には正直に\Nぼっちで来たって言っとくわ
Dialogue: 0,0:17:02.23,0:17:06.30,Default#3,,0,0,0,,（ヨル）そんなことをして\Nこの方に何か得があるのでしょうか
Dialogue: 0,0:17:07.36,0:17:09.10,Default#3,,0,0,0,,なんかもう面倒ですね
Dialogue: 0,0:17:09.69,0:17:12.31,Default#3,,0,0,0,,ここにいる大人\N全員お亡くなりになれば
Dialogue: 0,0:17:12.94,0:17:14.81,Default#3,,0,0,0,,弟に伝わることも…
Dialogue: 0,0:17:14.90,0:17:17.69,Default#3,,0,0,0,,（ドミニク）ごめんね\Nまあ 楽しんでって
Dialogue: 0,0:17:18.28,0:17:20.94,Default#3,,0,0,0,,ほら カミラ\Nグラタン焼くの手伝ってくれ
Dialogue: 0,0:17:25.17,0:17:26.87,Default#3,,0,0,0,,（女）でさ その男がね…
Dialogue: 0,0:17:26.96,0:17:27.70,Default#3,,0,0,0,,（女の子）ママ～
Dialogue: 0,0:17:29.79,0:17:32.62,Default#3,,0,0,0,,（シャロン）\Nこの上の子 今年受験なのよ
Dialogue: 0,0:17:32.71,0:17:33.96,Default#3,,0,0,0,,名門イーデンの
Dialogue: 0,0:17:35.59,0:17:38.17,Default#3,,0,0,0,,（女）へ～ すごい\N（男）エリート確定…
Dialogue: 0,0:17:38.26,0:17:42.55,Default#3,,0,0,0,,（ヨル）あれがきっと\N“普通”なのでしょうね
Dialogue: 0,0:17:42.64,0:17:47.51,Default#3,,0,0,0,,弟は私に あんなふうに\Nなってほしかったのでしょう
Dialogue: 0,0:17:47.60,0:17:50.05,Default#3,,0,0,0,,ねえ あの人 まだいるよ
Dialogue: 0,0:17:50.15,0:17:51.89,Default#3,,0,0,0,,（ミリー）メンタル強すぎじゃね？
Dialogue: 0,0:17:51.98,0:17:54.52,Default#3,,0,0,0,,てか そもそも\Nよく１人で来れたよね
Dialogue: 0,0:17:55.15,0:17:56.14,Default#3,,0,0,0,,（ヨル）なるほど
Dialogue: 0,0:17:56.70,0:17:59.69,Default#3,,0,0,0,,私には分不相応な\N場所だったようです
Dialogue: 0,0:18:01.87,0:18:03.53,Default#3,,0,0,0,,（ヨル）申し訳ありません
Dialogue: 0,0:18:03.62,0:18:05.90,Default#3,,0,0,0,,私 ここでおいとま…
Dialogue: 0,0:18:08.00,0:18:08.57,Default#3,,0,0,0,,あっ
Dialogue: 0,0:18:09.50,0:18:11.41,Default#3,,0,0,0,,遅くなり申し訳ない
Dialogue: 0,0:18:12.05,0:18:14.41,Default#3,,0,0,0,,ヨルの夫の\Nロイド･フォージャーです
Dialogue: 0,0:18:16.13,0:18:16.92,Default#3,,0,0,0,,あの…
Dialogue: 0,0:18:17.01,0:18:19.33,Default#3,,0,0,0,,夫でなく恋人でよいのですが
Dialogue: 0,0:18:19.43,0:18:20.46,Default#3,,0,0,0,,しまった！
Dialogue: 0,0:18:20.55,0:18:22.30,Default#3,,0,0,0,,（ロイド）\N自分の任務とごっちゃに…
Dialogue: 0,0:18:22.39,0:18:25.17,Default#3,,0,0,0,,あ～ フォージャーさん？\N血が…
Dialogue: 0,0:18:25.27,0:18:28.13,Default#3,,0,0,0,,ああ 失礼\N急患が少々暴れまして
Dialogue: 0,0:18:28.90,0:18:31.60,Default#3,,0,0,0,,精神科医の仕事には\Nよくあることなんです
Dialogue: 0,0:18:32.23,0:18:33.64,Default#3,,0,0,0,,楽しんでたかい？　ヨル
Dialogue: 0,0:18:33.73,0:18:34.97,Default#3,,0,0,0,,（カミラ）ウソでしょ
Dialogue: 0,0:18:35.07,0:18:37.19,Default#3,,0,0,0,,ヨル先輩 結婚してたの？
Dialogue: 0,0:18:37.86,0:18:39.23,Default#3,,0,0,0,,なんで黙ってたの？
Dialogue: 0,0:18:39.32,0:18:40.90,Default#3,,0,0,0,,えっと いや…
Dialogue: 0,0:18:40.99,0:18:44.57,Default#3,,0,0,0,,恥ずかしながら\N僕がバツイチ子持ちなもので
Dialogue: 0,0:18:44.66,0:18:46.61,Default#3,,0,0,0,,いろいろ気にされていたのかも
Dialogue: 0,0:18:46.70,0:18:48.32,Default#3,,0,0,0,,（カミラ）ウソよ ウソ！
Dialogue: 0,0:18:48.41,0:18:49.20,Default#3,,0,0,0,,ヨルにこんな
Dialogue: 0,0:18:49.29,0:18:51.28,Default#3,,0,0,0,,スマートイケメン旦那が\Nいるわけない
Dialogue: 0,0:18:51.88,0:18:53.28,Default#3,,0,0,0,,恥かかせてやるわ
Dialogue: 0,0:18:53.38,0:18:54.45,Default#3,,0,0,0,,ヨル先輩！
Dialogue: 0,0:18:54.55,0:18:56.70,Default#3,,0,0,0,,あつあつのグラタン\N焼き上がりましたよ
Dialogue: 0,0:18:56.80,0:18:59.46,Default#3,,0,0,0,,ああっ 足がもつれて～
Dialogue: 0,0:18:59.55,0:19:00.96,Default#3,,0,0,0,,フフ… ん？
Dialogue: 0,0:19:04.35,0:19:05.96,Default#3,,0,0,0,,（悶(もだ)える声）
Dialogue: 0,0:19:06.60,0:19:09.43,Default#3,,0,0,0,,食べ物を大切にするのは\Nすばらしいけど
Dialogue: 0,0:19:09.52,0:19:12.39,Default#3,,0,0,0,,足を使うのは\Nちょっとはしたないよ ヨル
Dialogue: 0,0:19:12.48,0:19:13.97,Default#3,,0,0,0,,あっ すみません！
Dialogue: 0,0:19:14.07,0:19:15.06,Default#3,,0,0,0,,そこ？
Dialogue: 0,0:19:15.15,0:19:16.77,Default#3,,0,0,0,,（ロイド）ん～ これ おいしいね
Dialogue: 0,0:19:16.86,0:19:17.98,Default#3,,0,0,0,,（ヨル）はい
Dialogue: 0,0:19:18.07,0:19:20.23,Default#3,,0,0,0,,レンジャーさん ご存じ？
Dialogue: 0,0:19:20.32,0:19:21.15,Default#3,,0,0,0,,フォージャーです
Dialogue: 0,0:19:21.24,0:19:23.40,Default#3,,0,0,0,,この人ね 役所に来る前
Dialogue: 0,0:19:23.49,0:19:26.28,Default#3,,0,0,0,,いかがわしい仕事\Nしてたらしいですよ
Dialogue: 0,0:19:26.37,0:19:26.99,Default#3,,0,0,0,,あっ…
Dialogue: 0,0:19:27.08,0:19:28.95,Default#3,,0,0,0,,（カミラ）なんでしたっけ？\N（ドミニク）やめろって
Dialogue: 0,0:19:29.04,0:19:30.32,Default#3,,0,0,0,,男の人に呼ばれて
Dialogue: 0,0:19:30.42,0:19:32.74,Default#3,,0,0,0,,ホテルとかで\Nマッサージするんでしたっけ
Dialogue: 0,0:19:32.83,0:19:34.24,Default#3,,0,0,0,,やらし～！
Dialogue: 0,0:19:34.34,0:19:35.95,Default#3,,0,0,0,,それは…
Dialogue: 0,0:19:36.05,0:19:39.46,Default#3,,0,0,0,,（ヨル）鍼灸(しんきゅう)マッサージとうたった\N刺殺の仕事で…
Dialogue: 0,0:19:39.55,0:19:41.79,Default#3,,0,0,0,,あの 違うんです ロイドさん
Dialogue: 0,0:19:41.89,0:19:42.63,Default#3,,0,0,0,,私…
Dialogue: 0,0:19:43.26,0:19:44.34,Default#3,,0,0,0,,すてきです
Dialogue: 0,0:19:44.43,0:19:45.04,Default#3,,0,0,0,,え？
Dialogue: 0,0:19:46.35,0:19:48.88,Default#3,,0,0,0,,ヨルは両親を早くに亡くし
Dialogue: 0,0:19:48.98,0:19:52.26,Default#3,,0,0,0,,幼い弟を養うために\N必死で頑張ってきました
Dialogue: 0,0:19:53.15,0:19:55.31,Default#3,,0,0,0,,自分を犠牲にしてまでも
Dialogue: 0,0:19:55.82,0:19:57.22,Default#3,,0,0,0,,誰かのために
Dialogue: 0,0:19:57.32,0:20:00.48,Default#3,,0,0,0,,何かのために\N過酷な仕事に耐え続けることは
Dialogue: 0,0:20:01.32,0:20:03.10,Default#3,,0,0,0,,並の覚悟では務まりません
Dialogue: 0,0:20:03.62,0:20:06.23,Default#3,,0,0,0,,それは誇るべきことです
Dialogue: 0,0:20:07.24,0:20:08.28,Default#3,,0,0,0,,帰ろうか ヨル
Dialogue: 0,0:20:08.83,0:20:09.53,Default#3,,0,0,0,,（ヨル）はい
Dialogue: 0,0:20:10.04,0:20:11.95,Default#3,,0,0,0,,皆さん ごちそうさまでした
Dialogue: 0,0:20:15.34,0:20:18.37,Default#3,,0,0,0,,す… すいません\N夫などと…
Dialogue: 0,0:20:18.46,0:20:19.50,Default#3,,0,0,0,,あ… いえ
Dialogue: 0,0:20:19.59,0:20:22.54,Default#3,,0,0,0,,弟さんに伝わったら\Nなんと言い訳をすれば…
Dialogue: 0,0:20:24.80,0:20:28.50,Default#3,,0,0,0,,あの ロイドさん\Nご提案なのですが…
Dialogue: 0,0:20:29.39,0:20:29.88,Default#3,,0,0,0,,ハッ
Dialogue: 0,0:20:33.77,0:20:34.47,Default#3,,0,0,0,,なんだ？
Dialogue: 0,0:20:35.06,0:20:36.43,Default#3,,0,0,0,,（ロイド）\N密輸組織の残党？
Dialogue: 0,0:20:37.44,0:20:38.77,Default#3,,0,0,0,,（ロイド）ヨルさん\Nつかまって！
Dialogue: 0,0:20:38.86,0:20:39.47,Default#3,,0,0,0,,（ヨル）え？
Dialogue: 0,0:20:43.57,0:20:45.02,Default#3,,0,0,0,,（ロイド）\Nなぜ居場所が…
Dialogue: 0,0:20:45.11,0:20:45.61,Default#3,,0,0,0,,まさか！
Dialogue: 0,0:20:46.41,0:20:48.02,Default#3,,0,0,0,,美術品の中に\N発信機が…
Dialogue: 0,0:20:48.70,0:20:49.32,Default#3,,0,0,0,,くっ
Dialogue: 0,0:20:49.87,0:20:52.07,Default#3,,0,0,0,,やはり気が緩んでるぞ\N黄昏(たそがれ)！
Dialogue: 0,0:20:52.16,0:20:54.03,Default#3,,0,0,0,,なんですか？\Nあの方たち
Dialogue: 0,0:20:54.71,0:20:56.66,Default#3,,0,0,0,,（ロイド）\Nえっと えっと…
Dialogue: 0,0:20:56.75,0:20:58.66,Default#3,,0,0,0,,（ロイド）か…\N患者のヒステリーが
Dialogue: 0,0:20:58.75,0:21:00.54,Default#3,,0,0,0,,まだ治まって\Nなかったようで
Dialogue: 0,0:21:01.71,0:21:04.33,Default#3,,0,0,0,,（ヨル）大変なのですね\Nお医者様は
Dialogue: 0,0:21:04.43,0:21:05.58,Default#3,,0,0,0,,（ロイド）信じた！
Dialogue: 0,0:21:05.68,0:21:07.88,Default#3,,0,0,0,,ひとまず逃げます
Dialogue: 0,0:21:10.60,0:21:12.30,Default#3,,0,0,0,,（残党）バンが\N乗り捨ててあります
Dialogue: 0,0:21:12.39,0:21:14.13,Default#3,,0,0,0,,（残党）\Nどこかに潜んでるはずだ
Dialogue: 0,0:21:14.23,0:21:15.64,Default#3,,0,0,0,,手分けして捜し出すぞ
Dialogue: 0,0:21:19.73,0:21:20.47,Default#3,,0,0,0,,（殴る音）\N（残党）ぐっ
Dialogue: 0,0:21:20.57,0:21:21.14,Default#3,,0,0,0,,（残党）あっ
Dialogue: 0,0:21:22.15,0:21:23.64,Default#3,,0,0,0,,（ロイド）ヨルさん\Nこっちへ 早く！
Dialogue: 0,0:21:24.36,0:21:27.94,Default#3,,0,0,0,,あの… いいのですか\N患者さんを殴ったりして
Dialogue: 0,0:21:28.49,0:21:29.65,Default#3,,0,0,0,,えっと それは…
Dialogue: 0,0:21:30.41,0:21:34.40,Default#3,,0,0,0,,近年の医学界では\N殴打療法というのが最先端でして…
Dialogue: 0,0:21:34.50,0:21:35.66,Default#3,,0,0,0,,（ヨル）へえ
Dialogue: 0,0:21:35.75,0:21:38.74,Default#3,,0,0,0,,（ロイド）\N鋭いのか鈍いのか分からん人だな
Dialogue: 0,0:21:40.25,0:21:40.95,Default#3,,0,0,0,,危ない ヨルさん！
Dialogue: 0,0:21:41.05,0:21:42.16,Default#3,,0,0,0,,（残党）ぐわあ！
Dialogue: 0,0:21:42.71,0:21:44.12,Default#3,,0,0,0,,（ロイド）\Nもう１人？　かわしきれん！
Dialogue: 0,0:21:44.22,0:21:45.08,Default#3,,0,0,0,,（残党）ぐはあ！
Dialogue: 0,0:21:45.17,0:21:46.04,Default#3,,0,0,0,,（ロイド）あっ
Dialogue: 0,0:21:50.56,0:21:54.55,Default#3,,0,0,0,,あっ すみません！\N素人の私が勝手に治療を…
Dialogue: 0,0:21:55.14,0:21:59.05,Default#3,,0,0,0,,実は私 護身術とか得意で\N弟に習って それで…
Dialogue: 0,0:21:59.65,0:22:02.06,Default#3,,0,0,0,,（ロイド）ハハッ\Nありがとう ヨルさん
Dialogue: 0,0:22:04.15,0:22:06.77,Default#3,,0,0,0,,ハハハハッ すごいですね
Dialogue: 0,0:22:06.86,0:22:08.44,Default#3,,0,0,0,,あいつ 吹っ飛んでましたよ
Dialogue: 0,0:22:08.53,0:22:11.52,Default#3,,0,0,0,,すみません\N吹っ飛ばしてしまいました
Dialogue: 0,0:22:11.62,0:22:12.73,Default#3,,0,0,0,,フフフッ
Dialogue: 0,0:22:12.83,0:22:13.74,Default#3,,0,0,0,,（残党）いたぞ！
Dialogue: 0,0:22:15.33,0:22:17.66,Default#3,,0,0,0,,（残党）\N手段 選ぶな！　必ず殺(や)れ！
Dialogue: 0,0:22:18.67,0:22:20.24,Default#3,,0,0,0,,（残党たち）ぐわ！　ああっ…
Dialogue: 0,0:22:25.88,0:22:29.25,Default#3,,0,0,0,,（ヨル）あの ロイドさん\Nこんなときに なんですが…
Dialogue: 0,0:22:29.84,0:22:31.00,Default#3,,0,0,0,,結婚しませんか？
Dialogue: 0,0:22:31.10,0:22:31.71,Default#3,,0,0,0,,うっ！
Dialogue: 0,0:22:32.35,0:22:33.50,Default#3,,0,0,0,,ぐふっ！
Dialogue: 0,0:22:33.60,0:22:34.30,Default#3,,0,0,0,,（ヨル）ああっ
Dialogue: 0,0:22:36.35,0:22:36.92,Default#3,,0,0,0,,（ロイド）はい？
Dialogue: 0,0:22:37.44,0:22:40.76,Default#3,,0,0,0,,（ヨル）\Nいえ 交換条件の延長というか
Dialogue: 0,0:22:41.65,0:22:44.97,Default#3,,0,0,0,,その… 私みたいな独身女性は
Dialogue: 0,0:22:45.07,0:22:47.77,Default#3,,0,0,0,,それだけで\N怪しまれてしまうらしいので
Dialogue: 0,0:22:47.86,0:22:49.77,Default#3,,0,0,0,,カモフラージュのために
Dialogue: 0,0:22:50.36,0:22:52.90,Default#3,,0,0,0,,（ヨル）\N殺しの仕事を続けていくためにも
Dialogue: 0,0:22:53.53,0:22:54.78,Default#3,,0,0,0,,（ヨル）えっと つまり…
Dialogue: 0,0:22:55.37,0:22:58.95,Default#3,,0,0,0,,もしよければ\N面接の１回だけでなく
Dialogue: 0,0:22:59.04,0:23:01.37,Default#3,,0,0,0,,ちゃんと一緒になるのは\Nどうかなと…
Dialogue: 0,0:23:02.42,0:23:04.79,Default#3,,0,0,0,,お互いの利益のために
Dialogue: 0,0:23:05.76,0:23:08.37,Default#3,,0,0,0,,（ヨル）今のこんな私を\N受け入れてくれるのは
Dialogue: 0,0:23:09.09,0:23:11.33,Default#3,,0,0,0,,きっと この人しかいない
Dialogue: 0,0:23:14.89,0:23:17.76,Default#3,,0,0,0,,では 帰りに役所に寄って手続きを
Dialogue: 0,0:23:17.85,0:23:19.13,Default#3,,0,0,0,,えっ 今から？
Dialogue: 0,0:23:19.23,0:23:20.64,Default#3,,0,0,0,,（ロイド）善は急げです
Dialogue: 0,0:23:20.73,0:23:21.64,Default#3,,0,0,0,,おっ そうだ
Dialogue: 0,0:23:22.19,0:23:22.80,Default#3,,0,0,0,,あ…
Dialogue: 0,0:23:24.23,0:23:24.85,Default#3,,0,0,0,,（ロイド）あっ
Dialogue: 0,0:23:25.48,0:23:26.77,Default#3,,0,0,0,,（ロイド）どこで落とした？
Dialogue: 0,0:23:27.49,0:23:29.14,Default#3,,0,0,0,,（残党）野郎！
Dialogue: 0,0:23:29.24,0:23:31.31,Default#3,,0,0,0,,（残党）追い詰めたぞ！
Dialogue: 0,0:23:32.91,0:23:34.90,Default#3,,0,0,0,,（残党）ぶち殺したるわああ！
Dialogue: 0,0:23:34.99,0:23:36.11,Default#3,,0,0,0,,（ロイド）ヨルさん
Dialogue: 0,0:23:36.66,0:23:37.78,Default#3,,0,0,0,,病めるときも
Dialogue: 0,0:23:39.29,0:23:40.53,Default#3,,0,0,0,,悲しみのときも
Dialogue: 0,0:23:41.50,0:23:43.78,Default#3,,0,0,0,,どんな困難が訪れようとも
Dialogue: 0,0:23:44.59,0:23:45.79,Default#3,,0,0,0,,（爆発音）
Dialogue: 0,0:23:46.30,0:23:47.66,Default#3,,0,0,0,,共に助け合おう
Dialogue: 0,0:23:48.38,0:23:49.04,Default#3,,0,0,0,,はい
Dialogue: 0,0:23:50.22,0:23:51.04,Default#3,,0,0,0,,（ロイド）任務が…
Dialogue: 0,0:23:52.14,0:23:53.04,Default#3,,0,0,0,,（ヨル）殺しが…
Dialogue: 0,0:23:54.18,0:23:56.13,Default#3,,0,0,0,,（ロイド･ヨル）続くかぎり
