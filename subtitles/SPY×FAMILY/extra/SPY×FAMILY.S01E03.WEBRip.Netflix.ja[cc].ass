Dialogue: 0,0:00:01.91,0:00:05.84,Default#3,,0,0,0,,（ナレーション）人は皆\N誰にも見せぬ自分を持っている
Dialogue: 0,0:00:06.34,0:00:10.13,Default#3,,0,0,0,,友人にも恋人にも家族にさえも
Dialogue: 0,0:00:10.63,0:00:15.76,Default#3,,0,0,0,,張り付けた笑顔や虚勢で\N本音を隠し 本性を隠し
Dialogue: 0,0:00:16.26,0:00:21.10,Default#3,,0,0,0,,そうやって世界は\Nかりそめの平穏を取り繕っている
Dialogue: 0,0:00:22.31,0:00:26.86,Default#3,,0,0,0,,世界各国が水面下で\N熾烈(しれつ)な情報戦を繰り広げる時代
Dialogue: 0,0:00:26.94,0:00:29.36,Default#3,,0,0,0,,にらみ合う東西２つの国
Dialogue: 0,0:00:29.44,0:00:32.36,Default#3,,0,0,0,,戦争を企てる東の東国(オスタニア)
Dialogue: 0,0:00:32.45,0:00:35.11,Default#3,,0,0,0,,それを阻止したい西の西国(ウェスタリス)
Dialogue: 0,0:00:35.20,0:00:38.20,Default#3,,0,0,0,,東国政府要人の動向を探るべく
Dialogue: 0,0:00:38.28,0:00:41.75,Default#3,,0,0,0,,西国はオペレーション〈梟(ストリクス)〉を発動
Dialogue: 0,0:00:41.83,0:00:44.71,Default#3,,0,0,0,,作戦を担う敏腕エージェント\N“黄昏(たそがれ)”
Dialogue: 0,0:00:45.25,0:00:50.17,Default#3,,0,0,0,,百の顔を使い分ける彼の任務は\N家族を作ること
Dialogue: 0,0:01:20.41,0:01:22.41,Default#3,,0,0,0,,♪～
Dialogue: 0,0:02:46.45,0:02:48.46,Default#3,,0,0,0,,～♪
Dialogue: 0,0:02:59.84,0:03:01.01,Default#3,,0,0,0,,（ドアが開く音）
Dialogue: 0,0:03:06.81,0:03:09.06,Default#3,,0,0,0,,（ロイド）荷物 これだけですか？
Dialogue: 0,0:03:09.77,0:03:10.90,Default#3,,0,0,0,,（ヨル）はい
Dialogue: 0,0:03:10.98,0:03:12.56,Default#3,,0,0,0,,（ロイド）少ないですね
Dialogue: 0,0:03:15.57,0:03:16.78,Default#3,,0,0,0,,（開封する音）
Dialogue: 0,0:03:20.99,0:03:23.70,Default#3,,0,0,0,,（ヨル）あの ロイドさん 寝室は？
Dialogue: 0,0:03:23.78,0:03:25.62,Default#3,,0,0,0,,（ロイド）もちろん別々です
Dialogue: 0,0:03:25.70,0:03:28.20,Default#3,,0,0,0,,来客のときだけ それっぽく\Nごまかしましょう
Dialogue: 0,0:03:28.29,0:03:29.71,Default#3,,0,0,0,,そ… そうですね
Dialogue: 0,0:03:30.92,0:03:33.38,Default#3,,0,0,0,,（アーニャ）\Nアーニャんちへ いらさいませ
Dialogue: 0,0:03:33.46,0:03:35.67,Default#3,,0,0,0,,よろしくお願いします\Nアーニャさん
Dialogue: 0,0:03:35.75,0:03:39.22,Default#3,,0,0,0,,（ナレーション）\Nここに 一風変わった家族があった
Dialogue: 0,0:03:39.30,0:03:42.43,Default#3,,0,0,0,,父 ロイド･フォージャー 精神科医
Dialogue: 0,0:03:42.51,0:03:46.60,Default#3,,0,0,0,,正体 スパイ\Nコードネーム“黄昏”
Dialogue: 0,0:03:46.68,0:03:50.31,Default#3,,0,0,0,,母 ヨル･フォージャー 市役所職員
Dialogue: 0,0:03:50.39,0:03:54.73,Default#3,,0,0,0,,正体 殺し屋\Nコードネーム“いばら姫”
Dialogue: 0,0:03:54.81,0:03:58.69,Default#3,,0,0,0,,娘 アーニャ･フォージャー\N正体 超能力者(エスパー)
Dialogue: 0,0:03:58.78,0:04:01.49,Default#3,,0,0,0,,（アーニャ）\Nアーニャに はは生まれて めでたい
Dialogue: 0,0:04:01.57,0:04:02.53,Default#3,,0,0,0,,はは！
Dialogue: 0,0:04:02.61,0:04:03.91,Default#3,,0,0,0,,お手伝い～
Dialogue: 0,0:04:03.99,0:04:05.70,Default#3,,0,0,0,,そうそう とりあえず…
Dialogue: 0,0:04:05.78,0:04:07.62,Default#3,,0,0,0,,知り合いの判事のコネで
Dialogue: 0,0:04:07.70,0:04:10.96,Default#3,,0,0,0,,入籍したのは１年前\Nってことに してもらいました
Dialogue: 0,0:04:11.04,0:04:12.75,Default#3,,0,0,0,,（ロイド）ホントは偽造だけど
Dialogue: 0,0:04:13.67,0:04:15.42,Default#3,,0,0,0,,（ロイド）今まで別居だったことや
Dialogue: 0,0:04:15.50,0:04:18.92,Default#3,,0,0,0,,弟さんへのもろもろの言い訳は\Nあとで考えましょう
Dialogue: 0,0:04:19.00,0:04:20.09,Default#3,,0,0,0,,分かりました
Dialogue: 0,0:04:20.67,0:04:22.88,Default#3,,0,0,0,,（ロイド）\N入籍したのが試験の直前じゃ
Dialogue: 0,0:04:22.97,0:04:25.34,Default#3,,0,0,0,,学校側に怪しまれるからな
Dialogue: 0,0:04:25.43,0:04:29.26,Default#3,,0,0,0,,（ヨル）店長の許可も下りたし\Nこれで ひと安心です
Dialogue: 0,0:04:30.14,0:04:31.52,Default#3,,0,0,0,,（アーニャ）わくわく
Dialogue: 0,0:04:31.60,0:04:34.06,Default#3,,0,0,0,,（ナレーション）父 母 娘
Dialogue: 0,0:04:34.14,0:04:37.40,Default#3,,0,0,0,,己(おの)が利のため\N疑似家族を作った３人は
Dialogue: 0,0:04:37.48,0:04:39.36,Default#3,,0,0,0,,互いの正体を隠しながら
Dialogue: 0,0:04:39.44,0:04:42.90,Default#3,,0,0,0,,ひとつ屋根の下で\N暮らすこととなった
Dialogue: 0,0:04:43.45,0:04:45.36,Default#3,,0,0,0,,アーニャ はは 案内する
Dialogue: 0,0:04:45.86,0:04:48.74,Default#3,,0,0,0,,ひと段落ついたら\N面接の練習するぞ
Dialogue: 0,0:04:49.45,0:04:50.91,Default#3,,0,0,0,,（アーニャ）ここがキッチン
Dialogue: 0,0:04:51.45,0:04:52.91,Default#3,,0,0,0,,ちち 料理 うまい！
Dialogue: 0,0:04:53.00,0:04:54.25,Default#3,,0,0,0,,（ヨル）そうなんですね
Dialogue: 0,0:04:58.17,0:05:00.50,Default#3,,0,0,0,,ここがトイレ お風呂
Dialogue: 0,0:05:01.05,0:05:02.01,Default#3,,0,0,0,,ピカピカです
Dialogue: 0,0:05:06.76,0:05:08.85,Default#3,,0,0,0,,アーニャ 一人で顔 洗える
Dialogue: 0,0:05:08.93,0:05:10.26,Default#3,,0,0,0,,（ヨル）アーニャさん 偉いです
Dialogue: 0,0:05:10.35,0:05:11.27,Default#3,,0,0,0,,（ドアが開く音）
Dialogue: 0,0:05:14.52,0:05:15.77,Default#3,,0,0,0,,（アーニャ）ここ アーニャの部屋
Dialogue: 0,0:05:16.48,0:05:17.77,Default#3,,0,0,0,,いらさいませ
Dialogue: 0,0:05:17.86,0:05:20.15,Default#3,,0,0,0,,（ヨル）フフフッ おじゃまします
Dialogue: 0,0:05:20.77,0:05:22.61,Default#3,,0,0,0,,まあ かわいいお部屋
Dialogue: 0,0:05:23.19,0:05:25.90,Default#3,,0,0,0,,（アーニャ）\Nキメラさんを紹介する～
Dialogue: 0,0:05:27.70,0:05:31.54,Default#3,,0,0,0,,私がキメラだ\Nよろろすお願いするます
Dialogue: 0,0:05:32.24,0:05:34.12,Default#3,,0,0,0,,はじめまして キメラさん
Dialogue: 0,0:05:34.20,0:05:36.04,Default#3,,0,0,0,,アーニャさんのははです
Dialogue: 0,0:05:38.88,0:05:40.09,Default#3,,0,0,0,,ちちの部屋
Dialogue: 0,0:05:40.17,0:05:42.71,Default#3,,0,0,0,,ここがロイドさんのお部屋ですか
Dialogue: 0,0:05:45.59,0:05:47.18,Default#3,,0,0,0,,こっちが ははの部屋
Dialogue: 0,0:05:50.68,0:05:51.76,Default#3,,0,0,0,,はは どうぞ
Dialogue: 0,0:05:52.64,0:05:54.52,Default#3,,0,0,0,,（ヨル）はい 失礼します
Dialogue: 0,0:05:55.64,0:05:57.56,Default#3,,0,0,0,,これが 私の…
Dialogue: 0,0:06:04.28,0:06:06.53,Default#3,,0,0,0,,とってもすてきなお部屋です
Dialogue: 0,0:06:06.61,0:06:07.65,Default#3,,0,0,0,,それはよかった
Dialogue: 0,0:06:08.61,0:06:09.82,Default#3,,0,0,0,,これで全部です
Dialogue: 0,0:06:10.49,0:06:12.24,Default#3,,0,0,0,,家具の配置はどうです？
Dialogue: 0,0:06:12.87,0:06:14.66,Default#3,,0,0,0,,軽く掃除もしておきましたが
Dialogue: 0,0:06:14.75,0:06:16.71,Default#3,,0,0,0,,はい 大丈夫です
Dialogue: 0,0:06:16.79,0:06:18.62,Default#3,,0,0,0,,（アーニャ）\Nアーニャもお掃除 手伝った！
Dialogue: 0,0:06:18.71,0:06:21.38,Default#3,,0,0,0,,そうなんですか\Nありがとうございます
Dialogue: 0,0:06:21.96,0:06:24.42,Default#3,,0,0,0,,エヘヘッ 偉い？　偉い？
Dialogue: 0,0:06:25.46,0:06:28.88,Default#3,,0,0,0,,ほっ あっ ああああ～！
Dialogue: 0,0:06:28.97,0:06:31.76,Default#3,,0,0,0,,お前はバケツを\Nひっくり返しただけだろ
Dialogue: 0,0:06:31.85,0:06:32.89,Default#3,,0,0,0,,ガーン！
Dialogue: 0,0:06:32.97,0:06:35.85,Default#3,,0,0,0,,（ヨル）ウフフッ どうしましょう
Dialogue: 0,0:06:35.93,0:06:38.56,Default#3,,0,0,0,,こんな立派なお部屋を\Nお借りして
Dialogue: 0,0:06:38.64,0:06:41.77,Default#3,,0,0,0,,何言ってるんですか\N自分の家だと思って…
Dialogue: 0,0:06:41.86,0:06:44.90,Default#3,,0,0,0,,というか自分の家なので\Nお好きに使ってください
Dialogue: 0,0:06:45.78,0:06:46.44,Default#3,,0,0,0,,はい！
Dialogue: 0,0:06:48.86,0:06:50.86,Default#3,,0,0,0,,ダンボール空いたら\N取りに来ます
Dialogue: 0,0:06:51.45,0:06:52.07,Default#3,,0,0,0,,あ…
Dialogue: 0,0:06:52.57,0:06:53.08,Default#3,,0,0,0,,あっ
Dialogue: 0,0:06:53.99,0:06:56.12,Default#3,,0,0,0,,ロイドさん 収納できました
Dialogue: 0,0:06:56.79,0:06:58.25,Default#3,,0,0,0,,手際いいですね
Dialogue: 0,0:07:00.67,0:07:03.63,Default#3,,0,0,0,,おっ こちらが例の弟さんですか
Dialogue: 0,0:07:03.71,0:07:05.21,Default#3,,0,0,0,,あっ はい
Dialogue: 0,0:07:05.30,0:07:07.88,Default#3,,0,0,0,,ヨルさんと同じ\N公務員なんでしたっけ
Dialogue: 0,0:07:07.97,0:07:09.17,Default#3,,0,0,0,,そうです
Dialogue: 0,0:07:09.26,0:07:13.60,Default#3,,0,0,0,,最近 忙しいらしくて\N電話でしか話せてないんですが
Dialogue: 0,0:07:13.68,0:07:15.64,Default#3,,0,0,0,,姉弟(きょうだい)でよく似てますね
Dialogue: 0,0:07:15.72,0:07:18.85,Default#3,,0,0,0,,アーニャの試験が終わったら\Nいずれご挨拶を
Dialogue: 0,0:07:18.93,0:07:22.15,Default#3,,0,0,0,,あの ロイドさん\Nそのことなんですが…
Dialogue: 0,0:07:22.23,0:07:25.90,Default#3,,0,0,0,,私 面接に着ていけるような服を\N持ってないのですが
Dialogue: 0,0:07:25.98,0:07:29.03,Default#3,,0,0,0,,（ロイド）では 今日にでも\N仕立屋に見に行きましょう
Dialogue: 0,0:07:29.11,0:07:30.36,Default#3,,0,0,0,,（ヨル）\Nありがとうございます
Dialogue: 0,0:07:30.86,0:07:32.66,Default#3,,0,0,0,,お手伝い お手伝い
Dialogue: 0,0:07:33.78,0:07:34.28,Default#3,,0,0,0,,ん？
Dialogue: 0,0:07:34.87,0:07:37.74,Default#3,,0,0,0,,あっ アーニャさん！\Nそれは私が…
Dialogue: 0,0:07:37.83,0:07:40.12,Default#3,,0,0,0,,（ヨル）\Nその中には お仕事専用の武器(エモノ)と
Dialogue: 0,0:07:40.21,0:07:41.79,Default#3,,0,0,0,,ゾウをも倒す猛毒が…
Dialogue: 0,0:07:41.87,0:07:42.54,Default#3,,0,0,0,,ひっ！
Dialogue: 0,0:07:43.38,0:07:45.00,Default#3,,0,0,0,,（お湯が沸く音）
Dialogue: 0,0:07:45.71,0:07:46.92,Default#3,,0,0,0,,（ヨル）ロイドさんは…
Dialogue: 0,0:07:47.00,0:07:49.30,Default#3,,0,0,0,,（ロイド）\Nコーヒー ブラックでお願いします
Dialogue: 0,0:07:50.63,0:07:52.47,Default#3,,0,0,0,,（アーニャ）\Nアーニャ ココア飲みたい
Dialogue: 0,0:07:52.55,0:07:54.30,Default#3,,0,0,0,,砂糖 ミルク入りな
Dialogue: 0,0:07:54.39,0:07:55.47,Default#3,,0,0,0,,（アーニャ）それ～
Dialogue: 0,0:07:58.39,0:07:59.60,Default#3,,0,0,0,,すごい
Dialogue: 0,0:07:59.68,0:08:01.35,Default#3,,0,0,0,,クッキー 手作りなんですね
Dialogue: 0,0:08:01.85,0:08:04.35,Default#3,,0,0,0,,実は初めてでして クッキー作り
Dialogue: 0,0:08:04.94,0:08:07.86,Default#3,,0,0,0,,レシピ本どおりなので\Nそれなりの出来です
Dialogue: 0,0:08:07.94,0:08:09.61,Default#3,,0,0,0,,お口に合えばいいのですが
Dialogue: 0,0:08:10.65,0:08:11.61,Default#3,,0,0,0,,いただきます
Dialogue: 0,0:08:13.95,0:08:15.32,Default#3,,0,0,0,,おいしい！
Dialogue: 0,0:08:15.41,0:08:17.08,Default#3,,0,0,0,,ホントに料理 お上手です
Dialogue: 0,0:08:17.16,0:08:18.24,Default#3,,0,0,0,,それほどでも
Dialogue: 0,0:08:18.33,0:08:20.41,Default#3,,0,0,0,,アーニャもクッキー 手伝った！
Dialogue: 0,0:08:20.50,0:08:21.62,Default#3,,0,0,0,,そうなんですね
Dialogue: 0,0:08:23.29,0:08:24.12,Default#3,,0,0,0,,っくし！
Dialogue: 0,0:08:27.13,0:08:29.63,Default#3,,0,0,0,,お前は粉まみれになって\N邪魔したあとは
Dialogue: 0,0:08:29.71,0:08:31.80,Default#3,,0,0,0,,ピーナツ\Nつまみ食いしてただけだろ
Dialogue: 0,0:08:32.42,0:08:33.09,Default#3,,0,0,0,,ハッ
Dialogue: 0,0:08:33.68,0:08:35.51,Default#3,,0,0,0,,ウフフッ
Dialogue: 0,0:08:35.59,0:08:37.64,Default#3,,0,0,0,,ひと息ついたら始めましょうか
Dialogue: 0,0:08:39.77,0:08:43.14,Default#3,,0,0,0,,イーデン校は\N国内トップクラスのエリート校です
Dialogue: 0,0:08:43.23,0:08:46.94,Default#3,,0,0,0,,面接では 子供はもちろん\N親も審査の対象
Dialogue: 0,0:08:47.02,0:08:49.98,Default#3,,0,0,0,,家柄の良さや\N教養の深さを試されます
Dialogue: 0,0:08:50.07,0:08:51.28,Default#3,,0,0,0,,いいですね？
Dialogue: 0,0:08:51.36,0:08:52.32,Default#3,,0,0,0,,では 質問です
Dialogue: 0,0:08:52.86,0:08:55.03,Default#3,,0,0,0,,まず名前と住所を教えてください
Dialogue: 0,0:08:55.70,0:08:58.78,Default#3,,0,0,0,,アーニャ\Nフォ… フォ… フォージャ
Dialogue: 0,0:08:58.87,0:08:59.74,Default#3,,0,0,0,,じゅうしょ？
Dialogue: 0,0:08:59.83,0:09:00.83,Default#3,,0,0,0,,（ロイド）ここのことだ
Dialogue: 0,0:09:00.91,0:09:02.29,Default#3,,0,0,0,,あっ アーニャんち！
Dialogue: 0,0:09:02.37,0:09:04.00,Default#3,,0,0,0,,そういうことじゃない
Dialogue: 0,0:09:04.08,0:09:07.00,Default#3,,0,0,0,,え～ アーニャさん\N休日は いつもどんなことを？
Dialogue: 0,0:09:07.58,0:09:10.13,Default#3,,0,0,0,,ちちに留守番を命じられるので
Dialogue: 0,0:09:10.21,0:09:12.63,Default#3,,0,0,0,,１人さみしくテレビを見てます
Dialogue: 0,0:09:12.71,0:09:15.38,Default#3,,0,0,0,,（ロイド）待て待て 印象悪い
Dialogue: 0,0:09:15.47,0:09:16.26,Default#3,,0,0,0,,お母様
Dialogue: 0,0:09:16.34,0:09:16.84,Default#3,,0,0,0,,はい！
Dialogue: 0,0:09:17.34,0:09:20.39,Default#3,,0,0,0,,（ロイド）イーデン校を\N志望する理由を教えてください
Dialogue: 0,0:09:20.47,0:09:22.14,Default#3,,0,0,0,,しぼう理由ですか？
Dialogue: 0,0:09:22.22,0:09:24.31,Default#3,,0,0,0,,えっと しぼう… しぼう…
Dialogue: 0,0:09:24.94,0:09:28.27,Default#3,,0,0,0,,心肺停止とか出血多量とか
Dialogue: 0,0:09:28.36,0:09:33.15,Default#3,,0,0,0,,あっ 全身複雑骨折とか\N頭蓋骨陥没みたいな…
Dialogue: 0,0:09:33.24,0:09:34.53,Default#3,,0,0,0,,（ロイド）質問を変えます
Dialogue: 0,0:09:34.61,0:09:36.99,Default#3,,0,0,0,,お母様の教育方針は？
Dialogue: 0,0:09:37.07,0:09:39.83,Default#3,,0,0,0,,え？　え… えっと… 先手必勝？
Dialogue: 0,0:09:39.91,0:09:43.00,Default#3,,0,0,0,,（ロイド）よく弟が\Nまっとうに育ったな この人
Dialogue: 0,0:09:43.75,0:09:45.12,Default#3,,0,0,0,,スパイ歴 十数年
Dialogue: 0,0:09:45.62,0:09:47.83,Default#3,,0,0,0,,数々のミッションをこなしてきた\Nこの俺は
Dialogue: 0,0:09:47.92,0:09:50.34,Default#3,,0,0,0,,今 初めて くじけかけている
Dialogue: 0,0:09:50.42,0:09:53.96,Default#3,,0,0,0,,（ロイド）ダメだ\Nこんなんで面接が通るわけない
Dialogue: 0,0:09:54.05,0:09:55.55,Default#3,,0,0,0,,入学は諦めよう
Dialogue: 0,0:09:55.63,0:09:56.51,Default#3,,0,0,0,,（ヨル）ロイドさん！
Dialogue: 0,0:09:56.59,0:09:59.34,Default#3,,0,0,0,,亡き奥様のご遺志を\N思い出してください！
Dialogue: 0,0:09:59.85,0:10:02.35,Default#3,,0,0,0,,（ロイド）\N模擬面接には まだ早かったか
Dialogue: 0,0:10:02.85,0:10:04.77,Default#3,,0,0,0,,（ロイド）よし！　出かけよう
Dialogue: 0,0:10:04.85,0:10:06.73,Default#3,,0,0,0,,まずは見聞を広めて
Dialogue: 0,0:10:06.81,0:10:09.69,Default#3,,0,0,0,,上流家庭における\N一般常識を身につけ
Dialogue: 0,0:10:09.77,0:10:12.02,Default#3,,0,0,0,,３人の間で共通認識を作るぞ
Dialogue: 0,0:10:13.32,0:10:16.49,Default#3,,0,0,0,,おでけけ おでけけ ランラララン♪
Dialogue: 0,0:10:16.57,0:10:17.70,Default#3,,0,0,0,,“お出かけ”な
Dialogue: 0,0:10:18.74,0:10:20.28,Default#3,,0,0,0,,はは 手つなぐ？
Dialogue: 0,0:10:20.37,0:10:23.54,Default#3,,0,0,0,,あっ よ… よろしくお願いします
Dialogue: 0,0:10:23.62,0:10:26.04,Default#3,,0,0,0,,（ロイド）\N懐いてくれてるみたいでよかった
Dialogue: 0,0:10:26.62,0:10:28.33,Default#3,,0,0,0,,プランは既に出来ている
Dialogue: 0,0:10:28.42,0:10:30.13,Default#3,,0,0,0,,今日１日で普通の…
Dialogue: 0,0:10:30.21,0:10:33.30,Default#3,,0,0,0,,いや 良家の家族としか\N見えんようになるはずだ
Dialogue: 0,0:10:34.13,0:10:34.88,Default#3,,0,0,0,,（ヨル）ああ
Dialogue: 0,0:10:34.96,0:10:38.05,Default#3,,0,0,0,,小さくてかわいかったころの\N弟を思い出します
Dialogue: 0,0:10:40.05,0:10:41.76,Default#3,,0,0,0,,思い切りハグしてあげたら
Dialogue: 0,0:10:41.85,0:10:44.81,Default#3,,0,0,0,,肋骨(ろっこつ)２本折ってしまったことも\Nありましたっけ
Dialogue: 0,0:10:44.89,0:10:46.52,Default#3,,0,0,0,,フフフッ 気をつけないと
Dialogue: 0,0:10:47.52,0:10:48.02,Default#3,,0,0,0,,（ヨル）あっ
Dialogue: 0,0:10:48.10,0:10:51.48,Default#3,,0,0,0,,（ロイド）えっ？\Nあれ？　そうでもないのか？
Dialogue: 0,0:10:54.23,0:10:56.57,Default#3,,0,0,0,,と… ところで これからどこへ？
Dialogue: 0,0:10:56.65,0:11:00.20,Default#3,,0,0,0,,仕事のコネで いろんなチケット\N手に入れてまして
Dialogue: 0,0:11:01.37,0:11:04.37,Default#3,,0,0,0,,（オペラの歌声）
Dialogue: 0,0:11:15.38,0:11:18.59,Default#3,,0,0,0,,（ロイド）\Nイーデン校は伝統と格式を重んじる
Dialogue: 0,0:11:19.13,0:11:21.84,Default#3,,0,0,0,,一流のものに触れて教養を深め
Dialogue: 0,0:11:21.93,0:11:24.22,Default#3,,0,0,0,,ふとしたときに\Nボロが出んようにせねば
Dialogue: 0,0:11:31.10,0:11:32.81,Default#3,,0,0,0,,よし 次行こう
Dialogue: 0,0:11:34.31,0:11:37.23,Default#3,,0,0,0,,（ロイド）アーニャ\Nここでは大きな声を出すなよ
Dialogue: 0,0:11:37.32,0:11:37.98,Default#3,,0,0,0,,（アーニャ）うい
Dialogue: 0,0:11:38.90,0:11:43.20,Default#3,,0,0,0,,（ロイド）一流の音楽の次は\N一流のアートに触れて 以下略
Dialogue: 0,0:11:43.28,0:11:46.03,Default#3,,0,0,0,,（アーニャ）ちち！\Nすっぽんぽん すっぽんぽん
Dialogue: 0,0:11:46.66,0:11:49.33,Default#3,,0,0,0,,首ちょんぱ 体ちょんぱ
Dialogue: 0,0:11:49.41,0:11:51.42,Default#3,,0,0,0,,大声 出すなって言ったろ
Dialogue: 0,0:11:51.50,0:11:52.79,Default#3,,0,0,0,,（ヨル）わあ～
Dialogue: 0,0:11:58.05,0:11:59.38,Default#3,,0,0,0,,ヨルさん？
Dialogue: 0,0:11:59.46,0:12:00.67,Default#3,,0,0,0,,ああ～
Dialogue: 0,0:12:00.76,0:12:01.55,Default#3,,0,0,0,,ヨ…
Dialogue: 0,0:12:06.10,0:12:07.64,Default#3,,0,0,0,,アーニャ あれやりたい
Dialogue: 0,0:12:07.72,0:12:09.43,Default#3,,0,0,0,,（ロイド）お絵描きか いいぞ
Dialogue: 0,0:12:10.02,0:12:12.31,Default#3,,0,0,0,,ヨルさん ひと休みしましょうか
Dialogue: 0,0:12:12.39,0:12:12.98,Default#3,,0,0,0,,はい
Dialogue: 0,0:12:13.81,0:12:16.69,Default#3,,0,0,0,,アーニャんちは デッカいお城～
Dialogue: 0,0:12:16.77,0:12:20.03,Default#3,,0,0,0,,秘密組織のボスはキメラ長官
Dialogue: 0,0:12:20.11,0:12:22.40,Default#3,,0,0,0,,ちち スパイ ぱしゅん ぽしゅん
Dialogue: 0,0:12:22.49,0:12:25.20,Default#3,,0,0,0,,はは 殺し屋 ざくざく どしゅん
Dialogue: 0,0:12:27.41,0:12:28.91,Default#3,,0,0,0,,そろそろ行きましょうか
Dialogue: 0,0:12:29.49,0:12:30.16,Default#3,,0,0,0,,ええ
Dialogue: 0,0:12:32.33,0:12:33.12,Default#3,,0,0,0,,出来た！
Dialogue: 0,0:12:33.75,0:12:35.58,Default#3,,0,0,0,,アーニャ 次行くぞ
Dialogue: 0,0:12:36.17,0:12:37.25,Default#3,,0,0,0,,（アーニャ）これ見たら
Dialogue: 0,0:12:37.34,0:12:40.01,Default#3,,0,0,0,,アーニャの能力\Nちちとははにバレちゃう
Dialogue: 0,0:12:40.67,0:12:43.34,Default#3,,0,0,0,,（ロイド）ん？　おお 大作だな
Dialogue: 0,0:12:44.34,0:12:46.05,Default#3,,0,0,0,,森の中にブタがいるな
Dialogue: 0,0:12:46.14,0:12:48.76,Default#3,,0,0,0,,（アーニャ）\Nお城に住んでるキメラさん！
Dialogue: 0,0:12:48.85,0:12:49.56,Default#3,,0,0,0,,（ロイド）そうか
Dialogue: 0,0:12:50.10,0:12:52.35,Default#3,,0,0,0,,でも ボンドマンは分かるぞ\N当たりだろ
Dialogue: 0,0:12:52.43,0:12:53.52,Default#3,,0,0,0,,（アーニャ）う… うい
Dialogue: 0,0:12:53.60,0:12:55.65,Default#3,,0,0,0,,（ロイド）先週のスパイアニメだな
Dialogue: 0,0:12:55.73,0:12:59.32,Default#3,,0,0,0,,とすると\Nこの魔女っぽいのはお姫様か
Dialogue: 0,0:12:59.40,0:13:00.73,Default#3,,0,0,0,,なんかグロいな
Dialogue: 0,0:13:00.82,0:13:01.86,Default#3,,0,0,0,,この猫は かわいいが
Dialogue: 0,0:13:01.94,0:13:02.86,Default#3,,0,0,0,,（アーニャ）犬さん！
Dialogue: 0,0:13:02.94,0:13:04.11,Default#3,,0,0,0,,（ロイド）そうか すまん
Dialogue: 0,0:13:04.74,0:13:05.78,Default#3,,0,0,0,,フウ～
Dialogue: 0,0:13:06.91,0:13:08.62,Default#3,,0,0,0,,次は仕立屋に行くぞ
Dialogue: 0,0:13:10.41,0:13:13.08,Default#3,,0,0,0,,（ヨル）\Nすみません せっかくの休日なのに
Dialogue: 0,0:13:13.16,0:13:15.25,Default#3,,0,0,0,,いえ 必要なことですから
Dialogue: 0,0:13:15.33,0:13:17.79,Default#3,,0,0,0,,（アーニャ）アーニャのよそ行き\Nすごいかわいい
Dialogue: 0,0:13:17.88,0:13:19.29,Default#3,,0,0,0,,アーニャにぴったり
Dialogue: 0,0:13:19.38,0:13:21.84,Default#3,,0,0,0,,このお店 すごい！　お気に入り
Dialogue: 0,0:13:21.92,0:13:23.05,Default#3,,0,0,0,,（女主人）ありがとう
Dialogue: 0,0:13:23.13,0:13:26.18,Default#3,,0,0,0,,この服着て 初めてのおでけけ
Dialogue: 0,0:13:26.26,0:13:28.35,Default#3,,0,0,0,,（女主人）じゃあ 今日は記念日ね
Dialogue: 0,0:13:28.43,0:13:29.89,Default#3,,0,0,0,,おでけけ記念日！
Dialogue: 0,0:13:29.97,0:13:32.64,Default#3,,0,0,0,,それなら このあとで\N写真館に寄るか
Dialogue: 0,0:13:32.72,0:13:34.77,Default#3,,0,0,0,,（店員）\Nヨルちゃん こちらへどうぞ
Dialogue: 0,0:13:34.85,0:13:35.52,Default#3,,0,0,0,,はい
Dialogue: 0,0:13:37.94,0:13:41.86,Default#3,,0,0,0,,セミフォーマルなら\Nこのくらいの色もいいと思うけど
Dialogue: 0,0:13:41.94,0:13:42.86,Default#3,,0,0,0,,どう？
Dialogue: 0,0:13:42.94,0:13:46.11,Default#3,,0,0,0,,私 ドレスは\N黒か赤色がよいのですけど
Dialogue: 0,0:13:46.20,0:13:49.74,Default#3,,0,0,0,,（女主人）そう？\Nヨルちゃん よく似合うと思うけど
Dialogue: 0,0:13:49.83,0:13:51.03,Default#3,,0,0,0,,じゃあ これならどう？
Dialogue: 0,0:13:51.12,0:13:53.16,Default#3,,0,0,0,,（ロイド）\N引っ越しの荷物の量から見て
Dialogue: 0,0:13:53.25,0:13:55.41,Default#3,,0,0,0,,服には無頓着なのかと思ったが
Dialogue: 0,0:13:56.46,0:13:58.83,Default#3,,0,0,0,,カラーコーディネートに\Nこだわりがあるんですね
Dialogue: 0,0:13:58.92,0:14:01.34,Default#3,,0,0,0,,いえ 別にそういうわけでは…
Dialogue: 0,0:14:01.42,0:14:03.13,Default#3,,0,0,0,,（ヨル）とても すてきなんですが
Dialogue: 0,0:14:03.21,0:14:06.09,Default#3,,0,0,0,,いざというとき\N返り血が目立つのはちょっと…
Dialogue: 0,0:14:06.17,0:14:06.80,Default#3,,0,0,0,,ハッ！
Dialogue: 0,0:14:10.39,0:14:13.93,Default#3,,0,0,0,,（店員）ヨルちゃんにも\Nやっといい人ができたのね
Dialogue: 0,0:14:14.02,0:14:15.85,Default#3,,0,0,0,,続くといいわね
Dialogue: 0,0:14:15.93,0:14:19.06,Default#3,,0,0,0,,私なら子持ちは\N恋愛対象から外すけど
Dialogue: 0,0:14:19.15,0:14:20.36,Default#3,,0,0,0,,（ロイド）面接までには
Dialogue: 0,0:14:20.44,0:14:22.82,Default#3,,0,0,0,,予備の服の仕立ても\N間に合いそうですね
Dialogue: 0,0:14:22.90,0:14:24.69,Default#3,,0,0,0,,すみません 何着も
Dialogue: 0,0:14:25.44,0:14:28.15,Default#3,,0,0,0,,ヨルさん\N赤 黒以外も似合いますから
Dialogue: 0,0:14:28.24,0:14:30.24,Default#3,,0,0,0,,これから お出かけの機会も\N増えますし
Dialogue: 0,0:14:30.91,0:14:32.12,Default#3,,0,0,0,,（アーニャ）おでけけ おでけけ
Dialogue: 0,0:14:32.20,0:14:33.74,Default#3,,0,0,0,,あの店は行きつけでしょう？
Dialogue: 0,0:14:34.24,0:14:36.04,Default#3,,0,0,0,,今後も利用するでしょうし
Dialogue: 0,0:14:36.12,0:14:37.37,Default#3,,0,0,0,,次回 行った際には
Dialogue: 0,0:14:37.46,0:14:40.21,Default#3,,0,0,0,,店主に結婚の話を\Nしないといけませんね
Dialogue: 0,0:14:40.29,0:14:41.33,Default#3,,0,0,0,,そうでした
Dialogue: 0,0:14:49.05,0:14:49.63,Default#3,,0,0,0,,（ストロボの音）
Dialogue: 0,0:14:55.85,0:14:58.81,Default#3,,0,0,0,,（政治家）\N我が国民党は西方政策において
Dialogue: 0,0:14:58.89,0:15:00.48,Default#3,,0,0,0,,宥和(ゆうわ)路線をとるもので…
Dialogue: 0,0:15:00.56,0:15:03.27,Default#3,,0,0,0,,（ロイド）\Nイーデンでは愛国教育も盛んだ
Dialogue: 0,0:15:03.36,0:15:05.78,Default#3,,0,0,0,,政治や歴史にも精通しておかないと
Dialogue: 0,0:15:06.32,0:15:08.19,Default#3,,0,0,0,,（聴衆）引っ込め売国奴！
Dialogue: 0,0:15:08.82,0:15:10.99,Default#3,,0,0,0,,（聴衆）\Nあいつらに家族を殺されたんだ
Dialogue: 0,0:15:11.07,0:15:12.24,Default#3,,0,0,0,,（聴衆）金(かね)くれ\N（聴衆）きれい事ばかり…
Dialogue: 0,0:15:12.32,0:15:13.33,Default#3,,0,0,0,,（ロイド）どうした？
Dialogue: 0,0:15:13.41,0:15:15.58,Default#3,,0,0,0,,（アーニャ）\Nアーニャ こういう所 苦手
Dialogue: 0,0:15:15.66,0:15:17.62,Default#3,,0,0,0,,すまん ちょっと怖かったな
Dialogue: 0,0:15:17.70,0:15:19.58,Default#3,,0,0,0,,（ヨル）どこかで休みましょうか
Dialogue: 0,0:15:20.83,0:15:22.83,Default#3,,0,0,0,,（聴衆）ちくしょう\N（聴衆）西の犬め
Dialogue: 0,0:15:22.92,0:15:24.88,Default#3,,0,0,0,,（聴衆）ブタ野郎\N（聴衆）足 臭い…
Dialogue: 0,0:15:25.67,0:15:26.76,Default#3,,0,0,0,,ホッ…
Dialogue: 0,0:15:26.84,0:15:28.76,Default#3,,0,0,0,,ちち おなかすいた
Dialogue: 0,0:15:28.84,0:15:31.89,Default#3,,0,0,0,,（ロイド）\Nえ？　元気なの？　どっちなの？
Dialogue: 0,0:15:36.72,0:15:39.85,Default#3,,0,0,0,,こら アーニャ\Nナッツの部分だけ食うな
Dialogue: 0,0:15:39.94,0:15:41.02,Default#3,,0,0,0,,手づかみもダメ
Dialogue: 0,0:15:42.69,0:15:45.15,Default#3,,0,0,0,,ヨルさんは なんか… うん…
Dialogue: 0,0:15:45.98,0:15:47.82,Default#3,,0,0,0,,（ロイド）スパイ歴 十数年
Dialogue: 0,0:15:47.90,0:15:50.74,Default#3,,0,0,0,,数々のミッションをこなしてきた\Nこの俺は
Dialogue: 0,0:15:50.82,0:15:52.57,Default#3,,0,0,0,,再度 くじけかけている
Dialogue: 0,0:15:53.07,0:15:54.45,Default#3,,0,0,0,,ドンマイ ちち
Dialogue: 0,0:15:54.53,0:15:55.24,Default#3,,0,0,0,,（ロイド）何が
Dialogue: 0,0:15:55.99,0:15:58.62,Default#3,,0,0,0,,（ロイド）\Nくっ やはり人選をミスったか
Dialogue: 0,0:15:58.70,0:15:59.33,Default#3,,0,0,0,,ハッ
Dialogue: 0,0:15:59.91,0:16:00.71,Default#3,,0,0,0,,（ロイド）いや
Dialogue: 0,0:16:00.79,0:16:04.00,Default#3,,0,0,0,,そもそも他人をアテにすることに\N無理があったのだ
Dialogue: 0,0:16:04.08,0:16:06.80,Default#3,,0,0,0,,他人の\Nそれも素人の価値判断などに
Dialogue: 0,0:16:06.88,0:16:09.51,Default#3,,0,0,0,,成功の可否を\N委ねるべきではなかった
Dialogue: 0,0:16:09.59,0:16:11.76,Default#3,,0,0,0,,あらゆる事態を予測し 準備し
Dialogue: 0,0:16:11.84,0:16:15.35,Default#3,,0,0,0,,周到な計画に基づいて行動するのが\Nスパイの鉄則
Dialogue: 0,0:16:15.43,0:16:19.85,Default#3,,0,0,0,,ならば 想定される面接官の質問を\Nひとつ残らず列挙し
Dialogue: 0,0:16:19.93,0:16:22.69,Default#3,,0,0,0,,それに対する完璧な回答を\N２人に暗記させる
Dialogue: 0,0:16:22.77,0:16:24.44,Default#3,,0,0,0,,あるいは 俺が\Nフォローできるような…
Dialogue: 0,0:16:24.52,0:16:25.36,Default#3,,0,0,0,,（ヨル）あの…
Dialogue: 0,0:16:25.94,0:16:26.73,Default#3,,0,0,0,,ロイドさん
Dialogue: 0,0:16:27.57,0:16:29.82,Default#3,,0,0,0,,少々 気晴らしはどうですか？
Dialogue: 0,0:16:35.78,0:16:37.87,Default#3,,0,0,0,,（アーニャ）わあ～\N（ロイド）おお～
Dialogue: 0,0:16:40.58,0:16:42.58,Default#3,,0,0,0,,（アーニャ）人がゴミのようだ
Dialogue: 0,0:16:42.66,0:16:43.62,Default#3,,0,0,0,,（ロイド）どこで覚えた
Dialogue: 0,0:16:44.21,0:16:45.21,Default#3,,0,0,0,,（アーニャ）アニメ
Dialogue: 0,0:16:45.29,0:16:45.96,Default#3,,0,0,0,,（ロイド）うん
Dialogue: 0,0:16:46.75,0:16:49.50,Default#3,,0,0,0,,街外れに\Nこんな公園があったとは
Dialogue: 0,0:16:49.59,0:16:51.97,Default#3,,0,0,0,,（ヨル）\N私も たまにしか来ませんが
Dialogue: 0,0:16:52.05,0:16:55.47,Default#3,,0,0,0,,仕事で疲れたときとかに ふらっと
Dialogue: 0,0:16:56.05,0:16:57.80,Default#3,,0,0,0,,私のしてることは
Dialogue: 0,0:16:57.89,0:17:01.68,Default#3,,0,0,0,,ここで暮らしてる方々の\N役に立ってるんだって思うと
Dialogue: 0,0:17:01.77,0:17:03.48,Default#3,,0,0,0,,また頑張ろうって…
Dialogue: 0,0:17:04.56,0:17:08.40,Default#3,,0,0,0,,あっ こんな所 退屈ですよね\Nすみません！
Dialogue: 0,0:17:08.48,0:17:10.48,Default#3,,0,0,0,,アーニャ 人いっぱいより好き
Dialogue: 0,0:17:11.19,0:17:12.24,Default#3,,0,0,0,,（ヨル）よかったです
Dialogue: 0,0:17:20.20,0:17:20.83,Default#3,,0,0,0,,むっ？
Dialogue: 0,0:17:22.33,0:17:22.83,Default#3,,0,0,0,,あっ
Dialogue: 0,0:17:23.66,0:17:25.83,Default#3,,0,0,0,,（老女）ひったくりよ 誰か！
Dialogue: 0,0:17:25.92,0:17:27.42,Default#3,,0,0,0,,不用心なばあさんだ
Dialogue: 0,0:17:29.42,0:17:30.38,Default#3,,0,0,0,,許せません！
Dialogue: 0,0:17:30.46,0:17:32.30,Default#3,,0,0,0,,待ちなさ～い！
Dialogue: 0,0:17:32.38,0:17:33.01,Default#3,,0,0,0,,（ロイド）あっ
Dialogue: 0,0:17:33.09,0:17:34.38,Default#3,,0,0,0,,（ロイド）しかたない
Dialogue: 0,0:17:38.76,0:17:39.89,Default#3,,0,0,0,,（ヨル）あっさり見失いました
Dialogue: 0,0:17:41.10,0:17:42.64,Default#3,,0,0,0,,大丈夫ですか？　おケガは？
Dialogue: 0,0:17:42.72,0:17:44.81,Default#3,,0,0,0,,（老女）平気よ かすり傷
Dialogue: 0,0:17:45.31,0:17:48.73,Default#3,,0,0,0,,必ず捕まえますので！\Nあとで病院へお連れします
Dialogue: 0,0:17:49.27,0:17:51.11,Default#3,,0,0,0,,あらまあ ありがとう
Dialogue: 0,0:17:51.19,0:17:51.69,Default#3,,0,0,0,,（ヨル）あっ
Dialogue: 0,0:17:51.78,0:17:53.65,Default#3,,0,0,0,,（ヨル）\Nロイドさんたちを置き去りに
Dialogue: 0,0:17:58.20,0:18:00.66,Default#3,,0,0,0,,（ロイド）チッ 人混みに紛れたか
Dialogue: 0,0:18:01.41,0:18:02.74,Default#3,,0,0,0,,ヨルさんには悪いが
Dialogue: 0,0:18:02.83,0:18:04.66,Default#3,,0,0,0,,こうなっては\Nもう見つからんだろう
Dialogue: 0,0:18:07.00,0:18:08.67,Default#3,,0,0,0,,（通行人）あ～ 腹減った
Dialogue: 0,0:18:08.75,0:18:10.29,Default#3,,0,0,0,,（通行人）高いな\N（通行人）わ～ これかわいい
Dialogue: 0,0:18:10.38,0:18:12.67,Default#3,,0,0,0,,（通行人）ヤベえ 漏れそう\N（通行人）足 イテえ
Dialogue: 0,0:18:12.75,0:18:14.88,Default#3,,0,0,0,,（通行人）\N100万ダルクくらい落ちてねえかな
Dialogue: 0,0:18:14.96,0:18:18.18,Default#3,,0,0,0,,（通行人たちの心の声）
Dialogue: 0,0:18:19.09,0:18:21.51,Default#3,,0,0,0,,ん！　おい お前また…
Dialogue: 0,0:18:22.26,0:18:26.68,Default#3,,0,0,0,,（ひったくり）ヘヘッ あのババア\Nたんまり持ってやがるじゃんか
Dialogue: 0,0:18:28.77,0:18:29.73,Default#3,,0,0,0,,ちち！
Dialogue: 0,0:18:29.81,0:18:31.06,Default#3,,0,0,0,,うおっ どうした？
Dialogue: 0,0:18:31.15,0:18:32.69,Default#3,,0,0,0,,ケーキ 食べたい！
Dialogue: 0,0:18:32.77,0:18:33.61,Default#3,,0,0,0,,（ロイド）なぬっ？
Dialogue: 0,0:18:34.28,0:18:36.32,Default#3,,0,0,0,,さっき レストラン出たばかり…
Dialogue: 0,0:18:38.32,0:18:39.74,Default#3,,0,0,0,,（ロイド）あいつ…
Dialogue: 0,0:18:39.82,0:18:41.41,Default#3,,0,0,0,,服装は違うが
Dialogue: 0,0:18:41.49,0:18:44.58,Default#3,,0,0,0,,歩き方の癖というのは\Nそうそう変えられんものだ
Dialogue: 0,0:18:44.66,0:18:46.08,Default#3,,0,0,0,,俺を欺けると思うなよ
Dialogue: 0,0:18:46.16,0:18:47.54,Default#3,,0,0,0,,（ヨル）ロイドさ～ん
Dialogue: 0,0:18:48.04,0:18:49.25,Default#3,,0,0,0,,（アーニャ）はは 来た
Dialogue: 0,0:18:49.33,0:18:50.96,Default#3,,0,0,0,,（ロイド）\Nヨルさん アーニャを頼む
Dialogue: 0,0:18:51.04,0:18:51.75,Default#3,,0,0,0,,（ひったくり）ヒヒッ
Dialogue: 0,0:18:51.84,0:18:54.59,Default#3,,0,0,0,,こんだけありゃ\Nしばらく遊んで暮らせるな
Dialogue: 0,0:18:54.67,0:18:56.88,Default#3,,0,0,0,,とりあえず\Nうまいもんでも食いに…
Dialogue: 0,0:18:58.88,0:19:00.01,Default#3,,0,0,0,,（ひったくり）ぐわっ
Dialogue: 0,0:19:01.05,0:19:04.06,Default#3,,0,0,0,,（ロイド）\N貴様には臭いメシが妥当だ
Dialogue: 0,0:19:08.60,0:19:11.52,Default#3,,0,0,0,,（ロイド）\Nスパイは目立ってはいけない…
Dialogue: 0,0:19:11.60,0:19:13.61,Default#3,,0,0,0,,（ざわめき）
Dialogue: 0,0:19:14.48,0:19:17.07,Default#3,,0,0,0,,あの こいつ\Nひったくり犯なので
Dialogue: 0,0:19:17.15,0:19:19.28,Default#3,,0,0,0,,警察 呼んで\N突き出しといてください
Dialogue: 0,0:19:23.66,0:19:26.12,Default#3,,0,0,0,,（老女）わざわざ送っていただいて
Dialogue: 0,0:19:26.20,0:19:28.91,Default#3,,0,0,0,,孫にあげるプレゼントのお金\N入ってたのよ
Dialogue: 0,0:19:29.58,0:19:31.79,Default#3,,0,0,0,,ありがとうね 助かったわ
Dialogue: 0,0:19:31.88,0:19:32.92,Default#3,,0,0,0,,（ヨル）あっ いえ…
Dialogue: 0,0:19:33.00,0:19:35.88,Default#3,,0,0,0,,取り返してくれたのは\Nうちのオット…
Dialogue: 0,0:19:35.96,0:19:37.96,Default#3,,0,0,0,,シュジンですので
Dialogue: 0,0:19:38.05,0:19:39.09,Default#3,,0,0,0,,いや
Dialogue: 0,0:19:39.17,0:19:42.55,Default#3,,0,0,0,,僕はヨルさんがいなかったら\N追いかけもしなかった
Dialogue: 0,0:19:42.64,0:19:45.81,Default#3,,0,0,0,,ありがとうね いい男だね
Dialogue: 0,0:19:45.89,0:19:47.06,Default#3,,0,0,0,,（ロイド）いや だから…
Dialogue: 0,0:19:47.85,0:19:49.48,Default#3,,0,0,0,,（局長）影なき英雄よ
Dialogue: 0,0:19:49.98,0:19:53.10,Default#3,,0,0,0,,君たち エージェントの活躍が\N日の目を見ることはない
Dialogue: 0,0:19:53.61,0:19:55.06,Default#3,,0,0,0,,（老女）ありがとう
Dialogue: 0,0:19:56.57,0:20:00.03,Default#3,,0,0,0,,（ロイド）たまには まあ\N感謝されるのも悪くないか
Dialogue: 0,0:20:00.11,0:20:01.53,Default#3,,0,0,0,,（アーニャ）ちち ツンデレ
Dialogue: 0,0:20:02.95,0:20:04.41,Default#3,,0,0,0,,（ロイド）ありがとう ヨルさん
Dialogue: 0,0:20:06.41,0:20:08.75,Default#3,,0,0,0,,今日は いい気分転換になりました
Dialogue: 0,0:20:08.83,0:20:11.16,Default#3,,0,0,0,,僕も改めて仕事 頑張ります
Dialogue: 0,0:20:17.59,0:20:19.30,Default#3,,0,0,0,,ちちとはは イチャイチャ
Dialogue: 0,0:20:19.38,0:20:20.17,Default#3,,0,0,0,,（２人）ハッ…
Dialogue: 0,0:20:20.26,0:20:21.05,Default#3,,0,0,0,,（ロイド）してない！\N（ヨル）してません！
Dialogue: 0,0:20:21.13,0:20:23.84,Default#3,,0,0,0,,（アーニャ）アーニャには\Nなんか ありがとうない？
Dialogue: 0,0:20:23.93,0:20:25.09,Default#3,,0,0,0,,（ロイド）そうだな
Dialogue: 0,0:20:25.18,0:20:28.35,Default#3,,0,0,0,,お前の胃袋のおかげで\N犯人 見つけたからな
Dialogue: 0,0:20:28.43,0:20:29.52,Default#3,,0,0,0,,偉い 偉い
Dialogue: 0,0:20:29.60,0:20:30.56,Default#3,,0,0,0,,（老女）フフフッ
Dialogue: 0,0:20:31.48,0:20:34.85,Default#3,,0,0,0,,あなたたち\Nとってもすてきな家族ね
Dialogue: 0,0:20:36.94,0:20:38.57,Default#3,,0,0,0,,飴(あめ)ちゃん あげる
Dialogue: 0,0:20:38.65,0:20:40.03,Default#3,,0,0,0,,（アーニャ）飴ちゃん
Dialogue: 0,0:20:40.11,0:20:41.65,Default#3,,0,0,0,,（ロイド）ちゃんとお礼言え
Dialogue: 0,0:20:45.66,0:20:47.53,Default#3,,0,0,0,,（アーニャ）おでけけ 終わり～
Dialogue: 0,0:20:47.62,0:20:49.58,Default#3,,0,0,0,,（ロイド）長い１日だったな
Dialogue: 0,0:20:52.66,0:20:53.58,Default#3,,0,0,0,,（アーニャ）はは
Dialogue: 0,0:20:54.58,0:20:56.08,Default#3,,0,0,0,,アーニャ ココア飲みたい
Dialogue: 0,0:20:57.79,0:20:59.09,Default#3,,0,0,0,,承りました
Dialogue: 0,0:21:05.43,0:21:06.22,Default#3,,0,0,0,,おい
Dialogue: 0,0:21:06.30,0:21:08.97,Default#3,,0,0,0,,それよりも\N模擬面接に再チャレンジだ
Dialogue: 0,0:21:10.10,0:21:12.43,Default#3,,0,0,0,,はい アーニャさん 休日は何を？
Dialogue: 0,0:21:12.52,0:21:16.19,Default#3,,0,0,0,,オペラ観(み)て 美術館行って\Nレストランで食べたりします
Dialogue: 0,0:21:16.27,0:21:18.27,Default#3,,0,0,0,,（ロイド）そう！　それ
Dialogue: 0,0:21:18.36,0:21:20.48,Default#3,,0,0,0,,何か聞かれたら 今日のこと言え
Dialogue: 0,0:21:20.57,0:21:22.44,Default#3,,0,0,0,,よし じゃあ 次の質問
Dialogue: 0,0:21:22.94,0:21:26.36,Default#3,,0,0,0,,お友達が悪いことを\Nしているところを見てしまいました
Dialogue: 0,0:21:26.45,0:21:27.28,Default#3,,0,0,0,,どうしますか？
Dialogue: 0,0:21:31.87,0:21:35.33,Default#3,,0,0,0,,上から飛び乗って やっつけて\N臭いメシ食わせます
Dialogue: 0,0:21:35.42,0:21:38.38,Default#3,,0,0,0,,（ロイド）うん\Nそこは見なかったことにしとけ
Dialogue: 0,0:21:38.88,0:21:41.55,Default#3,,0,0,0,,（ロイド）ダメだ やはり諦めよう
Dialogue: 0,0:21:42.92,0:21:44.80,Default#3,,0,0,0,,ちち カッコよかった！
Dialogue: 0,0:21:44.88,0:21:46.68,Default#3,,0,0,0,,（ヨル）カッコよかったですね
Dialogue: 0,0:21:46.76,0:21:48.89,Default#3,,0,0,0,,（アーニャ）ずどば～ん！　ぐっ！
Dialogue: 0,0:21:48.97,0:21:51.06,Default#3,,0,0,0,,臭いメシだ！
Dialogue: 0,0:21:57.44,0:21:58.31,Default#3,,0,0,0,,フフッ
Dialogue: 0,0:22:02.78,0:22:03.65,Default#3,,0,0,0,,フウ…
Dialogue: 0,0:22:04.78,0:22:06.53,Default#3,,0,0,0,,ココア 最高
Dialogue: 0,0:22:07.20,0:22:10.74,Default#3,,0,0,0,,（老女）あなたたち\Nとってもすてきな家族ね
Dialogue: 0,0:22:12.37,0:22:12.95,Default#3,,0,0,0,,フウ
Dialogue: 0,0:22:13.79,0:22:16.04,Default#3,,0,0,0,,（ロイド）\Nそう見えたのなら まあ⸺
Dialogue: 0,0:22:16.71,0:22:20.88,Default#3,,0,0,0,,準備の１％くらいは進んだと\N思うことにするか
Dialogue: 0,0:22:30.05,0:22:32.06,Default#3,,0,0,0,,♪～
Dialogue: 0,0:23:57.89,0:23:59.89,Default#3,,0,0,0,,～♪
