Dialogue: 0,0:20:54.40,0:20:57.74,JP_ED-furigana,NTP,0,0,0,,{\pos(393.421875,660.5)\an8}こころ
Dialogue: 0,0:20:54.40,0:20:57.74,JP_ED-furigana,NTP,0,0,0,,{\pos(451.140625,660.5)\an8}つよ
Dialogue: 0,0:20:58.20,0:21:01.53,JP_ED-furigana,NTP,0,0,0,,{\pos(436.7109375,660.5)\an8}み
Dialogue: 0,0:21:02.18,0:21:05.78,JP_ED-furigana,NTP,0,0,0,,{\pos(422.28125,660.5)\an8}はし
Dialogue: 0,0:21:02.18,0:21:05.78,JP_ED-furigana,NTP,0,0,0,,{\pos(480,660.5)\an8}つづ
Dialogue: 0,0:21:18.66,0:21:22.33,JP_ED-furigana,NTP,0,0,0,,{\pos(407.8515625,660.5)\an8}とお
Dialogue: 0,0:21:18.66,0:21:22.33,JP_ED-furigana,NTP,0,0,0,,{\pos(465.5703125,660.5)\an8}ひ
Dialogue: 0,0:21:18.66,0:21:22.33,JP_ED-furigana,NTP,0,0,0,,{\pos(523.2890625,660.5)\an8}ゆめ
Dialogue: 0,0:21:23.05,0:21:27.27,JP_ED-furigana,NTP,0,0,0,,{\pos(407.8515625,660.5)\an8}いま
