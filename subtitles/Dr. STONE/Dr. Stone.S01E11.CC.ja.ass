Dialogue: 0,0:00:06.41,0:00:08.96,Default#3,,0,0,0,,（コハク･金狼(キンロー)）\Nふっ！　ふっ！　ふっ！
Dialogue: 0,0:00:09.92,0:00:10.79,Default#3,,0,0,0,,（金狼）覇(は)～！
Dialogue: 0,0:00:10.92,0:00:11.84,Default#3,,0,0,0,,（金狼･コハク）うっ！
Dialogue: 0,0:00:11.96,0:00:13.00,Default#3,,0,0,0,,（コハク）攻撃が浅いぞ！
Dialogue: 0,0:00:13.13,0:00:14.71,Default#3,,0,0,0,,はっ！\N（銀狼(ギンロー)）ひいいいい…
Dialogue: 0,0:00:15.05,0:00:17.42,Default#3,,0,0,0,,（コハク）我々の誰かが\N御前試合で優勝し―
Dialogue: 0,0:00:18.01,0:00:20.30,Default#3,,0,0,0,,ルリ姉に万能薬を飲ませるのだ！
Dialogue: 0,0:00:20.93,0:00:21.97,Default#3,,0,0,0,,（金狼）うっ！
Dialogue: 0,0:00:23.30,0:00:24.60,Default#3,,0,0,0,,（千空(せんくう)）さ～て…
Dialogue: 0,0:00:24.72,0:00:27.35,Default#3,,0,0,0,,ゲンが だまして\N司(つかさ) 食い止めてる間に―
Dialogue: 0,0:00:27.48,0:00:29.73,Default#3,,0,0,0,,とっとと万能薬作り進めんぞ
Dialogue: 0,0:00:29.98,0:00:32.48,Default#3,,0,0,0,,（クロム）おうよ！\N次は科学で何作る？
Dialogue: 0,0:00:33.36,0:00:37.61,Default#3,,0,0,0,,文明作りの資材に\N鉄とタメ張る大事なもんがある
Dialogue: 0,0:00:38.24,0:00:43.16,Default#3,,0,0,0,,人類が歴史上初めて\N科学で合成した人工資材
Dialogue: 0,0:00:43.91,0:00:46.75,Default#3,,0,0,0,,それは ガラスだ
Dialogue: 0,0:00:47.66,0:00:50.17,Default#3,,0,0,0,,（スイカ）え？\N（クロム）おう 何だ そりゃ？
Dialogue: 0,0:00:50.29,0:00:51.67,Default#3,,0,0,0,,電気もゲットしたし―
Dialogue: 0,0:00:51.79,0:00:54.04,Default#3,,0,0,0,,こっから いろんな薬\N作んじゃねえのかよ
Dialogue: 0,0:00:54.17,0:00:55.46,Default#3,,0,0,0,,（千空）ああ そうだ
Dialogue: 0,0:00:56.13,0:00:58.80,Default#3,,0,0,0,,万能薬 サルファ剤に向けて―
Dialogue: 0,0:00:58.92,0:01:02.18,Default#3,,0,0,0,,楽しい化学(ばけがく)実験は\N土器じゃ もう無理ゲーだ
Dialogue: 0,0:01:02.55,0:01:05.43,Default#3,,0,0,0,,ガラスなら\Nほとんどの薬品に耐える
Dialogue: 0,0:01:05.56,0:01:08.31,Default#3,,0,0,0,,ガラスは化学(ばけがく)の原点なんだよ\N（クロム）あっ
Dialogue: 0,0:01:09.31,0:01:11.81,Default#3,,0,0,0,,（千空）加工しやすさ100億％！
Dialogue: 0,0:01:11.94,0:01:14.23,Default#3,,0,0,0,,なのにアホほど硬くて強(つえ)え！
Dialogue: 0,0:01:14.36,0:01:18.11,Default#3,,0,0,0,,スッケスケだから\N薬品の状態まで丸見えだ！
Dialogue: 0,0:01:18.53,0:01:20.65,Default#3,,0,0,0,,マジか… ヤベえ！
Dialogue: 0,0:01:20.99,0:01:22.11,Default#3,,0,0,0,,ククク…
Dialogue: 0,0:01:22.24,0:01:24.78,Default#3,,0,0,0,,ついでにな\Nガラスさえありゃ―
Dialogue: 0,0:01:25.37,0:01:27.87,Default#3,,0,0,0,,その邪魔くせえ\Nかぶりもん 外して―
Dialogue: 0,0:01:27.99,0:01:30.04,Default#3,,0,0,0,,名探偵スイカの謎の素顔\N拝めっかもしんねえぞ
Dialogue: 0,0:01:30.04,0:01:31.21,Default#3,,0,0,0,,名探偵スイカの謎の素顔\N拝めっかもしんねえぞ
Dialogue: 0,0:01:30.04,0:01:31.21,Default#3,,0,0,0,,（スイカ）えっ！
Dialogue: 0,0:01:31.21,0:01:31.83,Default#3,,0,0,0,,名探偵スイカの謎の素顔\N拝めっかもしんねえぞ
Dialogue: 0,0:01:31.96,0:01:32.92,Default#3,,0,0,0,,ああああ…
Dialogue: 0,0:01:33.29,0:01:34.33,Default#3,,0,0,0,,（千空）えい\N（クロム）あっ！
Dialogue: 0,0:01:34.46,0:01:35.54,Default#3,,0,0,0,,（コハク）待て 千空！
Dialogue: 0,0:01:35.67,0:01:38.17,Default#3,,0,0,0,,スイカは顔を見られたくないと…\N（クロム）ああああ…
Dialogue: 0,0:01:38.30,0:01:39.80,Default#3,,0,0,0,,よく見やがれ
Dialogue: 0,0:01:40.47,0:01:42.76,Default#3,,0,0,0,,（コハク）おお～！
Dialogue: 0,0:01:42.88,0:01:46.26,Default#3,,0,0,0,,何だ スイカ\Nめっぽう かわいいじゃないか！
Dialogue: 0,0:01:46.39,0:01:47.89,Default#3,,0,0,0,,（スイカ）ああ…
Dialogue: 0,0:01:48.01,0:01:48.85,Default#3,,0,0,0,,ああああ…
Dialogue: 0,0:01:48.97,0:01:50.73,Default#3,,0,0,0,,（コハク･クロム）\Nどういうことだ!?
Dialogue: 0,0:01:51.27,0:01:56.27,Default#3,,0,0,0,,♪～
Dialogue: 0,0:03:15.89,0:03:20.90,Default#3,,0,0,0,,～♪
Dialogue: 0,0:03:26.40,0:03:29.82,Default#3,,0,0,0,,（スイカ）スイカの目はね\Nボヤボヤ病なんだよ
Dialogue: 0,0:03:30.20,0:03:33.54,Default#3,,0,0,0,,ぼんやりしか見えなくて\N無理やり見ようとして―
Dialogue: 0,0:03:33.66,0:03:36.54,Default#3,,0,0,0,,こんなふうになっちゃうのが\N恥ずかしいんだよ
Dialogue: 0,0:03:39.00,0:03:40.71,Default#3,,0,0,0,,でも このマスクかぶってると―
Dialogue: 0,0:03:40.84,0:03:43.63,Default#3,,0,0,0,,なんでか ちょびっとだけ\N見やすいんだよ！　エヘヘヘ…
Dialogue: 0,0:03:43.76,0:03:46.84,Default#3,,0,0,0,,あ～ ピンホール効果ってやつな\N（スイカ）え？
Dialogue: 0,0:03:47.22,0:03:49.76,Default#3,,0,0,0,,（千空）穴からのぞいて\N光を狭めりゃ―
Dialogue: 0,0:03:49.89,0:03:52.68,Default#3,,0,0,0,,まあ 多少はピンボケが\N減るっつう理屈だ
Dialogue: 0,0:03:53.26,0:03:56.98,Default#3,,0,0,0,,そのスイカのボヤボヤ病が\Nガラスと関係あんのかよ？
Dialogue: 0,0:03:57.10,0:03:59.23,Default#3,,0,0,0,,100億％ありまくるわ！
Dialogue: 0,0:04:00.06,0:04:01.86,Default#3,,0,0,0,,よく聞け スイカ
Dialogue: 0,0:04:01.98,0:04:05.24,Default#3,,0,0,0,,テメーはな ド近眼だ\N（スイカ）あ…
Dialogue: 0,0:04:05.65,0:04:08.74,Default#3,,0,0,0,,それは病気じゃねえ\N欠陥でもねえ
Dialogue: 0,0:04:10.57,0:04:14.54,Default#3,,0,0,0,,科学文明の世界じゃ\N誰一人 気にもしねえ
Dialogue: 0,0:04:14.83,0:04:20.54,Default#3,,0,0,0,,ガラスで作る科学の目\N“眼鏡”ってのが全てを解決する
Dialogue: 0,0:04:22.75,0:04:25.84,Default#3,,0,0,0,,（スイカ）科学使いって\N目まで作れちゃうんだ…
Dialogue: 0,0:04:27.63,0:04:29.59,Default#3,,0,0,0,,すごすぎるんだよ
Dialogue: 0,0:04:30.76,0:04:31.84,Default#3,,0,0,0,,ああ
Dialogue: 0,0:04:33.39,0:04:35.68,Default#3,,0,0,0,,（スイカ）\Nスイカだってホントはね―
Dialogue: 0,0:04:36.27,0:04:40.94,Default#3,,0,0,0,,一度でいいから\Nきれいな世界 見てみたいんだよ
Dialogue: 0,0:04:42.56,0:04:44.98,Default#3,,0,0,0,,ボヤボヤじゃないみんなに…
Dialogue: 0,0:04:45.28,0:04:47.11,Default#3,,0,0,0,,ホントのみんなに…
Dialogue: 0,0:04:47.53,0:04:51.07,Default#3,,0,0,0,,一度でいいから\N会ってみたいんだよ
Dialogue: 0,0:04:52.70,0:04:54.87,Default#3,,0,0,0,,（クロム）おう！\N作ってやろうじゃねえか
Dialogue: 0,0:04:54.99,0:04:56.58,Default#3,,0,0,0,,ガラスってやつをよ！
Dialogue: 0,0:04:56.70,0:05:00.16,Default#3,,0,0,0,,（コハク）ああ！\N何を集めればいいのだ 千空
Dialogue: 0,0:05:00.96,0:05:04.63,Default#3,,0,0,0,,（千空）ガラスの材料は\Nほとんど珪砂(けいしゃ)っつう砂だ
Dialogue: 0,0:05:05.00,0:05:05.96,Default#3,,0,0,0,,クク…
Dialogue: 0,0:05:06.09,0:05:09.09,Default#3,,0,0,0,,さすがのクロムコレクションにも\Nなかったな
Dialogue: 0,0:05:09.22,0:05:11.26,Default#3,,0,0,0,,（クロム）いや 砂まで集めねえよ
Dialogue: 0,0:05:11.88,0:05:14.76,Default#3,,0,0,0,,ただな\N珪砂ゲットのヒントはあった
Dialogue: 0,0:05:15.18,0:05:16.35,Default#3,,0,0,0,,お？
Dialogue: 0,0:05:17.52,0:05:18.60,Default#3,,0,0,0,,（クロム）この辺だぜ！
Dialogue: 0,0:05:19.14,0:05:22.44,Default#3,,0,0,0,,ガキのころ\Nこの透明な石 ゲットしたのはよ
Dialogue: 0,0:05:22.56,0:05:25.98,Default#3,,0,0,0,,そのプチ情報だけで値千金だ
Dialogue: 0,0:05:27.19,0:05:29.65,Default#3,,0,0,0,,その透明な石 水晶は―
Dialogue: 0,0:05:29.78,0:05:34.20,Default#3,,0,0,0,,ここみてえな 花崗岩(かこうがん)とかが\N風化したエリアで採れる
Dialogue: 0,0:05:35.83,0:05:41.54,Default#3,,0,0,0,,そして この辺の砂が\N丸ごとガラスの原料… 珪砂だ
Dialogue: 0,0:05:41.79,0:05:43.00,Default#3,,0,0,0,,（３人）ああ！
Dialogue: 0,0:05:43.62,0:05:46.92,Default#3,,0,0,0,,ヤベえ！\N白い粒がボロボロ剥がせんぞ！
Dialogue: 0,0:05:47.05,0:05:48.92,Default#3,,0,0,0,,楽しいんだよ これ！
Dialogue: 0,0:05:49.59,0:05:51.38,Default#3,,0,0,0,,これはいっぱい採れそうだ
Dialogue: 0,0:05:51.76,0:05:54.34,Default#3,,0,0,0,,なるたけ透明な粒 集めまくれ！
Dialogue: 0,0:05:59.93,0:06:03.81,Default#3,,0,0,0,,（千空）集めた珪砂をすり潰して\N細けえ砂にする
Dialogue: 0,0:06:05.48,0:06:08.36,Default#3,,0,0,0,,あとは いろいろ混ぜて\N窯で溶かす
Dialogue: 0,0:06:08.48,0:06:09.98,Default#3,,0,0,0,,（クロム）ほかの材料は？
Dialogue: 0,0:06:10.11,0:06:12.82,Default#3,,0,0,0,,あ？\Nネタは とっくにそろってんだよ
Dialogue: 0,0:06:13.49,0:06:15.45,Default#3,,0,0,0,,ここまでの頑張りでな
Dialogue: 0,0:06:15.95,0:06:20.08,Default#3,,0,0,0,,炭酸カルシウム\Nつまり 貝殻を10％
Dialogue: 0,0:06:20.20,0:06:22.46,Default#3,,0,0,0,,焼いた海藻 20％
Dialogue: 0,0:06:23.62,0:06:27.79,Default#3,,0,0,0,,せっけん作ったとき\Nさんざん使った連中だ
Dialogue: 0,0:06:30.17,0:06:33.55,Default#3,,0,0,0,,（スイカ）フゥ フゥ フゥ フゥ…\N（千空）ハァハァ ハァハァ…
Dialogue: 0,0:06:33.67,0:06:35.18,Default#3,,0,0,0,,あとは…
Dialogue: 0,0:06:35.51,0:06:38.47,Default#3,,0,0,0,,来たぜ 方鉛鉱(ほうえんこう)！　鉛ゲット！
Dialogue: 0,0:06:39.26,0:06:41.43,Default#3,,0,0,0,,（千空）鉛さえブチ込めば…
Dialogue: 0,0:06:41.97,0:06:46.85,Default#3,,0,0,0,,レンズ用の\N超透明なクリスタルガラスが完成だ
Dialogue: 0,0:06:47.36,0:06:48.73,Default#3,,0,0,0,,おおお～っ！
Dialogue: 0,0:06:48.86,0:06:51.57,Default#3,,0,0,0,,氷みたいにキラキラ透明なんだよ！
Dialogue: 0,0:06:51.69,0:06:53.40,Default#3,,0,0,0,,宝石のようだ
Dialogue: 0,0:06:54.24,0:06:56.49,Default#3,,0,0,0,,（スイカ）\Nなんか変な道具があるんだよ？
Dialogue: 0,0:06:56.61,0:07:00.12,Default#3,,0,0,0,,（千空）ガラス削んのに\N手磨きだけじゃ 死ねるからな
Dialogue: 0,0:07:00.41,0:07:02.29,Default#3,,0,0,0,,（クラム）\Nおう！　削るといやあ―
Dialogue: 0,0:07:02.41,0:07:06.04,Default#3,,0,0,0,,例によって ヤベえ硬い\N鋼玉(コランダム)先生の出番だぜ
Dialogue: 0,0:07:06.17,0:07:09.04,Default#3,,0,0,0,,ガラスの硬さは\Nモース硬度６くらい
Dialogue: 0,0:07:09.17,0:07:11.17,Default#3,,0,0,0,,コランダム先生は９だ
Dialogue: 0,0:07:11.30,0:07:13.05,Default#3,,0,0,0,,余裕勝ちで削れるな
Dialogue: 0,0:07:13.17,0:07:13.42,Default#3,,0,0,0,,（コハク）また出たな 石トーク
Dialogue: 0,0:07:13.42,0:07:15.38,Default#3,,0,0,0,,（コハク）また出たな 石トーク
Dialogue: 0,0:07:13.42,0:07:15.38,Default#3,,0,0,0,,（千空）ちなみに\N最強はダイヤの10な
Dialogue: 0,0:07:15.38,0:07:15.51,Default#3,,0,0,0,,（千空）ちなみに\N最強はダイヤの10な
Dialogue: 0,0:07:15.51,0:07:16.05,Default#3,,0,0,0,,（千空）ちなみに\N最強はダイヤの10な
Dialogue: 0,0:07:15.51,0:07:16.05,Default#3,,0,0,0,,これが始まると\N私は ついていけん
Dialogue: 0,0:07:16.05,0:07:16.18,Default#3,,0,0,0,,これが始まると\N私は ついていけん
Dialogue: 0,0:07:16.18,0:07:17.39,Default#3,,0,0,0,,これが始まると\N私は ついていけん
Dialogue: 0,0:07:16.18,0:07:17.39,Default#3,,0,0,0,,（クロム）\Nダイヤ ヤベえ！
Dialogue: 0,0:07:17.39,0:07:17.51,Default#3,,0,0,0,,これが始まると\N私は ついていけん
Dialogue: 0,0:07:17.51,0:07:18.76,Default#3,,0,0,0,,これが始まると\N私は ついていけん
Dialogue: 0,0:07:17.51,0:07:18.76,Default#3,,0,0,0,,ハンパねえじゃねえか！
Dialogue: 0,0:08:11.19,0:08:15.65,Default#3,,0,0,0,,スイカ テメーの目の前に今\N何があるか見えっか？
Dialogue: 0,0:08:16.61,0:08:18.53,Default#3,,0,0,0,,（スイカ）普通にヒマワリだよ
Dialogue: 0,0:08:19.16,0:08:23.08,Default#3,,0,0,0,,別に見えてないわけじゃないんだよ\Nスイカだって…
Dialogue: 0,0:08:23.28,0:08:24.20,Default#3,,0,0,0,,うっ…
Dialogue: 0,0:08:43.85,0:08:47.68,Default#3,,0,0,0,,（チョークの鳴き声）
Dialogue: 0,0:09:16.59,0:09:18.84,Default#3,,0,0,0,,（金狼）うっ…\N（コハク）うおおおお！
Dialogue: 0,0:09:19.05,0:09:21.26,Default#3,,0,0,0,,うっ うっ…
Dialogue: 0,0:09:21.38,0:09:22.22,Default#3,,0,0,0,,覇っ！
Dialogue: 0,0:09:22.64,0:09:25.35,Default#3,,0,0,0,,ぐおっ… うっ…
Dialogue: 0,0:09:26.56,0:09:29.31,Default#3,,0,0,0,,やはりだ\Nなぜ 最後が浅い！
Dialogue: 0,0:09:29.43,0:09:31.39,Default#3,,0,0,0,,金狼 君の悪癖だぞ
Dialogue: 0,0:09:31.52,0:09:34.40,Default#3,,0,0,0,,（金狼の荒い息）\N（コハク）槍(やり)は間合いが命だ
Dialogue: 0,0:09:34.52,0:09:38.90,Default#3,,0,0,0,,距離感をつかみきれていなければ\Nマグマには決して勝てん！
Dialogue: 0,0:09:39.49,0:09:42.03,Default#3,,0,0,0,,（金狼）くっ… ハァ ハァ…
Dialogue: 0,0:09:42.15,0:09:45.20,Default#3,,0,0,0,,（銀狼）距離感… ねえ～
Dialogue: 0,0:09:49.08,0:09:52.12,Default#3,,0,0,0,,（銀狼）んなこと言ったって\N無理に決まってんじゃんねえ
Dialogue: 0,0:09:52.87,0:09:56.96,Default#3,,0,0,0,,だって金狼は ほら…\N目がボヤボヤ病なんだから
Dialogue: 0,0:09:57.67,0:10:01.05,Default#3,,0,0,0,,（銀狼）だから そんな\N目つき悪い人になっちゃってさあ
Dialogue: 0,0:10:01.17,0:10:02.47,Default#3,,0,0,0,,それは元からだ
Dialogue: 0,0:10:03.01,0:10:04.76,Default#3,,0,0,0,,意味分かんないよ
Dialogue: 0,0:10:04.89,0:10:07.35,Default#3,,0,0,0,,僕なら すぐ言っちゃうけどな～
Dialogue: 0,0:10:07.47,0:10:11.31,Default#3,,0,0,0,,僕のせいじゃないです～\N目のせいなんです～
Dialogue: 0,0:10:11.43,0:10:14.19,Default#3,,0,0,0,,ホントは\Nもっと強いはずなんです～って
Dialogue: 0,0:10:14.69,0:10:17.19,Default#3,,0,0,0,,言い訳は大の男のすることではない
Dialogue: 0,0:10:17.82,0:10:20.15,Default#3,,0,0,0,,誰にも言うなよ 銀狼
Dialogue: 0,0:10:21.44,0:10:23.65,Default#3,,0,0,0,,（チョークの鳴き声）
Dialogue: 0,0:10:23.78,0:10:26.70,Default#3,,0,0,0,,（スイカ）千空もクロムも\N意外とかっこよかったんだね
Dialogue: 0,0:10:27.16,0:10:29.37,Default#3,,0,0,0,,“意外と”って何だよ クソ
Dialogue: 0,0:10:29.49,0:10:30.75,Default#3,,0,0,0,,（千空）ククク…\N（クロム･スイカ）ん？
Dialogue: 0,0:10:30.87,0:10:33.66,Default#3,,0,0,0,,（千空）次は いよいよ\Nガラス器具作りだな
Dialogue: 0,0:10:34.08,0:10:37.63,Default#3,,0,0,0,,スイカの労働力も\N超絶レベルアップしたし
Dialogue: 0,0:10:37.75,0:10:40.76,Default#3,,0,0,0,,化学(ばけがく)のアイテムを\N一気にそろえまくんぞ
Dialogue: 0,0:10:40.88,0:10:42.42,Default#3,,0,0,0,,“労働力”言うな！
Dialogue: 0,0:10:42.55,0:10:43.97,Default#3,,0,0,0,,いい話でまとめとけよ！
Dialogue: 0,0:10:44.26,0:10:45.34,Default#3,,0,0,0,,（スイカ）ヘヘッ フフフ…
Dialogue: 0,0:10:45.47,0:10:47.85,Default#3,,0,0,0,,（クロム）\Nお前 何笑ってん…
Dialogue: 0,0:10:47.97,0:10:49.97,Default#3,,0,0,0,,（クロム･スイカ）\Nハハハハ！
Dialogue: 0,0:10:55.35,0:10:59.40,Default#3,,0,0,0,,（千空）ククク\N楽しいガラス細工教室のスタートだ
Dialogue: 0,0:10:59.52,0:11:01.61,Default#3,,0,0,0,,おう！　ガンガン作ってくぜ
Dialogue: 0,0:11:02.11,0:11:04.20,Default#3,,0,0,0,,この黒い石 入れりゃいいんだな
Dialogue: 0,0:11:04.70,0:11:07.07,Default#3,,0,0,0,,（スイカ）\Nどんどん運んでくるんだよ～！
Dialogue: 0,0:11:07.20,0:11:08.70,Default#3,,0,0,0,,ど～ん！\N（クロム）うおっ！
Dialogue: 0,0:11:08.82,0:11:11.99,Default#3,,0,0,0,,ますます ちょこまかスピード\Nアップしてんな スイカ
Dialogue: 0,0:11:12.12,0:11:15.29,Default#3,,0,0,0,,ボヤボヤじゃなくなったから\N失敗しないんだよ～
Dialogue: 0,0:11:15.96,0:11:19.13,Default#3,,0,0,0,,あっ なんか白い泡みたいの\N出てきたんだよ？
Dialogue: 0,0:11:19.67,0:11:20.75,Default#3,,0,0,0,,こいつは
Dialogue: 0,0:11:20.88,0:11:24.47,Default#3,,0,0,0,,黒曜石をあぶると湧いてくる\N発泡体だ
Dialogue: 0,0:11:25.88,0:11:30.76,Default#3,,0,0,0,,ゲットした泡を断熱材にして\Nガラスの整形窯を作る
Dialogue: 0,0:11:31.93,0:11:38.56,Default#3,,0,0,0,,あとは鉄のストロー作って\Nガラスを吹いて膨らますだけだ
Dialogue: 0,0:11:40.44,0:11:43.44,Default#3,,0,0,0,,（息を吹く音）
Dialogue: 0,0:11:48.57,0:11:51.58,Default#3,,0,0,0,,（一同）ぬ～ん…
Dialogue: 0,0:11:51.70,0:11:55.29,Default#3,,0,0,0,,（千空）ククク\Nまあ 最初はこんなもんだ
Dialogue: 0,0:11:55.41,0:11:57.25,Default#3,,0,0,0,,土器んときも\Nまともなもん作んのに―
Dialogue: 0,0:11:57.37,0:11:59.04,Default#3,,0,0,0,,何か月もかかった
Dialogue: 0,0:11:59.17,0:12:00.58,Default#3,,0,0,0,,何か月!?
Dialogue: 0,0:12:00.71,0:12:02.80,Default#3,,0,0,0,,あんま悠長にしちゃ\Nいらんねえぜ！
Dialogue: 0,0:12:02.92,0:12:05.38,Default#3,,0,0,0,,ルリ姉の容体も心配だしな
Dialogue: 0,0:12:05.51,0:12:07.76,Default#3,,0,0,0,,ガラス職人じゃねえんだ
Dialogue: 0,0:12:07.88,0:12:09.80,Default#3,,0,0,0,,トライ＆エラーしかねえだろ
Dialogue: 0,0:12:09.93,0:12:12.76,Default#3,,0,0,0,,あっ！　職人…
Dialogue: 0,0:12:12.89,0:12:14.31,Default#3,,0,0,0,,ん…
Dialogue: 0,0:12:18.39,0:12:19.94,Default#3,,0,0,0,,（足音）\N（千空）ん？
Dialogue: 0,0:12:22.86,0:12:26.36,Default#3,,0,0,0,,（カセキ）むむ…\Nこら やめんか クロム
Dialogue: 0,0:12:27.19,0:12:29.41,Default#3,,0,0,0,,（千空）誰だ あのジジイ？
Dialogue: 0,0:12:30.74,0:12:32.70,Default#3,,0,0,0,,頼むぜ カセキのじいさん！
Dialogue: 0,0:12:32.83,0:12:34.83,Default#3,,0,0,0,,あんたの職人技 借りてえんだ！
Dialogue: 0,0:12:35.20,0:12:40.54,Default#3,,0,0,0,,荒縄で人を縛っちゃう変態プレーが\Nぬしのものの頼み方か？
Dialogue: 0,0:12:40.92,0:12:45.05,Default#3,,0,0,0,,ワシは怪しい術のたぐいは\Nい～っさい手伝わんぞい
Dialogue: 0,0:12:45.17,0:12:48.67,Default#3,,0,0,0,,ほら\Nそう言って来てくんねえからよ～
Dialogue: 0,0:12:48.80,0:12:50.47,Default#3,,0,0,0,,カセキじいちゃん\N（一同）ん？
Dialogue: 0,0:12:51.09,0:12:54.39,Default#3,,0,0,0,,（スイカ）スイカは うっかりして\Nウルシ返し忘れてたんだよ
Dialogue: 0,0:12:54.76,0:12:57.31,Default#3,,0,0,0,,おお そういえば…
Dialogue: 0,0:12:57.43,0:13:00.64,Default#3,,0,0,0,,じいさん あんたが作ったのか？
Dialogue: 0,0:13:00.85,0:13:03.65,Default#3,,0,0,0,,この年季もんのご立派な盾
Dialogue: 0,0:13:03.94,0:13:07.61,Default#3,,0,0,0,,鉄の道具も染料も\Nロクにねえ環境でよ
Dialogue: 0,0:13:07.74,0:13:08.65,Default#3,,0,0,0,,（カセキ）ああ
Dialogue: 0,0:13:08.78,0:13:11.11,Default#3,,0,0,0,,元はコハクの父 コクヨウがー
Dialogue: 0,0:13:11.24,0:13:14.53,Default#3,,0,0,0,,御前試合で優勝したとき\N贈った物でな
Dialogue: 0,0:13:15.24,0:13:18.79,Default#3,,0,0,0,,ワシも結構\Nガッツ入れて作っちゃったの～
Dialogue: 0,0:13:19.00,0:13:21.75,Default#3,,0,0,0,,めっちゃ懐かしいのう
Dialogue: 0,0:13:21.87,0:13:24.29,Default#3,,0,0,0,,（コハク）わ～ ハハハッ…
Dialogue: 0,0:13:24.79,0:13:25.92,Default#3,,0,0,0,,（コハク）フフッ
Dialogue: 0,0:13:27.38,0:13:30.88,Default#3,,0,0,0,,（千空）文明のわりに\N工作技術がクッソ高(たけ)えから―
Dialogue: 0,0:13:31.01,0:13:34.39,Default#3,,0,0,0,,腕の立つ職人がいんじゃねえかとは\N思ってたが―
Dialogue: 0,0:13:34.60,0:13:37.68,Default#3,,0,0,0,,こんなヨボヨボのじいさんとはな
Dialogue: 0,0:13:37.81,0:13:40.35,Default#3,,0,0,0,,あのすげえつり橋も\Nあんたの仕事か
Dialogue: 0,0:13:40.48,0:13:44.06,Default#3,,0,0,0,,ああ そうじゃ\N勝手に板 剥がしおって！
Dialogue: 0,0:13:44.19,0:13:47.19,Default#3,,0,0,0,,ぬしらに協力など絶対に…
Dialogue: 0,0:13:47.32,0:13:51.49,Default#3,,0,0,0,,おおっ！\Nこ… この透明な石細工は… あ…
Dialogue: 0,0:13:51.61,0:13:53.66,Default#3,,0,0,0,,（千空とクロムの笑い声）
Dialogue: 0,0:13:54.07,0:13:56.83,Default#3,,0,0,0,,（千空）あ～\N手伝いたくねえんなら ねえでいい
Dialogue: 0,0:13:57.12,0:13:59.66,Default#3,,0,0,0,,ち～っと そこで眺めてやがれ
Dialogue: 0,0:13:59.79,0:14:01.71,Default#3,,0,0,0,,面白(おもしれ)えもん見してやる
Dialogue: 0,0:14:01.83,0:14:03.79,Default#3,,0,0,0,,カセキ あんたにとっちゃ―
Dialogue: 0,0:14:03.92,0:14:09.00,Default#3,,0,0,0,,100億％ 唆(そそ)るぜ これは！
Dialogue: 0,0:14:09.38,0:14:13.30,Default#3,,0,0,0,,（カセキ）ふむ… 熱すると\N軟らかくなって変形するのか
Dialogue: 0,0:14:13.43,0:14:14.97,Default#3,,0,0,0,,脂身のようだ
Dialogue: 0,0:14:15.80,0:14:17.30,Default#3,,0,0,0,,（息を吹く音）
Dialogue: 0,0:14:17.43,0:14:20.56,Default#3,,0,0,0,,（カセキ）\Nなんという加工のしやすさ！
Dialogue: 0,0:14:20.68,0:14:23.27,Default#3,,0,0,0,,（カセキ）あああ… ああ…\N（クロム）ヤベえ また吹きすぎた
Dialogue: 0,0:14:23.39,0:14:26.23,Default#3,,0,0,0,,（カセキ）\Nああ… 何しとる！　まったく！
Dialogue: 0,0:14:26.36,0:14:27.19,Default#3,,0,0,0,,（千空）ヘヘヘヘ…
Dialogue: 0,0:14:30.69,0:14:32.15,Default#3,,0,0,0,,よっ よっ
Dialogue: 0,0:14:36.87,0:14:39.16,Default#3,,0,0,0,,（カセキ）回す速さにムラがある
Dialogue: 0,0:14:39.29,0:14:42.20,Default#3,,0,0,0,,それでは きれいな器に\Nならんじゃろに
Dialogue: 0,0:14:42.33,0:14:46.79,Default#3,,0,0,0,,まさか ワシをたきつけるために\Nわざとやっとるんか？
Dialogue: 0,0:14:46.92,0:14:49.67,Default#3,,0,0,0,,ああ… もう…
Dialogue: 0,0:14:49.92,0:14:53.88,Default#3,,0,0,0,,ククク… もの作り一筋の男がよ
Dialogue: 0,0:14:54.01,0:14:55.47,Default#3,,0,0,0,,ガラス細工なんつう
Dialogue: 0,0:14:55.59,0:14:58.35,Default#3,,0,0,0,,ヨダレ垂れまくるもん\N目の前にして
Dialogue: 0,0:14:58.47,0:15:01.68,Default#3,,0,0,0,,おとなしく座ってられるわきゃあ\Nねえよなあ？
Dialogue: 0,0:15:01.81,0:15:03.85,Default#3,,0,0,0,,カセキのじいさんよ
Dialogue: 0,0:15:04.10,0:15:07.35,Default#3,,0,0,0,,くう… まったく見ちゃおれん！
Dialogue: 0,0:15:07.48,0:15:09.82,Default#3,,0,0,0,,ふんんんん…
Dialogue: 0,0:15:09.94,0:15:10.82,Default#3,,0,0,0,,ふおっ！
Dialogue: 0,0:15:10.94,0:15:12.90,Default#3,,0,0,0,,まんまと乗っちゃうわい もう！
Dialogue: 0,0:15:13.03,0:15:13.90,Default#3,,0,0,0,,（千空･クロム）へ？
Dialogue: 0,0:15:15.15,0:15:16.70,Default#3,,0,0,0,,ワシにやらせろ
Dialogue: 0,0:15:16.95,0:15:18.24,Default#3,,0,0,0,,おおおおおお！
Dialogue: 0,0:15:18.37,0:15:20.70,Default#3,,0,0,0,,マジか そのマッスル
Dialogue: 0,0:15:22.49,0:15:25.21,Default#3,,0,0,0,,（カセキ）うううう…
Dialogue: 0,0:15:26.21,0:15:27.71,Default#3,,0,0,0,,（息を吹く音）
Dialogue: 0,0:15:33.96,0:15:35.30,Default#3,,0,0,0,,（カセキ）ふん…
Dialogue: 0,0:15:38.97,0:15:40.51,Default#3,,0,0,0,,スイカ 水！
Dialogue: 0,0:15:40.64,0:15:42.68,Default#3,,0,0,0,,はいっ なんだよ～！
Dialogue: 0,0:15:48.73,0:15:51.11,Default#3,,0,0,0,,ヤベえ すげえ！
Dialogue: 0,0:15:51.23,0:15:54.49,Default#3,,0,0,0,,カセキのじいさんだって\Nガラス見んの初めてだろ？
Dialogue: 0,0:15:54.61,0:15:58.45,Default#3,,0,0,0,,ククク\N工作自体の年季が違うからな
Dialogue: 0,0:15:58.66,0:16:02.91,Default#3,,0,0,0,,オホー！\Nこれでも50年 職人やっとるんよ
Dialogue: 0,0:16:03.04,0:16:05.29,Default#3,,0,0,0,,わりかしすごいんよ ワシ
Dialogue: 0,0:16:06.16,0:16:09.17,Default#3,,0,0,0,,いつの時代にも いるってこった
Dialogue: 0,0:16:09.29,0:16:12.96,Default#3,,0,0,0,,黙って人生 仕事にささげて\N生きてきた
Dialogue: 0,0:16:13.09,0:16:15.88,Default#3,,0,0,0,,本物の腕のオッサンがな
Dialogue: 0,0:16:36.40,0:16:37.57,Default#3,,0,0,0,,（カセキ･スイカ）イエーイ！\N（クロム）しゃー！
Dialogue: 0,0:16:37.70,0:16:39.53,Default#3,,0,0,0,,おお！
Dialogue: 0,0:16:52.54,0:16:54.80,Default#3,,0,0,0,,（クロム）おう 何だ？\Nどうしたよ 千空
Dialogue: 0,0:16:54.92,0:16:58.38,Default#3,,0,0,0,,ついにラボ ゲットしたんだぜ\Nもっと喜べよ！
Dialogue: 0,0:16:58.51,0:17:01.39,Default#3,,0,0,0,,（千空）ああ めでたい めでたい
Dialogue: 0,0:17:01.51,0:17:02.64,Default#3,,0,0,0,,フッ…
Dialogue: 0,0:17:03.76,0:17:06.18,Default#3,,0,0,0,,こっからが化学(ばけがく)の夜明けだ
Dialogue: 0,0:17:06.52,0:17:09.64,Default#3,,0,0,0,,いよいよ\N豪華になってきたじゃねえか
Dialogue: 0,0:17:09.77,0:17:14.44,Default#3,,0,0,0,,司ランドよか 100億倍楽しい\Nアトラクションがいっぱいの―
Dialogue: 0,0:17:14.86,0:17:17.44,Default#3,,0,0,0,,科学王国がよ
Dialogue: 0,0:17:22.28,0:17:23.95,Default#3,,0,0,0,,（メカ千空）\N久しぶりだな
Dialogue: 0,0:17:24.07,0:17:25.99,Default#3,,0,0,0,,俺の名は メカ千空
Dialogue: 0,0:17:26.12,0:17:28.79,Default#3,,0,0,0,,これが科学王国の\N全貌だぞ
Dialogue: 0,0:17:30.12,0:17:32.42,Default#3,,0,0,0,,科学王国の本拠地だ
Dialogue: 0,0:17:32.71,0:17:36.88,Default#3,,0,0,0,,クロムがガキんころから集めた\N鉱石の山だぞ！
Dialogue: 0,0:17:37.13,0:17:40.63,Default#3,,0,0,0,,へヘッ ヤベえお宝ばっかだぜ
Dialogue: 0,0:17:45.14,0:17:47.68,Default#3,,0,0,0,,（メカ千空）ご存じ科学のグルメ\N猫じゃらしラーメン
Dialogue: 0,0:17:47.68,0:17:49.10,Default#3,,0,0,0,,（メカ千空）ご存じ科学のグルメ\N猫じゃらしラーメン
Dialogue: 0,0:17:47.68,0:17:49.10,Default#3,,0,0,0,,（ガンエン）ん～！
Dialogue: 0,0:17:49.43,0:17:51.02,Default#3,,0,0,0,,（ガンエン）何杯でも食べれる
Dialogue: 0,0:17:51.14,0:17:54.90,Default#3,,0,0,0,,すっげえ！\Nそれでいて すっげえ～！
Dialogue: 0,0:17:59.15,0:18:01.99,Default#3,,0,0,0,,（メカ千空）\N気合いの人海戦術で鉄を作る
Dialogue: 0,0:18:02.11,0:18:04.16,Default#3,,0,0,0,,目指せ 1500度！
Dialogue: 0,0:18:05.07,0:18:06.87,Default#3,,0,0,0,,（シャベル）あのね あのね…
Dialogue: 0,0:18:06.99,0:18:10.54,Default#3,,0,0,0,,めちゃくちゃ… 大変だ～
Dialogue: 0,0:18:14.75,0:18:16.42,Default#3,,0,0,0,,（金狼）ふっ はっ うっ！
Dialogue: 0,0:18:16.63,0:18:17.13,Default#3,,0,0,0,,ぬっ！
Dialogue: 0,0:18:17.13,0:18:17.88,Default#3,,0,0,0,,ぬっ！
Dialogue: 0,0:18:17.13,0:18:17.88,Default#3,,0,0,0,,（メカ千空）\N打倒マグマ！　地獄の訓練場だ！
Dialogue: 0,0:18:17.88,0:18:18.46,Default#3,,0,0,0,,（メカ千空）\N打倒マグマ！　地獄の訓練場だ！
Dialogue: 0,0:18:18.46,0:18:20.42,Default#3,,0,0,0,,（メカ千空）\N打倒マグマ！　地獄の訓練場だ！
Dialogue: 0,0:18:18.46,0:18:20.42,Default#3,,0,0,0,,くっ… 覇！
Dialogue: 0,0:18:20.55,0:18:21.22,Default#3,,0,0,0,,つうか 単なる広場だがな
Dialogue: 0,0:18:21.22,0:18:22.72,Default#3,,0,0,0,,つうか 単なる広場だがな
Dialogue: 0,0:18:21.22,0:18:22.72,Default#3,,0,0,0,,（銀狼）\Nえ？　お～っと
Dialogue: 0,0:18:22.84,0:18:24.84,Default#3,,0,0,0,,友情！　努力！　勝利！
Dialogue: 0,0:18:24.97,0:18:26.35,Default#3,,0,0,0,,君が言うな！\N（銀狼）ええ…
Dialogue: 0,0:18:26.47,0:18:28.76,Default#3,,0,0,0,,最低すぎるぞ！　銀狼 貴様！\N（銀狼）あっ あっ…
Dialogue: 0,0:18:28.97,0:18:30.60,Default#3,,0,0,0,,（金狼･銀狼）ぬおおおお！
Dialogue: 0,0:18:31.02,0:18:35.23,Default#3,,0,0,0,,（メカ千空）気合いの腕力発電\N究極のエコだぞ！
Dialogue: 0,0:18:31.02,0:18:35.23,Default#3,,0,0,0,,（銀狼･金狼）\Nおりゃ～！
Dialogue: 0,0:18:39.73,0:18:42.07,Default#3,,0,0,0,,ぬお～！　はあ～！
Dialogue: 0,0:18:42.32,0:18:44.95,Default#3,,0,0,0,,（メカ千空）\N職人技でガラス細工を作る
Dialogue: 0,0:18:45.07,0:18:47.32,Default#3,,0,0,0,,粘土みたくグニャグニャになんぞ
Dialogue: 0,0:18:52.29,0:18:54.25,Default#3,,0,0,0,,作ったクソ熱いガラスを―
Dialogue: 0,0:18:54.54,0:18:57.84,Default#3,,0,0,0,,割れねえように\Nジワジワ冷やしていく
Dialogue: 0,0:19:00.63,0:19:02.51,Default#3,,0,0,0,,そして ラボ
Dialogue: 0,0:19:03.30,0:19:08.64,Default#3,,0,0,0,,ヤベえ薬品作りまくって\Nサルファ剤ルートばく進だ！
Dialogue: 0,0:19:16.52,0:19:19.06,Default#3,,0,0,0,,おう なんか壮観だな
Dialogue: 0,0:19:19.19,0:19:22.15,Default#3,,0,0,0,,ヤベえことになってきたぜ\N科学王国
Dialogue: 0,0:19:22.28,0:19:26.57,Default#3,,0,0,0,,ククク\Nギリ必要なもん作っただけだ
Dialogue: 0,0:19:26.70,0:19:30.83,Default#3,,0,0,0,,こん中のどれが欠けても\N先に進めやしねえ
Dialogue: 0,0:19:31.41,0:19:36.04,Default#3,,0,0,0,,崩壊前の世界もよ\Nもっとヤベえ科学王国だったのかよ
Dialogue: 0,0:19:36.33,0:19:37.54,Default#3,,0,0,0,,ああ
Dialogue: 0,0:19:38.29,0:19:40.29,Default#3,,0,0,0,,200万年かけてな
Dialogue: 0,0:19:42.00,0:19:45.72,Default#3,,0,0,0,,俺らは それに追いついて\N追い越す
Dialogue: 0,0:19:47.59,0:19:50.14,Default#3,,0,0,0,,（クロム）ヒヒヒヒ…\N（千空）フッ
Dialogue: 0,0:19:50.93,0:19:55.77,Default#3,,0,0,0,,まずは その１歩目\N万能薬 サルファ剤だ！
Dialogue: 0,0:19:56.44,0:19:57.48,Default#3,,0,0,0,,（銀狼）あっ！
Dialogue: 0,0:19:57.60,0:20:00.56,Default#3,,0,0,0,,（金狼･銀狼）ハァハァ ハァハァ…
Dialogue: 0,0:20:00.69,0:20:02.11,Default#3,,0,0,0,,（スイカ）イエーイ！\N（カセキ）イエーイ！
Dialogue: 0,0:20:02.23,0:20:03.44,Default#3,,0,0,0,,（スイカ）イエーイ！\N（カセキ）イエーイ！
Dialogue: 0,0:20:03.57,0:20:05.49,Default#3,,0,0,0,,（カセキ･スイカ）\Nイエイ イエイ イエイ イエイ
Dialogue: 0,0:20:06.40,0:20:09.11,Default#3,,0,0,0,,いいなあ 妖術チームは
Dialogue: 0,0:20:09.24,0:20:12.12,Default#3,,0,0,0,,なんかキラキラで 楽しそうで
Dialogue: 0,0:20:12.24,0:20:13.91,Default#3,,0,0,0,,僕らだってさあ…
Dialogue: 0,0:20:14.04,0:20:15.83,Default#3,,0,0,0,,あっ！　ねえ 金狼
Dialogue: 0,0:20:16.29,0:20:20.54,Default#3,,0,0,0,,もしもだよ この槍先が\Nキラッキラに光ってたら―
Dialogue: 0,0:20:20.67,0:20:22.25,Default#3,,0,0,0,,目がボヤボヤの金狼でも―
Dialogue: 0,0:20:22.38,0:20:26.22,Default#3,,0,0,0,,間合いがつかみやすくて\N練習になると思わない？
Dialogue: 0,0:20:28.63,0:20:31.60,Default#3,,0,0,0,,何を言ってるんだ 銀狼 貴様
Dialogue: 0,0:20:31.97,0:20:33.10,Default#3,,0,0,0,,ぐっ…
Dialogue: 0,0:20:33.72,0:20:34.72,Default#3,,0,0,0,,あれ～？
Dialogue: 0,0:20:34.85,0:20:36.85,Default#3,,0,0,0,,そういえば\Nキラキラ光るといえば―
Dialogue: 0,0:20:36.98,0:20:39.98,Default#3,,0,0,0,,ほら 金の槍と銀の槍！
Dialogue: 0,0:20:40.10,0:20:41.98,Default#3,,0,0,0,,やっぱり要るんじゃないの～？
Dialogue: 0,0:20:42.11,0:20:44.61,Default#3,,0,0,0,,あくまでも御前試合のためにね
Dialogue: 0,0:20:44.73,0:20:48.82,Default#3,,0,0,0,,勝つためには\N絶対 要るよね～ これ！
Dialogue: 0,0:20:50.20,0:20:51.87,Default#3,,0,0,0,,（金狼･銀狼）ふん！
Dialogue: 0,0:20:53.53,0:20:54.37,Default#3,,0,0,0,,（千空）あ？
Dialogue: 0,0:20:54.49,0:20:57.62,Default#3,,0,0,0,,（銀狼）はあ～！　はあ～…
Dialogue: 0,0:20:57.75,0:21:00.08,Default#3,,0,0,0,,戦いのことは分かんねえ
Dialogue: 0,0:21:00.21,0:21:03.34,Default#3,,0,0,0,,金と銀の槍があれば\Nマグマに勝てるって
Dialogue: 0,0:21:03.46,0:21:04.88,Default#3,,0,0,0,,これ マジ話か？
Dialogue: 0,0:21:05.00,0:21:07.92,Default#3,,0,0,0,,千空 君の言い方を借りれば
Dialogue: 0,0:21:08.05,0:21:12.18,Default#3,,0,0,0,,まず100億％\N金銀の槍が欲しいだけだろうな
Dialogue: 0,0:21:12.84,0:21:16.56,Default#3,,0,0,0,,ほら～\Nこんないっぱい… ガラスだっけ？
Dialogue: 0,0:21:16.68,0:21:18.48,Default#3,,0,0,0,,すごい研究室も出来たし―
Dialogue: 0,0:21:19.02,0:21:22.81,Default#3,,0,0,0,,銀の槍とか\N作れるんじゃないかな～って
Dialogue: 0,0:21:22.94,0:21:25.90,Default#3,,0,0,0,,そしたらマグマなんて\Nちょちょいのちょいって…
Dialogue: 0,0:21:26.02,0:21:28.86,Default#3,,0,0,0,,（千空）クククク… そうだ
Dialogue: 0,0:21:29.19,0:21:34.12,Default#3,,0,0,0,,ガラス容器が出来た今\Nいよいよ万能薬の最難関素材！
Dialogue: 0,0:21:34.70,0:21:37.95,Default#3,,0,0,0,,腹くくって\N採りに行くしかねえわな
Dialogue: 0,0:21:38.79,0:21:39.71,Default#3,,0,0,0,,お～し！
Dialogue: 0,0:21:40.21,0:21:44.42,Default#3,,0,0,0,,ラボで作る記念すべき第１弾は\N銀の槍だ
Dialogue: 0,0:21:44.54,0:21:45.88,Default#3,,0,0,0,,（金狼）おおおおお！\N（銀狼）うわ～ はっ！
Dialogue: 0,0:21:46.00,0:21:49.80,Default#3,,0,0,0,,（銀狼）フフフ…\Nいや～ ごめんね 金狼
Dialogue: 0,0:21:46.00,0:21:49.80,Default#3,,0,0,0,,（金狼）\Nあ… あ… あ…
Dialogue: 0,0:21:49.92,0:21:52.84,Default#3,,0,0,0,,僕って ほら\Nかわいがられるタイプでさあ
Dialogue: 0,0:21:52.97,0:21:55.39,Default#3,,0,0,0,,（千空）\N１ミリも かわいかねえよ バカ
Dialogue: 0,0:21:56.01,0:22:00.52,Default#3,,0,0,0,,代金は文字どおり\N死ぬほど高えぞ 銀狼
Dialogue: 0,0:22:00.93,0:22:01.85,Default#3,,0,0,0,,え？
Dialogue: 0,0:22:01.98,0:22:05.40,Default#3,,0,0,0,,（千空）化学(ばけがく)史上最高のお宝を\Nゲットしに―
Dialogue: 0,0:22:05.52,0:22:08.48,Default#3,,0,0,0,,大冒険に出発だ！
Dialogue: 0,0:22:09.19,0:22:12.99,Default#3,,0,0,0,,マジで即死もある\N危険度マックスエリアで
Dialogue: 0,0:22:13.11,0:22:16.20,Default#3,,0,0,0,,テメーの命を張ってもらう
Dialogue: 0,0:22:18.54,0:22:19.70,Default#3,,0,0,0,,へ!?
Dialogue: 0,0:22:21.54,0:22:26.54,Default#3,,0,0,0,,♪～
Dialogue: 0,0:23:44.87,0:23:49.88,Default#3,,0,0,0,,～♪
