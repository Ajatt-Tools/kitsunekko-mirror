Dialogue: 0,0:00:09.12,0:00:13.42,Default#3,,0,0,0,,（千空(せんくう)）科学本 あさり始めて\N生まれて初めて読んだ伝記が―
Dialogue: 0,0:00:13.55,0:00:15.30,Default#3,,0,0,0,,エジソンだった
Dialogue: 0,0:00:16.72,0:00:19.47,Default#3,,0,0,0,,（幼い千空）つうか\Nそもそも電気って何だよ
Dialogue: 0,0:00:19.80,0:00:20.68,Default#3,,0,0,0,,ほ～ん
Dialogue: 0,0:00:20.80,0:00:24.14,Default#3,,0,0,0,,突き詰めると\N宇宙には４つの力しかねえのか
Dialogue: 0,0:00:24.26,0:00:25.31,Default#3,,0,0,0,,重力と電磁力…\Nこれが電気だな
Dialogue: 0,0:00:25.31,0:00:28.35,Default#3,,0,0,0,,重力と電磁力…\Nこれが電気だな
Dialogue: 0,0:00:25.31,0:00:28.35,Default#3,,0,0,0,,（教師）５＋３は\Nどうなるでしょうか
Dialogue: 0,0:00:28.35,0:00:28.48,Default#3,,0,0,0,,（教師）５＋３は\Nどうなるでしょうか
Dialogue: 0,0:00:28.48,0:00:28.89,Default#3,,0,0,0,,（教師）５＋３は\Nどうなるでしょうか
Dialogue: 0,0:00:28.48,0:00:28.89,Default#3,,0,0,0,,あと 強い力と弱い力？
Dialogue: 0,0:00:28.89,0:00:30.85,Default#3,,0,0,0,,あと 強い力と弱い力？
Dialogue: 0,0:00:30.85,0:00:31.69,Default#3,,0,0,0,,あと 強い力と弱い力？
Dialogue: 0,0:00:30.85,0:00:31.69,Default#3,,0,0,0,,ん？
Dialogue: 0,0:00:32.06,0:00:35.69,Default#3,,0,0,0,,ククク\N意味不明すぎて笑えるわ
Dialogue: 0,0:00:32.06,0:00:35.69,Default#3,,0,0,0,,（生徒たちのざわめき）
Dialogue: 0,0:00:37.86,0:00:39.53,Default#3,,0,0,0,,（幼い千空）\Nつまり 電気っつうのは―
Dialogue: 0,0:00:39.65,0:00:42.99,Default#3,,0,0,0,,この宇宙で いちばん根幹の\Nパワーだってこった
Dialogue: 0,0:00:43.74,0:00:45.91,Default#3,,0,0,0,,唆(そそ)るじゃねえか！
Dialogue: 0,0:01:08.18,0:01:12.65,Default#3,,0,0,0,,（千空）素っ裸で たった１人\N原始の地球に生まれて―
Dialogue: 0,0:01:13.52,0:01:16.98,Default#3,,0,0,0,,１年４か月で\Nようやくたどり着いた
Dialogue: 0,0:01:19.24,0:01:21.49,Default#3,,0,0,0,,科学の根幹に…
Dialogue: 0,0:01:22.82,0:01:24.24,Default#3,,0,0,0,,電気に…
Dialogue: 0,0:01:28.50,0:01:29.58,Default#3,,0,0,0,,（千空）来たぞ
Dialogue: 0,0:01:30.29,0:01:32.58,Default#3,,0,0,0,,まずは ここまでな
Dialogue: 0,0:01:44.43,0:01:45.89,Default#3,,0,0,0,,（クロム）しゃあ！
Dialogue: 0,0:01:47.43,0:01:50.60,Default#3,,0,0,0,,（ゲン）\Nいや～ 石器時代に電気だよ？
Dialogue: 0,0:01:50.73,0:01:51.98,Default#3,,0,0,0,,ジーマーで
Dialogue: 0,0:01:52.39,0:01:53.35,Default#3,,0,0,0,,フッ…
Dialogue: 0,0:01:55.65,0:01:58.15,Default#3,,0,0,0,,ねえ 司(つかさ)ちゃん
Dialogue: 0,0:01:59.28,0:02:04.28,Default#3,,0,0,0,,♪～
Dialogue: 0,0:03:23.90,0:03:28.91,Default#3,,0,0,0,,～♪
Dialogue: 0,0:03:34.58,0:03:36.16,Default#3,,0,0,0,,（コハク）というわけだ スイカ
Dialogue: 0,0:03:36.54,0:03:40.29,Default#3,,0,0,0,,（コハク）その司という男が\N千空殺人事件の犯人だ
Dialogue: 0,0:03:40.54,0:03:42.88,Default#3,,0,0,0,,（千空）ククク 死んでねえがな
Dialogue: 0,0:03:43.09,0:03:45.42,Default#3,,0,0,0,,相当ヤベえぞ そいつはよ\N（スイカ）あっ！
Dialogue: 0,0:03:45.55,0:03:47.59,Default#3,,0,0,0,,科学 発展させねえとか何とか
Dialogue: 0,0:03:47.72,0:03:50.35,Default#3,,0,0,0,,そのためなら平気で\N千空 ブチ殺してんだろ!?
Dialogue: 0,0:03:50.35,0:03:50.72,Default#3,,0,0,0,,そのためなら平気で\N千空 ブチ殺してんだろ!?
Dialogue: 0,0:03:50.35,0:03:50.72,Default#3,,0,0,0,,（スイカ）あわわわ…
Dialogue: 0,0:03:50.72,0:03:50.85,Default#3,,0,0,0,,（スイカ）あわわわ…
Dialogue: 0,0:03:50.85,0:03:52.60,Default#3,,0,0,0,,（スイカ）あわわわ…
Dialogue: 0,0:03:50.85,0:03:52.60,Default#3,,0,0,0,,だから 死んでねえがな
Dialogue: 0,0:03:53.26,0:03:55.93,Default#3,,0,0,0,,（スイカ）つまり\Nその司って人は悪い人で―
Dialogue: 0,0:03:56.06,0:03:57.98,Default#3,,0,0,0,,ゲンは その仲間ってこと？
Dialogue: 0,0:03:58.10,0:04:01.02,Default#3,,0,0,0,,じゃあ ゲンは悪い人なんだよ！
Dialogue: 0,0:04:01.36,0:04:02.61,Default#3,,0,0,0,,あれ？　でも ゲンは―
Dialogue: 0,0:04:02.73,0:04:05.28,Default#3,,0,0,0,,マグマから みんなを\N助けてくれたんだよ？
Dialogue: 0,0:04:05.40,0:04:06.53,Default#3,,0,0,0,,いい人なんだよ！
Dialogue: 0,0:04:06.65,0:04:10.11,Default#3,,0,0,0,,ククク\Nいいヤツでも悪いヤツでもねえよ
Dialogue: 0,0:04:10.24,0:04:12.78,Default#3,,0,0,0,,ひたすらに軽薄なコウモリ男だ
Dialogue: 0,0:04:13.16,0:04:16.04,Default#3,,0,0,0,,あさぎりゲンが\N良かろうが悪かろうが―
Dialogue: 0,0:04:16.16,0:04:19.25,Default#3,,0,0,0,,科学王国の仲間に\Nゲットするしかねえんだよ
Dialogue: 0,0:04:19.87,0:04:25.00,Default#3,,0,0,0,,“司 千空はちゃんと死んでた！”\Nって偽情報を流させる
Dialogue: 0,0:04:25.59,0:04:28.26,Default#3,,0,0,0,,俺らの勝利条件は それしかねえ！
Dialogue: 0,0:04:28.38,0:04:29.38,Default#3,,0,0,0,,（クロム）んっ！
Dialogue: 0,0:04:30.05,0:04:31.51,Default#3,,0,0,0,,おう 聞いたかよ ゲン！
Dialogue: 0,0:04:32.05,0:04:33.85,Default#3,,0,0,0,,テメーも見ただろうが！
Dialogue: 0,0:04:33.97,0:04:37.14,Default#3,,0,0,0,,電気を…\Nあのヤベえ科学の明かりをよ！
Dialogue: 0,0:04:37.56,0:04:39.10,Default#3,,0,0,0,,勝ち負けとか知ったことか
Dialogue: 0,0:04:39.44,0:04:43.11,Default#3,,0,0,0,,司 ＶＳ 千空\Nどっちが有利とかクッソ食らえだ！
Dialogue: 0,0:04:43.69,0:04:45.15,Default#3,,0,0,0,,面白(おもしれ)えんだよ
Dialogue: 0,0:04:45.28,0:04:47.11,Default#3,,0,0,0,,断然 科学王国はよ！
Dialogue: 0,0:04:49.36,0:04:52.53,Default#3,,0,0,0,,（ゲン）いや～ 熱いねえ\N青年の主張
Dialogue: 0,0:04:53.16,0:04:54.28,Default#3,,0,0,0,,でも 残念\N（クロム）あ…
Dialogue: 0,0:04:54.41,0:04:56.91,Default#3,,0,0,0,,（ゲン）俺は そういうのないのよ\Nクロムちゃん
Dialogue: 0,0:04:57.33,0:05:00.29,Default#3,,0,0,0,,世界一ペラッペラな男でね～
Dialogue: 0,0:05:00.42,0:05:02.67,Default#3,,0,0,0,,自分の損得しか見てないの
Dialogue: 0,0:05:05.17,0:05:07.26,Default#3,,0,0,0,,（コハク）うぐぐぐ…
Dialogue: 0,0:05:22.40,0:05:25.02,Default#3,,0,0,0,,（ゲン）あ… あああ…
Dialogue: 0,0:05:25.61,0:05:28.03,Default#3,,0,0,0,,何これ？　ドッキリ～？
Dialogue: 0,0:05:28.15,0:05:30.40,Default#3,,0,0,0,,マネジャーが止めなかった？
Dialogue: 0,0:05:30.53,0:05:32.20,Default#3,,0,0,0,,俺 メンタリストなんだから
Dialogue: 0,0:05:32.32,0:05:35.66,Default#3,,0,0,0,,仕掛け人の仕事しか受けないって\N言ったんだけどね～
Dialogue: 0,0:05:36.16,0:05:39.25,Default#3,,0,0,0,,（司）今日は西暦5739年
Dialogue: 0,0:05:39.37,0:05:40.37,Default#3,,0,0,0,,うっ！
Dialogue: 0,0:05:40.50,0:05:42.75,Default#3,,0,0,0,,（司）でも 君は19歳のままだ
Dialogue: 0,0:05:42.87,0:05:44.71,Default#3,,0,0,0,,（ゲン）ああ…
Dialogue: 0,0:05:44.83,0:05:46.84,Default#3,,0,0,0,,あれ～？　確か…
Dialogue: 0,0:05:47.34,0:05:49.84,Default#3,,0,0,0,,心理マジックの特番だっけ？
Dialogue: 0,0:05:50.17,0:05:52.26,Default#3,,0,0,0,,一緒に出たよね？
Dialogue: 0,0:05:54.05,0:05:58.10,Default#3,,0,0,0,,そうだ！\N霊長類最強の高校生…
Dialogue: 0,0:05:58.89,0:06:01.31,Default#3,,0,0,0,,獅子王(ししおう) 司ちゃん！
Dialogue: 0,0:06:02.60,0:06:04.69,Default#3,,0,0,0,,（司）ここには うん
Dialogue: 0,0:06:04.81,0:06:09.28,Default#3,,0,0,0,,君を含めて 優先的に\N復活させるべき人間たちの
Dialogue: 0,0:06:09.40,0:06:11.65,Default#3,,0,0,0,,石像を集めているんだ
Dialogue: 0,0:06:11.94,0:06:12.78,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:06:19.12,0:06:21.04,Default#3,,0,0,0,,ああ…
Dialogue: 0,0:06:22.71,0:06:25.50,Default#3,,0,0,0,,（司）この美しい石の世界(ストーンワールド)に―
Dialogue: 0,0:06:25.63,0:06:28.46,Default#3,,0,0,0,,旧世界の老いた権力者たちは\N要らない
Dialogue: 0,0:06:29.17,0:06:32.42,Default#3,,0,0,0,,俺たちで新しい世界を\N作ろうじゃないか
Dialogue: 0,0:06:32.55,0:06:33.72,Default#3,,0,0,0,,ゲン
Dialogue: 0,0:06:35.51,0:06:38.30,Default#3,,0,0,0,,君のメンタリストとしての\N腕を見込んで―
Dialogue: 0,0:06:38.43,0:06:40.47,Default#3,,0,0,0,,頼みたい仕事があるんだ
Dialogue: 0,0:06:43.39,0:06:44.98,Default#3,,0,0,0,,追跡だよ
Dialogue: 0,0:06:45.10,0:06:47.19,Default#3,,0,0,0,,心理を読んでほしい
Dialogue: 0,0:06:47.31,0:06:49.11,Default#3,,0,0,0,,あくまで念のためだが―
Dialogue: 0,0:06:49.23,0:06:52.49,Default#3,,0,0,0,,千空という男が\Nもし まだ生きているのなら―
Dialogue: 0,0:06:52.61,0:06:54.03,Default#3,,0,0,0,,捜し出してもらいたい
Dialogue: 0,0:06:54.40,0:06:55.91,Default#3,,0,0,0,,（ゲン）千空？
Dialogue: 0,0:06:56.03,0:06:58.28,Default#3,,0,0,0,,（司）千空は最初の復活者だ
Dialogue: 0,0:06:58.70,0:07:02.95,Default#3,,0,0,0,,ここで石化を解く薬を\N科学の力で作り上げ―
Dialogue: 0,0:07:03.08,0:07:05.33,Default#3,,0,0,0,,俺を復活させた男
Dialogue: 0,0:07:05.46,0:07:06.75,Default#3,,0,0,0,,（ゲン）ああ…
Dialogue: 0,0:07:07.04,0:07:13.13,Default#3,,0,0,0,,（司）そして 全人類…\N汚(けが)れた老人たちまで復活させて
Dialogue: 0,0:07:13.26,0:07:17.14,Default#3,,0,0,0,,科学の武器を生む文明を\N発展させようとしていた
Dialogue: 0,0:07:18.18,0:07:22.39,Default#3,,0,0,0,,俺が この手であやめた\N世界一切れる男だ
Dialogue: 0,0:07:37.07,0:07:38.07,Default#3,,0,0,0,,はっ！
Dialogue: 0,0:07:38.57,0:07:39.78,Default#3,,0,0,0,,がはっ…
Dialogue: 0,0:07:39.99,0:07:41.41,Default#3,,0,0,0,,ん？
Dialogue: 0,0:07:41.53,0:07:43.58,Default#3,,0,0,0,,うっ… ぐあっ
Dialogue: 0,0:07:44.00,0:07:45.37,Default#3,,0,0,0,,がはっ…
Dialogue: 0,0:07:45.79,0:07:47.96,Default#3,,0,0,0,,うっ… ハァハァ…
Dialogue: 0,0:07:48.75,0:07:50.13,Default#3,,0,0,0,,だ… れ…
Dialogue: 0,0:07:53.05,0:07:55.09,Default#3,,0,0,0,,なん… で…
Dialogue: 0,0:08:02.85,0:08:04.31,Default#3,,0,0,0,,（コハク）何の音だ？
Dialogue: 0,0:08:06.02,0:08:06.89,Default#3,,0,0,0,,（コハク）あっ！\N（クロム）はっ！
Dialogue: 0,0:08:08.06,0:08:09.69,Default#3,,0,0,0,,おう 大丈夫かよ！　ゲン！
Dialogue: 0,0:08:09.81,0:08:12.36,Default#3,,0,0,0,,（コハク）\Nダメだ… どう見ても即死だ
Dialogue: 0,0:08:12.48,0:08:13.65,Default#3,,0,0,0,,（スイカ）大変なんだよ！
Dialogue: 0,0:08:14.23,0:08:15.28,Default#3,,0,0,0,,いや 待て！
Dialogue: 0,0:08:19.53,0:08:21.45,Default#3,,0,0,0,,血のり袋のガードだ
Dialogue: 0,0:08:21.91,0:08:23.83,Default#3,,0,0,0,,あっちこっちに入ってるんだよ！
Dialogue: 0,0:08:23.95,0:08:25.29,Default#3,,0,0,0,,ヤベえぜ こいつ
Dialogue: 0,0:08:25.83,0:08:29.25,Default#3,,0,0,0,,万一に備えて\N死ぬほど仕込んでやがったのか
Dialogue: 0,0:08:29.54,0:08:32.00,Default#3,,0,0,0,,ククク やるじゃねえか
Dialogue: 0,0:08:32.46,0:08:37.01,Default#3,,0,0,0,,骨の髄までマジシャンだよ\Nテメーはよ
Dialogue: 0,0:08:38.30,0:08:40.38,Default#3,,0,0,0,,（コハク）仕込みのガードで\N防いだとはいえ―
Dialogue: 0,0:08:40.38,0:08:40.80,Default#3,,0,0,0,,（コハク）仕込みのガードで\N防いだとはいえ―
Dialogue: 0,0:08:40.38,0:08:40.80,Default#3,,0,0,0,,（うめき声）
Dialogue: 0,0:08:40.80,0:08:40.93,Default#3,,0,0,0,,（うめき声）
Dialogue: 0,0:08:40.93,0:08:42.64,Default#3,,0,0,0,,（うめき声）
Dialogue: 0,0:08:40.93,0:08:42.64,Default#3,,0,0,0,,ケガは相当ひどいぞ
Dialogue: 0,0:08:42.85,0:08:45.89,Default#3,,0,0,0,,クッソ！\N絶対 犯人 捕まえんぞ！
Dialogue: 0,0:08:46.02,0:08:48.68,Default#3,,0,0,0,,しかし 一体 誰が\Nこんなむごいことを
Dialogue: 0,0:08:48.89,0:08:50.06,Default#3,,0,0,0,,（スイカ）ああ…
Dialogue: 0,0:08:50.81,0:08:52.56,Default#3,,0,0,0,,ん… うん！
Dialogue: 0,0:08:53.23,0:08:54.07,Default#3,,0,0,0,,（３人）ん？
Dialogue: 0,0:08:54.48,0:08:57.82,Default#3,,0,0,0,,（スイカ）\Nスイカが調べてくるんだよ～！
Dialogue: 0,0:09:16.92,0:09:18.84,Default#3,,0,0,0,,（スイカ）大変だよ～！\N（千空･コハク）ん？
Dialogue: 0,0:09:18.97,0:09:21.13,Default#3,,0,0,0,,（スイカ）\N分かっちゃったんだよ～！
Dialogue: 0,0:09:21.43,0:09:25.01,Default#3,,0,0,0,,おお 大活躍じゃねえか\N名探偵スイカ
Dialogue: 0,0:09:25.68,0:09:29.39,Default#3,,0,0,0,,あさぎりゲン殺人事件の犯人が\N分かっちゃったんだよ！
Dialogue: 0,0:09:29.73,0:09:31.56,Default#3,,0,0,0,,（コハク）いや 死んではないぞ
Dialogue: 0,0:09:31.69,0:09:33.77,Default#3,,0,0,0,,（千空）で やったのは誰だ？
Dialogue: 0,0:09:33.90,0:09:35.40,Default#3,,0,0,0,,（スイカ）それは…
Dialogue: 0,0:09:35.69,0:09:37.65,Default#3,,0,0,0,,（マグマ）ムハハハハ…
Dialogue: 0,0:09:38.28,0:09:40.40,Default#3,,0,0,0,,妖術使いを殺した今―
Dialogue: 0,0:09:40.53,0:09:44.12,Default#3,,0,0,0,,最強は この俺\Nマグマ様で決まりだな
Dialogue: 0,0:09:44.53,0:09:49.33,Default#3,,0,0,0,,妖術など\N赤子の手をひねるのと同じことよ
Dialogue: 0,0:09:49.70,0:09:52.50,Default#3,,0,0,0,,（マントル）\Nあい～ 存じております
Dialogue: 0,0:09:52.62,0:09:55.92,Default#3,,0,0,0,,（マグマ）本当は\Nコハクを直で殺(や)れればよかったがな
Dialogue: 0,0:09:56.34,0:09:58.38,Default#3,,0,0,0,,さすがに隙がなさすぎた
Dialogue: 0,0:09:58.67,0:10:02.17,Default#3,,0,0,0,,コハクの 女だてらに\Nあの異常な強さ
Dialogue: 0,0:10:02.30,0:10:05.05,Default#3,,0,0,0,,間違いなく\N妖術の力でしょうなあ
Dialogue: 0,0:10:05.47,0:10:06.43,Default#3,,0,0,0,,（マグマ）フン
Dialogue: 0,0:10:06.55,0:10:10.02,Default#3,,0,0,0,,だが 仲間の強力な妖術使いを\N消した今―
Dialogue: 0,0:10:10.14,0:10:12.94,Default#3,,0,0,0,,コハクも これで ただの小娘だ
Dialogue: 0,0:10:13.69,0:10:16.69,Default#3,,0,0,0,,ムハハハ 泣いてわびるのなら―
Dialogue: 0,0:10:16.81,0:10:20.32,Default#3,,0,0,0,,ルリと一緒に\N俺の女にしてやらんでもないがな
Dialogue: 0,0:10:20.57,0:10:21.40,Default#3,,0,0,0,,（マントル）あい～
Dialogue: 0,0:10:21.53,0:10:25.95,Default#3,,0,0,0,,姉妹でマグマ様のものになれるとは\Nなんという果報者！
Dialogue: 0,0:10:26.07,0:10:26.66,Default#3,,0,0,0,,（マグマたちの笑い声）
Dialogue: 0,0:10:26.66,0:10:30.08,Default#3,,0,0,0,,（マグマたちの笑い声）
Dialogue: 0,0:10:26.66,0:10:30.08,Default#3,,0,0,0,,（スイカ）大変なこと\N聞いちゃったんだよ～
Dialogue: 0,0:10:30.79,0:10:33.12,Default#3,,0,0,0,,マグマか… あんの野郎！
Dialogue: 0,0:10:33.25,0:10:37.96,Default#3,,0,0,0,,そうか あさぎりゲンは\Nマグマたちの目の前で花束を消した
Dialogue: 0,0:10:38.63,0:10:40.42,Default#3,,0,0,0,,確かに マグマ視点だと―
Dialogue: 0,0:10:40.55,0:10:44.93,Default#3,,0,0,0,,ゲンこそが 私の連れてきた\Nウワサのよそ者妖術使いに見える
Dialogue: 0,0:10:45.34,0:10:49.47,Default#3,,0,0,0,,じゃあ マグマは千空と間違えて\Nゲンをブッ刺したっつうことか
Dialogue: 0,0:10:49.60,0:10:51.56,Default#3,,0,0,0,,ハ！　しかし マグマが
Dialogue: 0,0:10:51.68,0:10:55.44,Default#3,,0,0,0,,私の強さを妖術によるものだと\N勘違いしているとはな
Dialogue: 0,0:10:55.56,0:11:00.02,Default#3,,0,0,0,,おう コハクは素で\Nゴリラ並みの強さの素ゴリラだぞ
Dialogue: 0,0:11:00.15,0:11:01.82,Default#3,,0,0,0,,（コハク）カハ～ッ
Dialogue: 0,0:11:01.94,0:11:03.07,Default#3,,0,0,0,,（クロム）サーセン
Dialogue: 0,0:11:03.61,0:11:05.53,Default#3,,0,0,0,,で なんでマグマのヤツは―
Dialogue: 0,0:11:05.66,0:11:08.57,Default#3,,0,0,0,,あわよくば コハクをブチ殺そうと\Nたくらんでる？
Dialogue: 0,0:11:10.24,0:11:13.20,Default#3,,0,0,0,,理由は恐らく\Nいや 間違いなく…
Dialogue: 0,0:11:13.29,0:11:13.95,Default#3,,0,0,0,,（村人の歓声）
Dialogue: 0,0:11:13.95,0:11:15.67,Default#3,,0,0,0,,（村人の歓声）
Dialogue: 0,0:11:13.95,0:11:15.67,Default#3,,0,0,0,,（コハク）“御前試合”だ
Dialogue: 0,0:11:16.46,0:11:19.34,Default#3,,0,0,0,,村では代々 一世代に一度だけ―
Dialogue: 0,0:11:19.46,0:11:23.01,Default#3,,0,0,0,,大きな武術大会\N“御前試合”が行われる
Dialogue: 0,0:11:23.13,0:11:27.05,Default#3,,0,0,0,,（千空）あの奥の島にある\N武舞台みてえなのは それか
Dialogue: 0,0:11:27.39,0:11:29.85,Default#3,,0,0,0,,（コハク）\Nさかのぼること数か月前
Dialogue: 0,0:11:29.97,0:11:34.31,Default#3,,0,0,0,,村の巫女(みこ) つまりルリ姉が\N成人となる誕生日
Dialogue: 0,0:11:34.43,0:11:35.23,Default#3,,0,0,0,,（ハガネ）くっ！
Dialogue: 0,0:11:35.35,0:11:36.89,Default#3,,0,0,0,,（マグマ）ふん！\N（ハガネ）がっ…
Dialogue: 0,0:11:37.02,0:11:37.60,Default#3,,0,0,0,,（マグマたちの戦う声）
Dialogue: 0,0:11:37.60,0:11:41.69,Default#3,,0,0,0,,（マグマたちの戦う声）
Dialogue: 0,0:11:37.60,0:11:41.69,Default#3,,0,0,0,,（コハク）御前試合の優勝者が\N巫女の夫となり 長(おさ)を継ぐ
Dialogue: 0,0:11:42.27,0:11:45.65,Default#3,,0,0,0,,今の長である私の父も\Nそうしてきたのだ
Dialogue: 0,0:11:45.78,0:11:47.53,Default#3,,0,0,0,,（ハガネ）ハァ ハァ…
Dialogue: 0,0:11:47.99,0:11:49.53,Default#3,,0,0,0,,（マグマ）フッ… やあっ！
Dialogue: 0,0:11:49.91,0:11:51.28,Default#3,,0,0,0,,（ハガネ）ぐおっ…
Dialogue: 0,0:11:51.78,0:11:53.12,Default#3,,0,0,0,,（ジャスパー）勝者 マグマ！
Dialogue: 0,0:11:53.41,0:11:54.75,Default#3,,0,0,0,,（アラバスタ）決勝進出～
Dialogue: 0,0:11:54.87,0:11:57.92,Default#3,,0,0,0,,（カーボ）すげえ！\Nやっぱ マグマぶっちぎりだぜ！
Dialogue: 0,0:11:58.04,0:11:58.87,Default#3,,0,0,0,,あい～
Dialogue: 0,0:11:59.33,0:12:00.79,Default#3,,0,0,0,,（ルリのせき込み）\N（クロム）あっ！
Dialogue: 0,0:12:00.92,0:12:03.21,Default#3,,0,0,0,,（コクヨウ）ルリ！\N（ルリのせき込み）
Dialogue: 0,0:12:03.63,0:12:04.88,Default#3,,0,0,0,,おい 大丈夫かよ ルリ！
Dialogue: 0,0:12:05.01,0:12:05.97,Default#3,,0,0,0,,（ターコイズ）待ちな！
Dialogue: 0,0:12:06.42,0:12:08.88,Default#3,,0,0,0,,（ターコイズ）軽々しく\N巫女様に近づくんじゃないよ
Dialogue: 0,0:12:09.01,0:12:09.84,Default#3,,0,0,0,,（クロム）くっ…
Dialogue: 0,0:12:10.09,0:12:11.68,Default#3,,0,0,0,,（コクヨウ）ああ…
Dialogue: 0,0:12:15.27,0:12:16.10,Default#3,,0,0,0,,（コハク）あ…
Dialogue: 0,0:12:16.73,0:12:19.98,Default#3,,0,0,0,,ルリか…\Nまあ もって１年だな
Dialogue: 0,0:12:20.69,0:12:22.65,Default#3,,0,0,0,,看病はごめんだぜ
Dialogue: 0,0:12:22.77,0:12:27.11,Default#3,,0,0,0,,早めにくたばってくれりゃ\N長の座だけ頂けるんだが…
Dialogue: 0,0:12:27.61,0:12:29.32,Default#3,,0,0,0,,むしろ いっそのこと
Dialogue: 0,0:12:29.45,0:12:33.16,Default#3,,0,0,0,,病をはかなんで\N身を投げたってことにして…
Dialogue: 0,0:12:33.41,0:12:34.87,Default#3,,0,0,0,,（コハク）うう…
Dialogue: 0,0:12:36.00,0:12:37.41,Default#3,,0,0,0,,ぬあああああ！
Dialogue: 0,0:12:37.66,0:12:40.83,Default#3,,0,0,0,,（マグマ）ううっ… ま… 参った
Dialogue: 0,0:12:40.96,0:12:43.63,Default#3,,0,0,0,,（ジャスパー）\N御前試合優勝者はコハク！
Dialogue: 0,0:12:43.96,0:12:47.05,Default#3,,0,0,0,,マグマ 貴様のような男にだけは
Dialogue: 0,0:12:47.17,0:12:49.84,Default#3,,0,0,0,,ルリ姉を渡すわけには\Nいかないのでな
Dialogue: 0,0:12:50.18,0:12:51.64,Default#3,,0,0,0,,（マグマ）コハク こいつ―
Dialogue: 0,0:12:51.76,0:12:54.76,Default#3,,0,0,0,,最初から そのためだけに\N御前試合に！
Dialogue: 0,0:12:55.14,0:12:57.48,Default#3,,0,0,0,,（スイカ）\Nあれ？　どうなるんだよ これ
Dialogue: 0,0:12:55.14,0:12:57.48,Default#3,,0,0,0,,（村人たちのざわめき）
Dialogue: 0,0:12:57.48,0:12:57.60,Default#3,,0,0,0,,（村人たちのざわめき）
Dialogue: 0,0:12:57.60,0:13:00.14,Default#3,,0,0,0,,（村人たちのざわめき）
Dialogue: 0,0:12:57.60,0:13:00.14,Default#3,,0,0,0,,優勝した選手が\Nルリ姉と結婚する決まりだから…
Dialogue: 0,0:13:00.14,0:13:01.15,Default#3,,0,0,0,,優勝した選手が\Nルリ姉と結婚する決まりだから…
Dialogue: 0,0:13:01.27,0:13:02.31,Default#3,,0,0,0,,（銀狼(ギンロー)）のわっ！
Dialogue: 0,0:13:02.44,0:13:07.07,Default#3,,0,0,0,,ルリちゃんとコハクちゃんが…\N姉妹で結婚ってこと？
Dialogue: 0,0:13:07.19,0:13:10.03,Default#3,,0,0,0,,き… 禁断の愛だね！
Dialogue: 0,0:13:10.15,0:13:11.78,Default#3,,0,0,0,,あたっ！　あ… あ…\N（ターコイズ）なわけないでしょ！
Dialogue: 0,0:13:11.91,0:13:14.12,Default#3,,0,0,0,,あ～あ どうすんのよ これ
Dialogue: 0,0:13:14.24,0:13:17.12,Default#3,,0,0,0,,（コクヨウ）ぬおおおお…\N（ターコイズ）ルリ様の結婚相手…
Dialogue: 0,0:13:17.24,0:13:18.41,Default#3,,0,0,0,,（千空）ククク
Dialogue: 0,0:13:18.54,0:13:22.83,Default#3,,0,0,0,,それでコハク テメー\N長のおやじに勘当されてんのか
Dialogue: 0,0:13:23.21,0:13:26.88,Default#3,,0,0,0,,いや… まあ それが\N最後の引き金ではあったがな
Dialogue: 0,0:13:27.00,0:13:29.09,Default#3,,0,0,0,,いろいろ\Nヤンチャやらかしてんだよ
Dialogue: 0,0:13:29.22,0:13:30.47,Default#3,,0,0,0,,コハクは昔っから
Dialogue: 0,0:13:31.22,0:13:33.76,Default#3,,0,0,0,,（コハク）\Nむろん 御前試合は仕切り直し
Dialogue: 0,0:13:33.89,0:13:35.76,Default#3,,0,0,0,,ちょうど来月 行われる
Dialogue: 0,0:13:35.89,0:13:37.10,Default#3,,0,0,0,,（クロム）マグマのヤツ―
Dialogue: 0,0:13:37.22,0:13:40.31,Default#3,,0,0,0,,御前試合で優勝するために\Nゲンを殺したってことかよ
Dialogue: 0,0:13:40.43,0:13:43.35,Default#3,,0,0,0,,（スイカ）え～！\Nやっぱりゲンは死んじゃったんだよ
Dialogue: 0,0:13:43.48,0:13:45.73,Default#3,,0,0,0,,（コハク）いや 死んではないぞ
Dialogue: 0,0:13:46.19,0:13:49.65,Default#3,,0,0,0,,でもよ んなヤツが長とか\Nヤバすぎんじゃねえか？
Dialogue: 0,0:13:49.78,0:13:50.74,Default#3,,0,0,0,,（千空）ああ
Dialogue: 0,0:13:51.07,0:13:53.61,Default#3,,0,0,0,,マグマが長にでもなったらよ
Dialogue: 0,0:13:53.74,0:13:57.99,Default#3,,0,0,0,,万能薬 作っても\Nルリに飲ませんのすら厳しくなるな
Dialogue: 0,0:13:58.29,0:14:00.83,Default#3,,0,0,0,,ハ！　逆にチャンスでもある
Dialogue: 0,0:14:00.96,0:14:04.25,Default#3,,0,0,0,,科学王国民の男が\Nマグマを破って優勝
Dialogue: 0,0:14:04.38,0:14:07.67,Default#3,,0,0,0,,ルリ姉と結婚して長になれば\N全ては解決だ
Dialogue: 0,0:14:08.34,0:14:10.13,Default#3,,0,0,0,,科学王国のまとめ上げも―
Dialogue: 0,0:14:10.59,0:14:13.63,Default#3,,0,0,0,,ルリ姉に万能薬を飲ませるのも\N容易だ
Dialogue: 0,0:14:16.97,0:14:20.18,Default#3,,0,0,0,,金狼(キンロー) 銀狼\N君たちに聞きたいことがある
Dialogue: 0,0:14:21.18,0:14:23.52,Default#3,,0,0,0,,ルリ姉のことをどう思っている？
Dialogue: 0,0:14:23.64,0:14:24.94,Default#3,,0,0,0,,（銀狼が唾を飲み込む音）
Dialogue: 0,0:14:25.35,0:14:27.36,Default#3,,0,0,0,,妻として めとりたいと思うか？
Dialogue: 0,0:14:27.48,0:14:28.44,Default#3,,0,0,0,,（銀狼･金狼）はあ？
Dialogue: 0,0:14:28.77,0:14:31.44,Default#3,,0,0,0,,いや 考えたこともなかったけど
Dialogue: 0,0:14:31.57,0:14:34.86,Default#3,,0,0,0,,あ… そりゃ\Nルリちゃ… ルリ様はかわいいし
Dialogue: 0,0:14:34.99,0:14:38.95,Default#3,,0,0,0,,あっ いや 顔はコハクちゃんと\Nほぼ一緒なんだけども
Dialogue: 0,0:14:39.08,0:14:41.50,Default#3,,0,0,0,,おっぱ…\Nおしとやかでかわいいし―
Dialogue: 0,0:14:41.62,0:14:42.71,Default#3,,0,0,0,,目もクリッとして…
Dialogue: 0,0:14:42.96,0:14:44.62,Default#3,,0,0,0,,（銀狼）\Nヘヘヘヘ… アハッ
Dialogue: 0,0:14:42.96,0:14:44.62,Default#3,,0,0,0,,（金狼）顔のことのみじゃないか！
Dialogue: 0,0:14:44.62,0:14:44.75,Default#3,,0,0,0,,（金狼）顔のことのみじゃないか！
Dialogue: 0,0:14:44.75,0:14:45.00,Default#3,,0,0,0,,（金狼）顔のことのみじゃないか！
Dialogue: 0,0:14:44.75,0:14:45.00,Default#3,,0,0,0,,オホ オホ オホホ…
Dialogue: 0,0:14:45.00,0:14:45.12,Default#3,,0,0,0,,オホ オホ オホホ…
Dialogue: 0,0:14:45.12,0:14:47.50,Default#3,,0,0,0,,オホ オホ オホホ…
Dialogue: 0,0:14:45.12,0:14:47.50,Default#3,,0,0,0,,女子の外見しか見てないのか！
Dialogue: 0,0:14:47.63,0:14:49.84,Default#3,,0,0,0,,最低すぎるぞ 銀狼 貴様！
Dialogue: 0,0:14:47.63,0:14:49.84,Default#3,,0,0,0,,オホホホホ…
Dialogue: 0,0:14:49.96,0:14:51.59,Default#3,,0,0,0,,な… 何だよ
Dialogue: 0,0:14:51.71,0:14:53.09,Default#3,,0,0,0,,おっぱ… おっぱ…
Dialogue: 0,0:14:53.22,0:14:55.09,Default#3,,0,0,0,,おしとやかってのも\N言ったじゃんよ！
Dialogue: 0,0:14:55.55,0:14:57.72,Default#3,,0,0,0,,（金狼）俺の答えはシンプルだ
Dialogue: 0,0:14:57.85,0:14:59.56,Default#3,,0,0,0,,門番は問答しない
Dialogue: 0,0:14:59.68,0:15:00.85,Default#3,,0,0,0,,ルールはルールだ
Dialogue: 0,0:15:01.97,0:15:04.60,Default#3,,0,0,0,,だが\N勝手なつぶやきを聞くだけなら―
Dialogue: 0,0:15:04.73,0:15:06.35,Default#3,,0,0,0,,ルール違反ではない
Dialogue: 0,0:15:07.19,0:15:11.48,Default#3,,0,0,0,,もう分かっている\N千空が悪ではないことは
Dialogue: 0,0:15:11.73,0:15:12.78,Default#3,,0,0,0,,（クロム）フッ…
Dialogue: 0,0:15:13.61,0:15:16.78,Default#3,,0,0,0,,（金狼）いつものように\N説明は省いてかまわん
Dialogue: 0,0:15:16.91,0:15:17.87,Default#3,,0,0,0,,目的は何だ？
Dialogue: 0,0:15:23.66,0:15:28.13,Default#3,,0,0,0,,私は姉者(あねじゃ)の… 命を救いたいのだ
Dialogue: 0,0:15:35.80,0:15:36.93,Default#3,,0,0,0,,（金狼）分かった
Dialogue: 0,0:15:37.22,0:15:40.39,Default#3,,0,0,0,,（銀狼）\Nそれ以上の説明は要らないよ
Dialogue: 0,0:15:40.85,0:15:41.72,Default#3,,0,0,0,,浅い！
Dialogue: 0,0:15:41.85,0:15:43.93,Default#3,,0,0,0,,そんなことで\Nマグマを倒せると思うか！
Dialogue: 0,0:15:44.18,0:15:45.39,Default#3,,0,0,0,,（金狼）くっ…\N（銀狼）わあああ…
Dialogue: 0,0:15:45.52,0:15:47.85,Default#3,,0,0,0,,（銀狼）少し休憩しようよ！
Dialogue: 0,0:15:47.98,0:15:50.40,Default#3,,0,0,0,,（金狼）ダメだ！\Nあと３セットこなしてからだ！
Dialogue: 0,0:15:50.90,0:15:52.19,Default#3,,0,0,0,,待て！
Dialogue: 0,0:15:52.32,0:15:53.40,Default#3,,0,0,0,,ハァ…
Dialogue: 0,0:15:54.11,0:15:55.07,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:15:58.82,0:16:00.07,Default#3,,0,0,0,,クロム\N（クロム）あっ
Dialogue: 0,0:16:00.74,0:16:03.33,Default#3,,0,0,0,,（コハク）本当は\N君にこそ稽古をつけたいのだ
Dialogue: 0,0:16:03.91,0:16:06.66,Default#3,,0,0,0,,だが マグマを倒せる\N可能性があるのは―
Dialogue: 0,0:16:06.79,0:16:08.83,Default#3,,0,0,0,,金狼 銀狼だけでな
Dialogue: 0,0:16:09.83,0:16:11.04,Default#3,,0,0,0,,すまない
Dialogue: 0,0:16:11.75,0:16:14.76,Default#3,,0,0,0,,クロム\Nテメーがルリにホレてっから
Dialogue: 0,0:16:14.88,0:16:17.30,Default#3,,0,0,0,,金狼か銀狼が\Nルリと結婚しちまう作戦を
Dialogue: 0,0:16:17.42,0:16:19.80,Default#3,,0,0,0,,メンゴメンゴっつってんだよ！
Dialogue: 0,0:16:19.93,0:16:23.22,Default#3,,0,0,0,,ズバズバいくな…\Nいつもながら君は
Dialogue: 0,0:16:27.35,0:16:29.35,Default#3,,0,0,0,,（クロム）すっげえな ここ！
Dialogue: 0,0:16:29.65,0:16:32.11,Default#3,,0,0,0,,（ルリのせき込み）\N（クロム）おい ルリ！
Dialogue: 0,0:16:33.57,0:16:34.69,Default#3,,0,0,0,,（ルリ）大丈夫
Dialogue: 0,0:16:35.15,0:16:37.44,Default#3,,0,0,0,,（クロム）\Nまた いつものやつが出たのか？
Dialogue: 0,0:16:38.65,0:16:42.20,Default#3,,0,0,0,,（ルリ）クロム\N私の病気は治らないんだって
Dialogue: 0,0:16:42.62,0:16:43.45,Default#3,,0,0,0,,あ…
Dialogue: 0,0:16:43.58,0:16:46.50,Default#3,,0,0,0,,（ルリ）大人になるまでは\N生きられないって
Dialogue: 0,0:16:46.95,0:16:48.58,Default#3,,0,0,0,,俺に任せろよ ルリ！
Dialogue: 0,0:16:48.71,0:16:50.25,Default#3,,0,0,0,,何でも探しまくってよ―
Dialogue: 0,0:16:50.75,0:16:54.96,Default#3,,0,0,0,,俺がいつか\N絶対 治してやっからよ！
Dialogue: 0,0:16:59.22,0:17:02.18,Default#3,,0,0,0,,おう まるで問題ねえ
Dialogue: 0,0:17:04.76,0:17:08.81,Default#3,,0,0,0,,科学の万能薬でルリが治って\N幸せんなってくれりゃ―
Dialogue: 0,0:17:08.93,0:17:11.02,Default#3,,0,0,0,,それ以上の願いはねえよ
Dialogue: 0,0:17:11.73,0:17:15.44,Default#3,,0,0,0,,なんせ俺は\N天才“科学使い”だかんな
Dialogue: 0,0:17:16.28,0:17:20.65,Default#3,,0,0,0,,クク… 妖術使いから科学使いに\Nジョブチェンジか
Dialogue: 0,0:17:20.99,0:17:23.37,Default#3,,0,0,0,,（クロム）\Nああ もう妖術は名乗らねえ
Dialogue: 0,0:17:23.78,0:17:26.41,Default#3,,0,0,0,,（千空）御前試合は\Nバトルチームに任して―
Dialogue: 0,0:17:26.54,0:17:30.12,Default#3,,0,0,0,,俺ら科学チームは\Nサルファ剤作りに専念すんぞ
Dialogue: 0,0:17:30.25,0:17:31.54,Default#3,,0,0,0,,（クロム）おうよ！
Dialogue: 0,0:17:38.59,0:17:41.05,Default#3,,0,0,0,,御前試合には私も出るつもりだ
Dialogue: 0,0:17:41.59,0:17:44.72,Default#3,,0,0,0,,（コハク）金狼 銀狼 私の\N科学王国チームで―
Dialogue: 0,0:17:44.85,0:17:47.18,Default#3,,0,0,0,,何としても\Nマグマの優勝は阻止するぞ
Dialogue: 0,0:17:47.31,0:17:48.14,Default#3,,0,0,0,,（クロム）おう
Dialogue: 0,0:17:48.27,0:17:50.48,Default#3,,0,0,0,,（うめき声）
Dialogue: 0,0:17:50.77,0:17:52.60,Default#3,,0,0,0,,おい ゲン 大丈夫か？
Dialogue: 0,0:17:52.73,0:17:56.27,Default#3,,0,0,0,,この状態では\N回復まで当分かかりそうだな
Dialogue: 0,0:17:56.40,0:17:57.48,Default#3,,0,0,0,,つうことは…
Dialogue: 0,0:17:57.61,0:17:58.53,Default#3,,0,0,0,,（千空）ああ
Dialogue: 0,0:17:59.03,0:18:02.07,Default#3,,0,0,0,,司が俺を殺しに来る\Nタイムリミットまで―
Dialogue: 0,0:18:02.20,0:18:03.53,Default#3,,0,0,0,,そう長くねえ
Dialogue: 0,0:18:03.78,0:18:05.53,Default#3,,0,0,0,,（コハク･クロム）ああ…
Dialogue: 0,0:18:05.87,0:18:07.53,Default#3,,0,0,0,,ハァ ハァ…
Dialogue: 0,0:18:10.37,0:18:13.37,Default#3,,0,0,0,,（クロムのいびき）
Dialogue: 0,0:18:14.42,0:18:15.25,Default#3,,0,0,0,,あ？
Dialogue: 0,0:18:37.48,0:18:39.40,Default#3,,0,0,0,,（いびき）
Dialogue: 0,0:18:39.53,0:18:41.90,Default#3,,0,0,0,,（銀狼）まだ眠いよ～\N（クロム）あっ
Dialogue: 0,0:18:42.15,0:18:43.99,Default#3,,0,0,0,,目を覚ませ 銀狼\N（銀狼）フワ～
Dialogue: 0,0:18:44.11,0:18:47.32,Default#3,,0,0,0,,警備の交代時間まで\N朝練すると決めたばかりだろう！
Dialogue: 0,0:18:47.45,0:18:49.24,Default#3,,0,0,0,,（クロム）ヤベえ～！\N（銀狼）ひっ！
Dialogue: 0,0:18:49.58,0:18:51.50,Default#3,,0,0,0,,ゲンのヤツが消えやがった！
Dialogue: 0,0:18:51.62,0:18:56.88,Default#3,,0,0,0,,（ゲン）ハァハァ ハァハァ…
Dialogue: 0,0:18:57.25,0:18:59.09,Default#3,,0,0,0,,あ～ 戻ったんだろ
Dialogue: 0,0:18:59.21,0:19:00.88,Default#3,,0,0,0,,司んとこに\N（コハクたち）はっ！
Dialogue: 0,0:19:01.42,0:19:03.59,Default#3,,0,0,0,,でもよ なんで こんなこっそり…
Dialogue: 0,0:19:03.72,0:19:05.63,Default#3,,0,0,0,,もしや あのコウモリ男
Dialogue: 0,0:19:05.76,0:19:08.10,Default#3,,0,0,0,,千空が生きていると\N司に教えるつもりではないのか？
Dialogue: 0,0:19:08.10,0:19:08.93,Default#3,,0,0,0,,千空が生きていると\N司に教えるつもりではないのか？
Dialogue: 0,0:19:08.10,0:19:08.93,Default#3,,0,0,0,,（クロム･スイカ）\Nあっ！
Dialogue: 0,0:19:09.30,0:19:11.81,Default#3,,0,0,0,,ヤベえぞ 千空\Nソッコー追わねえと
Dialogue: 0,0:19:11.93,0:19:15.94,Default#3,,0,0,0,,（ゲンの荒い息）
Dialogue: 0,0:19:17.94,0:19:19.06,Default#3,,0,0,0,,うあっ！
Dialogue: 0,0:19:20.94,0:19:22.11,Default#3,,0,0,0,,うっ…
Dialogue: 0,0:19:22.48,0:19:27.03,Default#3,,0,0,0,,（ゲンの荒い息）
Dialogue: 0,0:19:27.16,0:19:30.28,Default#3,,0,0,0,,ハァハァ ハァハァ…
Dialogue: 0,0:19:31.41,0:19:32.24,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:19:32.54,0:19:35.04,Default#3,,0,0,0,,待って 司ちゃん！　俺だよ！
Dialogue: 0,0:19:35.16,0:19:36.83,Default#3,,0,0,0,,ハァハァ…
Dialogue: 0,0:19:37.67,0:19:41.42,Default#3,,0,0,0,,司ちゃん…\N原始的な村があったよ
Dialogue: 0,0:19:41.92,0:19:43.51,Default#3,,0,0,0,,その連中ともめて―
Dialogue: 0,0:19:43.63,0:19:46.13,Default#3,,0,0,0,,ちょ～っと\Nケガしちゃったんだけどね
Dialogue: 0,0:19:46.26,0:19:48.64,Default#3,,0,0,0,,ハァ… ハァ…
Dialogue: 0,0:19:50.30,0:19:54.39,Default#3,,0,0,0,,で そこに千空ちゃんは…
Dialogue: 0,0:19:54.93,0:19:57.35,Default#3,,0,0,0,,急いで\Nコウモリ男を捕まえなければ！
Dialogue: 0,0:19:57.94,0:19:58.77,Default#3,,0,0,0,,（千空）待て！\N（コハク）あっ！
Dialogue: 0,0:19:58.90,0:20:00.48,Default#3,,0,0,0,,（倒れる音）\N（一同）ああ…
Dialogue: 0,0:20:00.61,0:20:02.94,Default#3,,0,0,0,,（コハク）ハァ… ハァハァ…
Dialogue: 0,0:20:03.32,0:20:04.86,Default#3,,0,0,0,,なぜ止める 千空！
Dialogue: 0,0:20:04.99,0:20:06.49,Default#3,,0,0,0,,急いで\Nコウモリ男を捕まえなくては…
Dialogue: 0,0:20:06.61,0:20:07.70,Default#3,,0,0,0,,（千空）ククク…\N（コハク）ん？
Dialogue: 0,0:20:08.41,0:20:11.07,Default#3,,0,0,0,,ヤツは ほっといて問題ねえ\N（コハクたち）え？
Dialogue: 0,0:20:11.87,0:20:13.08,Default#3,,0,0,0,,（千空）そもそも ゲンが―
Dialogue: 0,0:20:13.37,0:20:16.54,Default#3,,0,0,0,,科学に１ミリも\N興味 持たねえ男なら―
Dialogue: 0,0:20:16.66,0:20:19.46,Default#3,,0,0,0,,最初から協力すらしねえだろ
Dialogue: 0,0:20:20.04,0:20:24.00,Default#3,,0,0,0,,俺が電気作った時点で\Nもうゲンのヤツは決めてたんだ
Dialogue: 0,0:20:24.71,0:20:26.34,Default#3,,0,0,0,,どっちにつくか
Dialogue: 0,0:20:27.09,0:20:33.93,Default#3,,0,0,0,,ただ 軽薄男にも軽薄男なりの\Nかっこのつけ方っつうのがあんだよ
Dialogue: 0,0:20:34.47,0:20:37.68,Default#3,,0,0,0,,面倒だな まったく男は
Dialogue: 0,0:20:37.94,0:20:39.69,Default#3,,0,0,0,,いつの時代も
Dialogue: 0,0:20:40.85,0:20:44.69,Default#3,,0,0,0,,（ゲン）う～ん\Nこれ 飲み物も欲しくなっちゃうね
Dialogue: 0,0:20:45.40,0:20:46.69,Default#3,,0,0,0,,コーラとか
Dialogue: 0,0:20:47.95,0:20:53.20,Default#3,,0,0,0,,（ゲン）このストーンワールドで\N作れる？　千空ちゃん
Dialogue: 0,0:20:54.62,0:20:57.66,Default#3,,0,0,0,,コーラを… １本
Dialogue: 0,0:21:01.21,0:21:03.04,Default#3,,0,0,0,,ああ 作れる
Dialogue: 0,0:21:03.42,0:21:04.54,Default#3,,0,0,0,,俺ならな
Dialogue: 0,0:21:04.84,0:21:05.88,Default#3,,0,0,0,,ヘッ…
Dialogue: 0,0:21:12.76,0:21:14.22,Default#3,,0,0,0,,（ゲン）千空ちゃんは―
Dialogue: 0,0:21:16.27,0:21:18.48,Default#3,,0,0,0,,影も形もいなかった
Dialogue: 0,0:21:18.60,0:21:19.48,Default#3,,0,0,0,,（指を鳴らす音）
Dialogue: 0,0:21:19.77,0:21:22.77,Default#3,,0,0,0,,死んでるよ～ 間違いなくね
Dialogue: 0,0:21:23.11,0:21:26.23,Default#3,,0,0,0,,おお\Nじゃあ ゲンは科学王国に…
Dialogue: 0,0:21:26.48,0:21:27.32,Default#3,,0,0,0,,ああ
Dialogue: 0,0:21:27.44,0:21:30.07,Default#3,,0,0,0,,そのうち しれっと\N戻ってくんじゃねえか
Dialogue: 0,0:21:30.20,0:21:31.15,Default#3,,0,0,0,,ハ！
Dialogue: 0,0:21:31.28,0:21:36.45,Default#3,,0,0,0,,司帝国 ＶＳ 科学王国 初戦\Nあさぎりゲン ゲット戦争は―
Dialogue: 0,0:21:36.87,0:21:38.70,Default#3,,0,0,0,,科学王国の勝利だな！
Dialogue: 0,0:21:38.83,0:21:41.21,Default#3,,0,0,0,,（スイカ）ゲンは\Nやっぱりいい人だったんだよ！
Dialogue: 0,0:21:41.33,0:21:43.13,Default#3,,0,0,0,,（クロム）しゃあ！
Dialogue: 0,0:21:43.96,0:21:49.42,Default#3,,0,0,0,,（千空）コーラ１本で結んだ\N世界一薄っぺらの同盟だ
Dialogue: 0,0:21:50.88,0:21:55.97,Default#3,,0,0,0,,（ゲン）あ～あ\N割に合わないね～ コーラ１本じゃ
Dialogue: 0,0:21:57.68,0:22:01.73,Default#3,,0,0,0,,せめて\Nたっぷり冷えててくれないとね
Dialogue: 0,0:22:01.98,0:22:04.19,Default#3,,0,0,0,,ククク しゃあねえ
Dialogue: 0,0:22:04.31,0:22:07.86,Default#3,,0,0,0,,ゲンが戻ってくるまでに\Nアホほどキンキンのやつ
Dialogue: 0,0:22:07.98,0:22:10.86,Default#3,,0,0,0,,科学 駆使して用意しといてやっか
Dialogue: 0,0:22:21.41,0:22:26.42,Default#3,,0,0,0,,♪～
Dialogue: 0,0:23:44.70,0:23:49.71,Default#3,,0,0,0,,～♪
