Dialogue: 0,0:00:11.88,0:00:17.42,Default#3,,0,0,0,,（千空(せんくう)）ギアと水車が出来りゃ\N人力の時代は終了だ
Dialogue: 0,0:00:20.55,0:00:23.72,Default#3,,0,0,0,,燃料要らずの無限装置で―
Dialogue: 0,0:00:26.31,0:00:30.15,Default#3,,0,0,0,,ついに自然様の力を頂く！
Dialogue: 0,0:00:30.60,0:00:31.98,Default#3,,0,0,0,,（一同）うおおお！
Dialogue: 0,0:00:32.11,0:00:33.52,Default#3,,0,0,0,,（一同）あああ！
Dialogue: 0,0:00:34.69,0:00:40.45,Default#3,,0,0,0,,（千空）マンパワーをぶっちぎる\N動力の時代だ！
Dialogue: 0,0:00:45.54,0:00:47.62,Default#3,,0,0,0,,（コハク）こ これは一体…
Dialogue: 0,0:00:47.75,0:00:49.16,Default#3,,0,0,0,,（千空）水力発電機だ！
Dialogue: 0,0:00:49.66,0:00:50.87,Default#3,,0,0,0,,（金狼(キンロー)）すいりょく？
Dialogue: 0,0:00:51.00,0:00:52.17,Default#3,,0,0,0,,（銀狼(ギンロー)）はつでん？
Dialogue: 0,0:00:52.29,0:00:55.17,Default#3,,0,0,0,,（ゲン）ゴイスーすぎでしょ これ\N千空ちゃん
Dialogue: 0,0:00:55.50,0:01:00.43,Default#3,,0,0,0,,ククク お褒めの言葉なら\Nこのクッソゴツい水車を作った
Dialogue: 0,0:01:00.55,0:01:02.76,Default#3,,0,0,0,,クロム＆カセキのジジイに\Nくれてやれ
Dialogue: 0,0:01:02.89,0:01:03.89,Default#3,,0,0,0,,（カセキ･クロム）うう～！
Dialogue: 0,0:01:04.35,0:01:06.56,Default#3,,0,0,0,,（千空）\N…つっても完成じゃねえぞ
Dialogue: 0,0:01:06.68,0:01:10.10,Default#3,,0,0,0,,まだ肝心の電気が\Nダラダラ垂れ流しっぱじゃねえか
Dialogue: 0,0:01:10.39,0:01:13.02,Default#3,,0,0,0,,（コハク）ん？\Nつまり どういうことだ？
Dialogue: 0,0:01:13.15,0:01:15.48,Default#3,,0,0,0,,（カセキとクロムの荒い息）\N（千空）電気をため込むハコを作る
Dialogue: 0,0:01:15.61,0:01:18.57,Default#3,,0,0,0,,なっ!?　あのグルグルパワーって\Nためられるの？
Dialogue: 0,0:01:18.99,0:01:22.11,Default#3,,0,0,0,,まあ ち～っと作んの面倒だがな
Dialogue: 0,0:01:22.57,0:01:26.45,Default#3,,0,0,0,,ククク\Nぶっ倒れてる場合じゃねえぞ！
Dialogue: 0,0:01:27.04,0:01:29.87,Default#3,,0,0,0,,こっから携帯\N完成までの道のりは―
Dialogue: 0,0:01:30.00,0:01:31.96,Default#3,,0,0,0,,難易度超絶アップの\N工作が―
Dialogue: 0,0:01:32.08,0:01:33.42,Default#3,,0,0,0,,アホほどあるからな
Dialogue: 0,0:01:33.54,0:01:35.42,Default#3,,0,0,0,,（一同）あああ…
Dialogue: 0,0:01:35.54,0:01:37.30,Default#3,,0,0,0,,（カセキ）ううあ…\N（クロム）ヤベえ…
Dialogue: 0,0:01:38.00,0:01:41.22,Default#3,,0,0,0,,（千空）\Nスパルタ工作クラブの始まりだ
Dialogue: 0,0:01:41.34,0:01:43.93,Default#3,,0,0,0,,唆(そそ)るぜ これは！
Dialogue: 0,0:01:44.43,0:01:49.43,Default#3,,0,0,0,,♪～
Dialogue: 0,0:03:08.89,0:03:13.89,Default#3,,0,0,0,,～♪
Dialogue: 0,0:03:22.73,0:03:24.36,Default#3,,0,0,0,,（千空）電気をためるハコ…
Dialogue: 0,0:03:24.49,0:03:27.66,Default#3,,0,0,0,,つまりバッテリーを作るには\N鉛の板２枚を―
Dialogue: 0,0:03:28.07,0:03:32.12,Default#3,,0,0,0,,おなじみ二面性の女神\Nリューさんに漬け込むだけだ
Dialogue: 0,0:03:32.24,0:03:33.37,Default#3,,0,0,0,,（リューさん）ウフフフ…
Dialogue: 0,0:03:33.49,0:03:34.58,Default#3,,0,0,0,,グアアア！
Dialogue: 0,0:03:34.70,0:03:37.67,Default#3,,0,0,0,,リューさん？　ああ 硫酸ね
Dialogue: 0,0:03:38.12,0:03:41.13,Default#3,,0,0,0,,ここに電気流しゃ\N勝手にたまってく
Dialogue: 0,0:03:41.25,0:03:43.84,Default#3,,0,0,0,,ククク 思ったよか簡単だろ？
Dialogue: 0,0:03:43.96,0:03:46.88,Default#3,,0,0,0,,簡単じゃねえよ 結構 キチーぞ
Dialogue: 0,0:03:47.01,0:03:49.51,Default#3,,0,0,0,,これをあと５台作って\N全部つなげる
Dialogue: 0,0:03:49.64,0:03:50.64,Default#3,,0,0,0,,（カセキ）オホー！
Dialogue: 0,0:03:50.76,0:03:52.47,Default#3,,0,0,0,,ワシら工作組ってば―
Dialogue: 0,0:03:52.60,0:03:55.81,Default#3,,0,0,0,,ちょっとこれ\N頑張りすぎじゃないの この間から
Dialogue: 0,0:04:01.69,0:04:04.61,Default#3,,0,0,0,,ホントに こんなちっちゃい\N入れ物にたまってんの？
Dialogue: 0,0:04:04.73,0:04:06.78,Default#3,,0,0,0,,（千空）お？\N（銀狼）円盤のグルグルパワーが…
Dialogue: 0,0:04:06.90,0:04:08.66,Default#3,,0,0,0,,ゴギャギャアアアア！
Dialogue: 0,0:04:08.78,0:04:10.32,Default#3,,0,0,0,,ちゃんと たまってたぞ
Dialogue: 0,0:04:13.87,0:04:14.70,Default#3,,0,0,0,,はっ！
Dialogue: 0,0:04:15.83,0:04:20.67,Default#3,,0,0,0,,あの超～必死に金狼と回すグルグル\Nやんなくていいってこと～？
Dialogue: 0,0:04:20.79,0:04:21.75,Default#3,,0,0,0,,（金狼）あっ！
Dialogue: 0,0:04:22.13,0:04:24.30,Default#3,,0,0,0,,ああ 一生な
Dialogue: 0,0:04:24.42,0:04:28.13,Default#3,,0,0,0,,言ったじゃねえか\N動力の時代だってよ
Dialogue: 0,0:04:28.68,0:04:30.84,Default#3,,0,0,0,,（金狼）うう… うう…\N（銀狼）動力バンザーイ！
Dialogue: 0,0:04:30.97,0:04:33.01,Default#3,,0,0,0,,（コクヨウ）\Nサボり魔 銀狼は ともかく
Dialogue: 0,0:04:33.14,0:04:35.89,Default#3,,0,0,0,,超勤勉な金狼まで感動しとる
Dialogue: 0,0:04:36.02,0:04:39.39,Default#3,,0,0,0,,（スイカ）きっとあのグルグル\Nホントに大変だったんだよ
Dialogue: 0,0:04:39.52,0:04:41.19,Default#3,,0,0,0,,楽するためじゃねえぞ
Dialogue: 0,0:04:41.31,0:04:43.86,Default#3,,0,0,0,,ほかにマンパワー回せるっつう話だ
Dialogue: 0,0:04:43.98,0:04:46.86,Default#3,,0,0,0,,戦闘員は訓練に集中できるな！
Dialogue: 0,0:04:47.74,0:04:49.24,Default#3,,0,0,0,,（コハク）気合いを入れろ！\N（銀狼）ひいいっ！
Dialogue: 0,0:04:49.36,0:04:51.20,Default#3,,0,0,0,,うお～！　うっ！　ふっ！
Dialogue: 0,0:04:51.57,0:04:51.99,Default#3,,0,0,0,,（金狼と銀狼の掛け声）
Dialogue: 0,0:04:51.99,0:04:53.95,Default#3,,0,0,0,,（金狼と銀狼の掛け声）
Dialogue: 0,0:04:51.99,0:04:53.95,Default#3,,0,0,0,,（コハク）\N決戦まで わずか数か月で―
Dialogue: 0,0:04:53.95,0:04:54.08,Default#3,,0,0,0,,（金狼と銀狼の掛け声）
Dialogue: 0,0:04:54.08,0:04:57.87,Default#3,,0,0,0,,（金狼と銀狼の掛け声）
Dialogue: 0,0:04:54.08,0:04:57.87,Default#3,,0,0,0,,司(つかさ)や氷月(ひょうが)に勝てるよう\N鍛え上げねばならないのだ！
Dialogue: 0,0:04:58.20,0:04:58.50,Default#3,,0,0,0,,（カセキのうめき声）
Dialogue: 0,0:04:58.50,0:05:01.17,Default#3,,0,0,0,,（カセキのうめき声）
Dialogue: 0,0:04:58.50,0:05:01.17,Default#3,,0,0,0,,おら 休んでる場合じゃねえぞ\Nテメーら
Dialogue: 0,0:05:01.17,0:05:01.29,Default#3,,0,0,0,,（カセキのうめき声）
Dialogue: 0,0:05:01.29,0:05:02.96,Default#3,,0,0,0,,（カセキのうめき声）
Dialogue: 0,0:05:01.29,0:05:02.96,Default#3,,0,0,0,,スパルタすぎんだろ…
Dialogue: 0,0:05:03.08,0:05:05.25,Default#3,,0,0,0,,カセキじいちゃん\N死んじゃうんだよ
Dialogue: 0,0:05:05.63,0:05:09.72,Default#3,,0,0,0,,水車の死ぬほど大事な使い道が\Nもういっちょあんだよ
Dialogue: 0,0:05:09.84,0:05:13.05,Default#3,,0,0,0,,ククク 楽しいギア工作だ
Dialogue: 0,0:05:13.18,0:05:15.18,Default#3,,0,0,0,,水車の回転が あら不思議…
Dialogue: 0,0:05:15.18,0:05:16.01,Default#3,,0,0,0,,水車の回転が あら不思議…
Dialogue: 0,0:05:15.18,0:05:16.01,Default#3,,0,0,0,,（割れる音）
Dialogue: 0,0:05:16.26,0:05:17.64,Default#3,,0,0,0,,ガーン！
Dialogue: 0,0:05:18.14,0:05:20.27,Default#3,,0,0,0,,クッソ また割れた
Dialogue: 0,0:05:20.39,0:05:22.31,Default#3,,0,0,0,,設計図から写し直しだ
Dialogue: 0,0:05:22.44,0:05:25.57,Default#3,,0,0,0,,（クロム･カセキ）\Nうああ… ハァ ハァ ハァ…
Dialogue: 0,0:05:25.69,0:05:26.98,Default#3,,0,0,0,,（スイカ･千空）あ？
Dialogue: 0,0:05:27.86,0:05:32.95,Default#3,,0,0,0,,円の半分にだけ付けたギザギザが\N上下(うえした)交互に引っ掛かって…
Dialogue: 0,0:05:33.07,0:05:36.99,Default#3,,0,0,0,,ただの回転が\N往復の動きにすり替わんのか
Dialogue: 0,0:05:37.12,0:05:40.12,Default#3,,0,0,0,,よく出来すぎだろ\N人類の考えたカラクリ
Dialogue: 0,0:05:40.25,0:05:42.08,Default#3,,0,0,0,,これはヤベえ
Dialogue: 0,0:05:42.21,0:05:45.08,Default#3,,0,0,0,,（カセキ）工作が もうめっちゃ\Nワクワクしちゃって―
Dialogue: 0,0:05:45.21,0:05:47.00,Default#3,,0,0,0,,腹立たしいの～
Dialogue: 0,0:05:47.13,0:05:51.01,Default#3,,0,0,0,,ほれ\Nこまい細工は職人に任せんかい！
Dialogue: 0,0:05:51.30,0:05:52.18,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:05:52.30,0:05:54.64,Default#3,,0,0,0,,設計図見たら\Nしょぼしょぼしてたのが
Dialogue: 0,0:05:54.76,0:05:57.10,Default#3,,0,0,0,,いきなり\Nかっこよくなっちゃったんだよ
Dialogue: 0,0:05:57.22,0:05:58.89,Default#3,,0,0,0,,（カセキ）うおおおお！
Dialogue: 0,0:06:04.73,0:06:06.36,Default#3,,0,0,0,,（一同）おお～！
Dialogue: 0,0:06:07.90,0:06:09.61,Default#3,,0,0,0,,（銀狼）こ これは…
Dialogue: 0,0:06:10.86,0:06:13.11,Default#3,,0,0,0,,火 燃やしまくる\Nフーフー装置も…
Dialogue: 0,0:06:13.65,0:06:15.11,Default#3,,0,0,0,,レベル１
Dialogue: 0,0:06:15.49,0:06:17.12,Default#3,,0,0,0,,レベル２
Dialogue: 0,0:06:17.37,0:06:19.70,Default#3,,0,0,0,,ついに レベル３だぜ！
Dialogue: 0,0:06:24.96,0:06:27.04,Default#3,,0,0,0,,（ガンエン）ラーメン代とか言って\Nやらされてた
Dialogue: 0,0:06:27.17,0:06:29.21,Default#3,,0,0,0,,あの一番大変な仕事…
Dialogue: 0,0:06:29.34,0:06:32.13,Default#3,,0,0,0,,地獄の製鉄がついに機械化！
Dialogue: 0,0:06:32.26,0:06:34.76,Default#3,,0,0,0,,（村人たちの歓声）\N（ガンエン）動力ばんじゃ～い！
Dialogue: 0,0:06:34.88,0:06:37.64,Default#3,,0,0,0,,（千空）だから\N楽するためじゃねえっつってんだろ
Dialogue: 0,0:06:37.76,0:06:41.43,Default#3,,0,0,0,,非戦闘員は\N冬備えに集中できるな
Dialogue: 0,0:06:46.31,0:06:48.56,Default#3,,0,0,0,,（コクヨウ）\N何もかもが初めて見る…
Dialogue: 0,0:06:49.11,0:06:52.11,Default#3,,0,0,0,,おお これは瓶詰というのか
Dialogue: 0,0:06:52.24,0:06:54.15,Default#3,,0,0,0,,缶詰のご先祖ね
Dialogue: 0,0:06:55.82,0:06:59.41,Default#3,,0,0,0,,冬備えで肉 取っとけるとか\N天国だよ～
Dialogue: 0,0:07:00.66,0:07:03.91,Default#3,,0,0,0,,（ゲン）スパルタ工作クラブは\N今度は何作ってんの？
Dialogue: 0,0:07:04.54,0:07:05.54,Default#3,,0,0,0,,電球
Dialogue: 0,0:07:05.67,0:07:06.75,Default#3,,0,0,0,,ええ…
Dialogue: 0,0:07:07.58,0:07:10.63,Default#3,,0,0,0,,（ゲン）うん…\N十分 ゴイスーなんだけど―
Dialogue: 0,0:07:10.75,0:07:13.30,Default#3,,0,0,0,,だいぶ慣れて 驚かなくなってきた
Dialogue: 0,0:07:13.80,0:07:16.22,Default#3,,0,0,0,,（千空）明かりつくと\N2000度とかになって―
Dialogue: 0,0:07:16.34,0:07:19.47,Default#3,,0,0,0,,つないだ金の配線が\N溶けちまうからな
Dialogue: 0,0:07:19.60,0:07:24.31,Default#3,,0,0,0,,ガラスを極小接着剤代わりに使って\N補強するしかねえ
Dialogue: 0,0:07:24.43,0:07:28.60,Default#3,,0,0,0,,（カセキ）さすがのワシでも\Nこますぎて てこずっちゃうわい
Dialogue: 0,0:07:28.73,0:07:29.61,Default#3,,0,0,0,,（電球が割れる音）\N（カセキ）おお！
Dialogue: 0,0:07:29.73,0:07:30.65,Default#3,,0,0,0,,ああ！
Dialogue: 0,0:07:30.94,0:07:33.03,Default#3,,0,0,0,,ヤベえ これはムズい！
Dialogue: 0,0:07:33.15,0:07:33.48,Default#3,,0,0,0,,（千空）ククク\Nなんだ もう心折れてやがんのか？
Dialogue: 0,0:07:33.48,0:07:36.53,Default#3,,0,0,0,,（千空）ククク\Nなんだ もう心折れてやがんのか？
Dialogue: 0,0:07:33.48,0:07:36.53,Default#3,,0,0,0,,（クロムの泣き声）
Dialogue: 0,0:07:36.65,0:07:38.61,Default#3,,0,0,0,,（カセキ）ん？　ぜ～んぜん
Dialogue: 0,0:07:38.74,0:07:40.91,Default#3,,0,0,0,,体は とっくに\Nくたばっとるがの
Dialogue: 0,0:07:41.03,0:07:44.04,Default#3,,0,0,0,,さすが職人\N頼もしいじゃねえか
Dialogue: 0,0:07:44.54,0:07:45.79,Default#3,,0,0,0,,（カセキ）まったく
Dialogue: 0,0:07:45.91,0:07:49.33,Default#3,,0,0,0,,60年 この村で\Nの～んびりやってきたのに…
Dialogue: 0,0:07:49.46,0:07:50.33,Default#3,,0,0,0,,（千空）ん？
Dialogue: 0,0:07:50.46,0:07:53.80,Default#3,,0,0,0,,（カセキ）千空\Nぬしが来てからっちゅうもの―
Dialogue: 0,0:07:54.59,0:07:57.55,Default#3,,0,0,0,,桁違いに大変になっちゃって
Dialogue: 0,0:07:57.68,0:08:00.93,Default#3,,0,0,0,,そして 桁違いに楽しいわい
Dialogue: 0,0:08:01.60,0:08:04.43,Default#3,,0,0,0,,村の連中も一部を除いて―
Dialogue: 0,0:08:05.06,0:08:07.94,Default#3,,0,0,0,,みんな\Nそう思っとるんじゃないかの～
Dialogue: 0,0:09:33.60,0:09:36.98,Default#3,,0,0,0,,（鳥のさえずり）
Dialogue: 0,0:09:42.66,0:09:47.66,Default#3,,0,0,0,,おう でも 電気の明かりってよ\N前に俺らがやったときは―
Dialogue: 0,0:09:47.99,0:09:50.25,Default#3,,0,0,0,,一瞬で燃え尽きたじゃねえか
Dialogue: 0,0:09:51.16,0:09:52.00,Default#3,,0,0,0,,どうすんだ？
Dialogue: 0,0:09:52.54,0:09:54.96,Default#3,,0,0,0,,物をガンガン燃やすには\Nどうする？
Dialogue: 0,0:09:55.08,0:09:58.88,Default#3,,0,0,0,,（クロム）ん？\Nフーフー装置でガンガン空気を送る
Dialogue: 0,0:09:59.01,0:10:00.72,Default#3,,0,0,0,,水車で作ったやつだろ？
Dialogue: 0,0:10:00.84,0:10:03.30,Default#3,,0,0,0,,（千空）じゃあ\N逆に燃やさねえためには？
Dialogue: 0,0:10:03.43,0:10:05.01,Default#3,,0,0,0,,（クロム）う～ん…
Dialogue: 0,0:10:05.55,0:10:09.39,Default#3,,0,0,0,,あっ… 空気を なくす？
Dialogue: 0,0:10:13.02,0:10:16.69,Default#3,,0,0,0,,（千空）超絶重い\N水銀の重量を利用して―
Dialogue: 0,0:10:16.81,0:10:19.57,Default#3,,0,0,0,,落ちるときに\N電球の中の空気を巻き込んで―
Dialogue: 0,0:10:19.69,0:10:20.99,Default#3,,0,0,0,,真空にしていく
Dialogue: 0,0:10:24.74,0:10:25.57,Default#3,,0,0,0,,（スイカ）ああ…
Dialogue: 0,0:10:26.12,0:10:27.58,Default#3,,0,0,0,,（カセキ）フオオオ…
Dialogue: 0,0:10:28.49,0:10:30.54,Default#3,,0,0,0,,オホ～
Dialogue: 0,0:10:34.42,0:10:36.75,Default#3,,0,0,0,,（千空）\Nどんだけ熱くなろうがな―
Dialogue: 0,0:10:37.29,0:10:40.05,Default#3,,0,0,0,,周りに空気がなきゃ\N火はつかねえ
Dialogue: 0,0:10:40.92,0:10:45.51,Default#3,,0,0,0,,ソッコー燃え尽きたりしねえで\N長持ちするっつうわけだ
Dialogue: 0,0:10:53.93,0:10:56.06,Default#3,,0,0,0,,千空 皆を連れてきたぞ
Dialogue: 0,0:10:56.73,0:10:59.23,Default#3,,0,0,0,,（千空）あ～ ちょっと待ってろ
Dialogue: 0,0:10:59.57,0:11:00.57,Default#3,,0,0,0,,もうすぐだ
Dialogue: 0,0:11:00.69,0:11:01.53,Default#3,,0,0,0,,（コハク）うん？
Dialogue: 0,0:11:03.07,0:11:06.82,Default#3,,0,0,0,,（クロム）ヤベえ 雪だ\Nテスト中止か？
Dialogue: 0,0:11:06.95,0:11:09.24,Default#3,,0,0,0,,（千空）\Nいや できれば今日がいい
Dialogue: 0,0:11:09.53,0:11:10.91,Default#3,,0,0,0,,（ゲン･クロム）うん？
Dialogue: 0,0:11:11.95,0:11:14.00,Default#3,,0,0,0,,（カセキ）\N準備オーケーじゃよん
Dialogue: 0,0:11:14.33,0:11:18.04,Default#3,,0,0,0,,フッ… よし じゃあ つけっぞ
Dialogue: 0,0:11:19.63,0:11:20.84,Default#3,,0,0,0,,ん！
Dialogue: 0,0:11:34.81,0:11:38.65,Default#3,,0,0,0,,うわあ～\Nキラキラお星様みたいなんだよ
Dialogue: 0,0:11:39.52,0:11:41.23,Default#3,,0,0,0,,イルミネーション…
Dialogue: 0,0:11:41.36,0:11:45.15,Default#3,,0,0,0,,なんか一気に戻ったみたいだね～\N現代に
Dialogue: 0,0:11:45.49,0:11:47.24,Default#3,,0,0,0,,ここまで２か月
Dialogue: 0,0:11:47.36,0:11:49.82,Default#3,,0,0,0,,オホー\N結構 かかっちゃったの～
Dialogue: 0,0:11:49.95,0:11:52.12,Default#3,,0,0,0,,（千空）いや まあ 予定どおりだ
Dialogue: 0,0:11:52.24,0:11:53.08,Default#3,,0,0,0,,（ゲン）ん？
Dialogue: 0,0:11:53.91,0:11:59.46,Default#3,,0,0,0,,あ～ もしかして今日って\Nクリスマスか！
Dialogue: 0,0:12:00.71,0:12:04.13,Default#3,,0,0,0,,（千空）あ～ そういや そうだな
Dialogue: 0,0:12:05.21,0:12:07.17,Default#3,,0,0,0,,偶然にもな
Dialogue: 0,0:12:07.80,0:12:09.43,Default#3,,0,0,0,,ウッソ～
Dialogue: 0,0:12:13.01,0:12:16.43,Default#3,,0,0,0,,（そりの鈴の音）
Dialogue: 0,0:12:30.49,0:12:32.49,Default#3,,0,0,0,,（クロム）\Nヤベえ ひらめいた～！
Dialogue: 0,0:12:32.95,0:12:34.03,Default#3,,0,0,0,,行けんじゃねえか！
Dialogue: 0,0:12:34.16,0:12:37.87,Default#3,,0,0,0,,たいまつの光だけじゃ行けなかった\N洞窟の奥深くまでよ！
Dialogue: 0,0:12:38.00,0:12:40.33,Default#3,,0,0,0,,あ～\Nバッテリーと電球さえありゃ―
Dialogue: 0,0:12:40.46,0:12:42.67,Default#3,,0,0,0,,その100億倍は あされるな
Dialogue: 0,0:12:42.79,0:12:46.34,Default#3,,0,0,0,,洞窟の奥とか 面白(おもしれ)え石だらけに\N決まってんじゃねえか！
Dialogue: 0,0:12:46.46,0:12:48.84,Default#3,,0,0,0,,おう 行くぜ 科学発掘隊！
Dialogue: 0,0:12:48.97,0:12:51.47,Default#3,,0,0,0,,ヤベえ鉱石素材のお宝ゲットによ！
Dialogue: 0,0:12:51.84,0:12:52.76,Default#3,,0,0,0,,おう 千空！
Dialogue: 0,0:12:52.89,0:12:54.97,Default#3,,0,0,0,,科学じゃ まだまだ勝てねえがな
Dialogue: 0,0:12:55.10,0:12:58.39,Default#3,,0,0,0,,探索なら\N俺のが何百億倍もうわてだぜ
Dialogue: 0,0:12:58.52,0:13:00.23,Default#3,,0,0,0,,だろうな 年季が違う
Dialogue: 0,0:13:00.73,0:13:01.94,Default#3,,0,0,0,,何が欲しい？
Dialogue: 0,0:13:02.06,0:13:05.07,Default#3,,0,0,0,,クロム探検隊長が\Nゲットしてきてやるよ！
Dialogue: 0,0:13:05.19,0:13:06.78,Default#3,,0,0,0,,携帯作りのロードマップ
Dialogue: 0,0:13:06.90,0:13:09.03,Default#3,,0,0,0,,例によって\N足りねえ素材があんだろ！
Dialogue: 0,0:13:09.53,0:13:12.49,Default#3,,0,0,0,,ああ 足りねえ素材は…
Dialogue: 0,0:13:13.20,0:13:15.41,Default#3,,0,0,0,,ねえ\N（クロム）ねえのかよ！
Dialogue: 0,0:13:15.53,0:13:17.41,Default#3,,0,0,0,,もう一式そろってるわ～
Dialogue: 0,0:13:18.00,0:13:18.83,Default#3,,0,0,0,,ここまで俺らが―
Dialogue: 0,0:13:18.95,0:13:21.42,Default#3,,0,0,0,,さんざ頑張ったミッションでな
Dialogue: 0,0:13:21.79,0:13:24.88,Default#3,,0,0,0,,なら探検など\N今 行く必要があるのか？
Dialogue: 0,0:13:25.00,0:13:26.42,Default#3,,0,0,0,,この人手不足に
Dialogue: 0,0:13:26.55,0:13:29.47,Default#3,,0,0,0,,自分が探検に行きたいだけ\Nなんじゃないの～
Dialogue: 0,0:13:29.59,0:13:31.59,Default#3,,0,0,0,,新装備ゲットしたからって
Dialogue: 0,0:13:31.72,0:13:32.55,Default#3,,0,0,0,,（クロム）ギクッ
Dialogue: 0,0:13:32.89,0:13:33.97,Default#3,,0,0,0,,（千空）ククク
Dialogue: 0,0:13:34.09,0:13:37.10,Default#3,,0,0,0,,まあ 銅とか\Nモリモリ欲しいっちゃ欲しいがな
Dialogue: 0,0:13:37.22,0:13:40.85,Default#3,,0,0,0,,次の鬼レベル工作で\Nどんだけ浪費してもおかしくねえ
Dialogue: 0,0:13:40.98,0:13:42.85,Default#3,,0,0,0,,（クロム）\Nお おう！　それだ それ！
Dialogue: 0,0:13:42.98,0:13:45.31,Default#3,,0,0,0,,任せろ～！\N（千空）とっとと戻れよ！
Dialogue: 0,0:13:45.31,0:13:46.11,Default#3,,0,0,0,,任せろ～！\N（千空）とっとと戻れよ！
Dialogue: 0,0:13:45.31,0:13:46.11,Default#3,,0,0,0,,（クロム）\Nうおおお らあああ！
Dialogue: 0,0:13:46.11,0:13:46.23,Default#3,,0,0,0,,（クロム）\Nうおおお らあああ！
Dialogue: 0,0:13:46.23,0:13:48.69,Default#3,,0,0,0,,（クロム）\Nうおおお らあああ！
Dialogue: 0,0:13:46.23,0:13:48.69,Default#3,,0,0,0,,来週の年越しくらいまでにはな
Dialogue: 0,0:13:48.69,0:13:50.36,Default#3,,0,0,0,,（クロム）\Nうおおお らあああ！
Dialogue: 0,0:13:50.36,0:13:52.24,Default#3,,0,0,0,,（クロム）\Nうおおお らあああ！
Dialogue: 0,0:13:50.36,0:13:52.24,Default#3,,0,0,0,,んで 鬼レベル工作って何？
Dialogue: 0,0:13:52.24,0:13:53.53,Default#3,,0,0,0,,んで 鬼レベル工作って何？
Dialogue: 0,0:13:53.66,0:13:57.91,Default#3,,0,0,0,,クロムが戻るまでに完成させて\Nビックリさせちゃいたいの～
Dialogue: 0,0:13:58.37,0:14:00.45,Default#3,,0,0,0,,電球のレベルアップ版だな
Dialogue: 0,0:14:00.58,0:14:01.50,Default#3,,0,0,0,,（カセキ）オホー！
Dialogue: 0,0:14:01.62,0:14:05.63,Default#3,,0,0,0,,電球で もう鬼難易度だったのに\Nワクワクしちゃうじゃないの
Dialogue: 0,0:14:06.50,0:14:08.63,Default#3,,0,0,0,,（千空）コンピューターの卵を作る
Dialogue: 0,0:14:09.51,0:14:13.34,Default#3,,0,0,0,,電子のギア 真空管だ
Dialogue: 0,0:14:17.60,0:14:19.97,Default#3,,0,0,0,,（ゲン）真空管ね～
Dialogue: 0,0:14:20.18,0:14:23.39,Default#3,,0,0,0,,ついに何だか分からないもん\N登場しちゃった
Dialogue: 0,0:14:23.52,0:14:24.81,Default#3,,0,0,0,,いや 知ってるよ
Dialogue: 0,0:14:24.94,0:14:27.06,Default#3,,0,0,0,,名前は知ってるけど…
Dialogue: 0,0:14:27.19,0:14:30.40,Default#3,,0,0,0,,実際 それが何なのって\N言われるとね…
Dialogue: 0,0:14:30.82,0:14:33.24,Default#3,,0,0,0,,高熱で電子をブチ飛ばして
Dialogue: 0,0:14:33.36,0:14:37.03,Default#3,,0,0,0,,電流の向きをそろえたり\Nパワーアップしたりする
Dialogue: 0,0:14:37.16,0:14:40.54,Default#3,,0,0,0,,まあ 電気の世界のギアだって\N思っときゃいい
Dialogue: 0,0:14:41.04,0:14:43.54,Default#3,,0,0,0,,携帯の心臓部だ
Dialogue: 0,0:14:44.25,0:14:46.08,Default#3,,0,0,0,,（カセキ）ハァ…\N大変だけど これ
Dialogue: 0,0:14:46.21,0:14:49.42,Default#3,,0,0,0,,電球と ほとんど\N作り方一緒じゃないの
Dialogue: 0,0:14:49.55,0:14:50.46,Default#3,,0,0,0,,（千空）ああ
Dialogue: 0,0:14:50.59,0:14:52.72,Default#3,,0,0,0,,ただ\N真空管っつうくらいだから―
Dialogue: 0,0:14:52.84,0:14:57.51,Default#3,,0,0,0,,真空… つまり空気を完璧に\Nなくさなきゃなんねえ
Dialogue: 0,0:14:58.51,0:15:00.81,Default#3,,0,0,0,,だから\Nフィラメントの熱くなるとこに―
Dialogue: 0,0:15:00.93,0:15:02.60,Default#3,,0,0,0,,リンをのせた
Dialogue: 0,0:15:02.73,0:15:05.81,Default#3,,0,0,0,,熱で燃えりゃ\N周りの酸素を吸い込んで…
Dialogue: 0,0:15:06.65,0:15:07.44,Default#3,,0,0,0,,（千空･カセキ）あっ！
Dialogue: 0,0:15:07.86,0:15:10.69,Default#3,,0,0,0,,オホー！\N何もしとらんよ ワシ
Dialogue: 0,0:15:10.82,0:15:12.82,Default#3,,0,0,0,,根元から\Nひび割れちゃったんだよ
Dialogue: 0,0:15:14.03,0:15:16.95,Default#3,,0,0,0,,（千空）あ～ 配線の膨張か
Dialogue: 0,0:15:17.36,0:15:18.20,Default#3,,0,0,0,,（スイカ･カセキ）ん？
Dialogue: 0,0:15:18.66,0:15:20.95,Default#3,,0,0,0,,ガラスに刺してある金属線が―
Dialogue: 0,0:15:21.08,0:15:24.46,Default#3,,0,0,0,,リン 燃やした熱で\Nち～っとだけ膨張する
Dialogue: 0,0:15:24.58,0:15:27.33,Default#3,,0,0,0,,その数ミクロンの変化で\Nガラスが割れた
Dialogue: 0,0:15:27.46,0:15:30.50,Default#3,,0,0,0,,そんな精密な世界の話なの これ？
Dialogue: 0,0:15:30.63,0:15:33.13,Default#3,,0,0,0,,じゃあ どうすんの？\Nジーマーで
Dialogue: 0,0:15:35.13,0:15:39.30,Default#3,,0,0,0,,（カセキ）相変わらずワシら\N一発じゃ うまくいかんの～
Dialogue: 0,0:15:39.55,0:15:42.89,Default#3,,0,0,0,,（千空）ククク\Nひたすらトライ＆エラーだ
Dialogue: 0,0:15:43.43,0:15:43.77,Default#3,,0,0,0,,（カセキ）\Nふん！　ふん！　ふん！
Dialogue: 0,0:15:43.77,0:15:47.39,Default#3,,0,0,0,,（カセキ）\Nふん！　ふん！　ふん！
Dialogue: 0,0:15:43.77,0:15:47.39,Default#3,,0,0,0,,（千空）その繰り返しじゃねえか\Nいつでもよ
Dialogue: 0,0:15:47.90,0:15:49.44,Default#3,,0,0,0,,（クロム）おう！　今 戻ったぜ！
Dialogue: 0,0:15:49.56,0:15:50.52,Default#3,,0,0,0,,（千空･カセキ）ん？
Dialogue: 0,0:15:51.19,0:15:53.03,Default#3,,0,0,0,,どうだ！　銅だ！
Dialogue: 0,0:15:53.44,0:15:54.82,Default#3,,0,0,0,,（千空･カセキ）うわ…
Dialogue: 0,0:15:54.94,0:15:56.90,Default#3,,0,0,0,,（クロム）\Nいや… ダジャレじゃねえぞ
Dialogue: 0,0:15:57.70,0:16:00.70,Default#3,,0,0,0,,（千空）\Nちょうどいいとこに来たな 銅
Dialogue: 0,0:16:00.82,0:16:01.87,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:16:06.87,0:16:09.17,Default#3,,0,0,0,,銅の… チューブ？
Dialogue: 0,0:16:09.63,0:16:11.17,Default#3,,0,0,0,,チューブ型なら―
Dialogue: 0,0:16:11.29,0:16:13.46,Default#3,,0,0,0,,膨張しても中に膨らむだけで―
Dialogue: 0,0:16:13.59,0:16:15.30,Default#3,,0,0,0,,周りのガラスは割れねえ
Dialogue: 0,0:16:15.42,0:16:17.01,Default#3,,0,0,0,,（カセキ）\Nこれで ようやく！
Dialogue: 0,0:16:17.13,0:16:19.97,Default#3,,0,0,0,,いや～ 苦労しちゃったわい\N真空管
Dialogue: 0,0:16:20.09,0:16:21.76,Default#3,,0,0,0,,（はじける音）\N（カセキ･千空）おお？
Dialogue: 0,0:16:26.35,0:16:30.73,Default#3,,0,0,0,,（クロム）ヤベえ\N竹の熱線がソッコーで燃え尽きた
Dialogue: 0,0:16:40.03,0:16:41.99,Default#3,,0,0,0,,（千空）ハァ…
Dialogue: 0,0:16:43.58,0:16:48.96,Default#3,,0,0,0,,クッソ　竹のフィラメントじゃ\N真空管には根本的にもたねえんだ
Dialogue: 0,0:16:49.29,0:16:52.33,Default#3,,0,0,0,,ハ！　科学の説明は\Nさっぱり分からんが
Dialogue: 0,0:16:52.46,0:16:55.05,Default#3,,0,0,0,,とにかく 竹では弱すぎるのだな
Dialogue: 0,0:16:55.50,0:16:56.80,Default#3,,0,0,0,,何が要る？
Dialogue: 0,0:16:56.92,0:16:59.59,Default#3,,0,0,0,,もっと強い素材が要るなら\N採ってくんぜ
Dialogue: 0,0:16:59.72,0:17:01.43,Default#3,,0,0,0,,（千空）いや\N（クロム）おお？
Dialogue: 0,0:17:04.89,0:17:06.52,Default#3,,0,0,0,,ねえんだよ
Dialogue: 0,0:17:06.64,0:17:09.23,Default#3,,0,0,0,,この時代じゃ そんなもんは
Dialogue: 0,0:17:09.73,0:17:11.15,Default#3,,0,0,0,,（３人）ああ…
Dialogue: 0,0:17:13.90,0:17:15.77,Default#3,,0,0,0,,（村人）おお～\N（スイカ）これはどうなんだよ？
Dialogue: 0,0:17:16.44,0:17:20.20,Default#3,,0,0,0,,いろんな洞窟で\N手当たり次第ゲットしてきた鉱石だ
Dialogue: 0,0:17:20.32,0:17:21.91,Default#3,,0,0,0,,なんとか こん中からよ―
Dialogue: 0,0:17:22.03,0:17:24.83,Default#3,,0,0,0,,熱線に使える強(つえ)え素材\N探してやるぜ！
Dialogue: 0,0:17:25.28,0:17:27.70,Default#3,,0,0,0,,（ルビィ）\Nや～ これ宝石みたい！
Dialogue: 0,0:17:27.83,0:17:29.58,Default#3,,0,0,0,,それはキンカメムシの死骸だ
Dialogue: 0,0:17:29.71,0:17:30.54,Default#3,,0,0,0,,ギャー！
Dialogue: 0,0:17:30.87,0:17:31.71,Default#3,,0,0,0,,この石は？
Dialogue: 0,0:17:31.83,0:17:34.63,Default#3,,0,0,0,,なんかすごい黒くって\Nこんなに軽いよ～
Dialogue: 0,0:17:34.75,0:17:36.55,Default#3,,0,0,0,,タヌキのクソだ\N（銀狼）クソー！
Dialogue: 0,0:17:40.05,0:17:45.43,Default#3,,0,0,0,,（千空）ククク\N詰みか… ここまで来てよ
Dialogue: 0,0:17:46.06,0:17:47.64,Default#3,,0,0,0,,（スイカ）ああ…
Dialogue: 0,0:17:53.85,0:17:57.27,Default#3,,0,0,0,,（スイカ）スイカは千空みたいに\N石博士じゃないけど―
Dialogue: 0,0:17:57.40,0:18:01.15,Default#3,,0,0,0,,森や山で遊んでるから\N石ならいっぱい見てるんだよ
Dialogue: 0,0:18:02.74,0:18:05.91,Default#3,,0,0,0,,役に立つかもしれない\N珍しい石 見つけて―
Dialogue: 0,0:18:06.03,0:18:08.16,Default#3,,0,0,0,,お役に立ちたいんだよ
Dialogue: 0,0:18:11.00,0:18:12.04,Default#3,,0,0,0,,あ？
Dialogue: 0,0:18:15.00,0:18:20.01,Default#3,,0,0,0,,（クロムのいびき）
Dialogue: 0,0:18:24.55,0:18:29.97,Default#3,,0,0,0,,（千空）携帯が詰みなら詰みで\Nヘコんでるほど暇人じゃねえ
Dialogue: 0,0:18:30.10,0:18:33.44,Default#3,,0,0,0,,何か考えろ 別の手を…
Dialogue: 0,0:18:33.77,0:18:36.19,Default#3,,0,0,0,,千空ちゃ～ん 起きよっか\N（千空）うおっ…
Dialogue: 0,0:18:37.90,0:18:39.90,Default#3,,0,0,0,,今日 新年だよ～
Dialogue: 0,0:18:40.03,0:18:43.15,Default#3,,0,0,0,,初日の出 みんなで拝まなきゃ
Dialogue: 0,0:18:44.53,0:18:47.07,Default#3,,0,0,0,,気分一新 切り替え大事
Dialogue: 0,0:18:47.20,0:18:48.95,Default#3,,0,0,0,,そういうの俺の仕事でしょ
Dialogue: 0,0:18:49.45,0:18:51.50,Default#3,,0,0,0,,（千空）めんどくせえな～\Nメンタリストは
Dialogue: 0,0:18:51.50,0:18:52.45,Default#3,,0,0,0,,（千空）めんどくせえな～\Nメンタリストは
Dialogue: 0,0:18:51.50,0:18:52.45,Default#3,,0,0,0,,（スイカ）あっ！
Dialogue: 0,0:18:52.58,0:18:54.87,Default#3,,0,0,0,,あっ ス スイカも行くんだよ！
Dialogue: 0,0:18:55.00,0:18:56.17,Default#3,,0,0,0,,うっ！　ハァ ハァ…
Dialogue: 0,0:18:57.38,0:19:00.67,Default#3,,0,0,0,,（クロム）なんだ スイカ\N一晩中 石 あさってたのかよ？
Dialogue: 0,0:19:00.80,0:19:03.84,Default#3,,0,0,0,,（ルリ）スイカも\Nお役に立ちたいんですよね
Dialogue: 0,0:19:03.97,0:19:05.22,Default#3,,0,0,0,,（スイカ）そうなんだよ
Dialogue: 0,0:19:05.59,0:19:07.05,Default#3,,0,0,0,,新年か～
Dialogue: 0,0:19:07.18,0:19:10.89,Default#3,,0,0,0,,でも 石化の分\N体内カレンダー ズレちゃってるし
Dialogue: 0,0:19:11.01,0:19:13.48,Default#3,,0,0,0,,もう自分の年も分かんないね～
Dialogue: 0,0:19:13.60,0:19:17.65,Default#3,,0,0,0,,ふあ～ 生きてた日数で考えろ\N（ゲン）ええ～
Dialogue: 0,0:19:18.23,0:19:21.02,Default#3,,0,0,0,,ハ！　そんなもの\N覚えてるわけなかろう
Dialogue: 0,0:19:22.36,0:19:23.86,Default#3,,0,0,0,,千空ちゃんは…
Dialogue: 0,0:19:24.28,0:19:26.36,Default#3,,0,0,0,,6268日
Dialogue: 0,0:19:26.91,0:19:29.12,Default#3,,0,0,0,,ひい～\N（コハク）アハハ…
Dialogue: 0,0:19:29.87,0:19:32.58,Default#3,,0,0,0,,（ルリ）あっ！　そろそろです
Dialogue: 0,0:19:42.71,0:19:44.59,Default#3,,0,0,0,,朝日が昇るぞ
Dialogue: 0,0:19:56.06,0:19:57.19,Default#3,,0,0,0,,（千空）ん？
Dialogue: 0,0:19:57.69,0:19:58.73,Default#3,,0,0,0,,あっ！
Dialogue: 0,0:20:04.03,0:20:08.28,Default#3,,0,0,0,,（千空）蒼(あお)い… 宝石…
Dialogue: 0,0:20:09.82,0:20:11.03,Default#3,,0,0,0,,（ルリ）きれい…
Dialogue: 0,0:20:11.58,0:20:16.29,Default#3,,0,0,0,,あっ えっ…\Nこ これは なんで光ってるんだよ？
Dialogue: 0,0:20:17.33,0:20:19.67,Default#3,,0,0,0,,（千空）灰重石(かいじゅうせき)っつってな
Dialogue: 0,0:20:19.79,0:20:22.84,Default#3,,0,0,0,,紫外線が当たると蒼く光る
Dialogue: 0,0:20:24.42,0:20:29.68,Default#3,,0,0,0,,紫外線の反応が強く出る時間は\N朝日が昇る直前
Dialogue: 0,0:20:29.80,0:20:31.76,Default#3,,0,0,0,,つまり 今だけだ
Dialogue: 0,0:20:32.72,0:20:36.85,Default#3,,0,0,0,,この一瞬だけ輝く 魔法の石だね
Dialogue: 0,0:20:37.56,0:20:39.98,Default#3,,0,0,0,,初めて見たぜ こんなもん
Dialogue: 0,0:20:40.52,0:20:42.65,Default#3,,0,0,0,,あ～ 俺もだ
Dialogue: 0,0:20:42.77,0:20:45.40,Default#3,,0,0,0,,ウルトラレア鉱石だな
Dialogue: 0,0:20:45.53,0:20:49.45,Default#3,,0,0,0,,クロム＆スイカの\N超絶グッジョブじゃねえか！
Dialogue: 0,0:20:49.57,0:20:52.78,Default#3,,0,0,0,,こいつは\N現代のフィラメントにも使われる…
Dialogue: 0,0:20:54.08,0:20:57.54,Default#3,,0,0,0,,原子番号74 タングステン！
Dialogue: 0,0:21:06.17,0:21:11.59,Default#3,,0,0,0,,熱に負けねえ\N全宇宙最強の金属だ！
Dialogue: 0,0:21:14.89,0:21:18.89,Default#3,,0,0,0,,こんなカケラじゃ足んねえけど\N洞窟の川に流れてきたんだ
Dialogue: 0,0:21:19.02,0:21:21.35,Default#3,,0,0,0,,きっと奥にザクザクだぜ！
Dialogue: 0,0:21:21.48,0:21:24.69,Default#3,,0,0,0,,熱耐性最強の\Nタングステンっつうお宝がよ！
Dialogue: 0,0:21:25.23,0:21:27.53,Default#3,,0,0,0,,ライトは残り１セット
Dialogue: 0,0:21:27.65,0:21:30.20,Default#3,,0,0,0,,発掘隊メンバー最後の１人は…
Dialogue: 0,0:21:30.32,0:21:32.74,Default#3,,0,0,0,,（一同が息をのむ音）
Dialogue: 0,0:21:35.33,0:21:36.58,Default#3,,0,0,0,,（コハク）私ではない
Dialogue: 0,0:21:36.70,0:21:38.75,Default#3,,0,0,0,,戦闘員の訓練指導がある
Dialogue: 0,0:21:38.87,0:21:40.25,Default#3,,0,0,0,,ならば 誰だ？
Dialogue: 0,0:21:40.37,0:21:42.83,Default#3,,0,0,0,,（金狼）\N指名があれば\N無論行く！
Dialogue: 0,0:21:42.96,0:21:44.33,Default#3,,0,0,0,,（銀狼）\Nやだな～
Dialogue: 0,0:21:44.46,0:21:47.13,Default#3,,0,0,0,,怖いから 頼むよ\Nほかの人で
Dialogue: 0,0:21:47.25,0:21:48.26,Default#3,,0,0,0,,（スイカ）\N呼ばれちゃったら―
Dialogue: 0,0:21:48.38,0:21:51.01,Default#3,,0,0,0,,一生懸命\Nお役に立つんだよ
Dialogue: 0,0:21:51.13,0:21:54.26,Default#3,,0,0,0,,（カセキ）\Nワシでもいいけど\N洞窟探検でしょ？
Dialogue: 0,0:21:54.39,0:21:56.05,Default#3,,0,0,0,,腰がもつかの～
Dialogue: 0,0:21:56.18,0:21:59.64,Default#3,,0,0,0,,（ゲン）\N洞窟にメンタリストは\N要らないと思うけど～
Dialogue: 0,0:22:00.02,0:22:01.27,Default#3,,0,0,0,,（千空）フッ
Dialogue: 0,0:22:01.77,0:22:03.77,Default#3,,0,0,0,,マグマ！\N（マグマ）ぬおっ！
Dialogue: 0,0:22:04.19,0:22:07.98,Default#3,,0,0,0,,クソほど硬(かて)え灰重石採るには\Nばか力が要る
Dialogue: 0,0:22:08.11,0:22:13.61,Default#3,,0,0,0,,ククク 仲良し探検隊トリオで\N潜りに行こうじゃねえか！
Dialogue: 0,0:22:14.32,0:22:17.28,Default#3,,0,0,0,,楽しいお宝ダンジョンだ！
Dialogue: 0,0:22:18.41,0:22:20.00,Default#3,,0,0,0,,（マグマ）仲良し…？
Dialogue: 0,0:22:21.46,0:22:26.46,Default#3,,0,0,0,,♪～
Dialogue: 0,0:23:43.70,0:23:48.71,Default#3,,0,0,0,,～♪
