Dialogue: 0,0:00:00.83,0:00:03.87,Default#1,,0,0,0,,（煉獄(れんごく)）参ノ型(さんのかた) 気炎万象(きえんばんしょう)！
Dialogue: 0,0:00:05.08,0:00:07.71,Default#1,,0,0,0,,肆ノ型(しのかた) 盛炎(せいえん)のうねり！
Dialogue: 0,0:00:10.67,0:00:12.09,Default#1,,0,0,0,,（猗窩座(あかざ)）破壊殺(はかいさつ)…
Dialogue: 0,0:00:13.84,0:00:16.72,Default#1,,0,0,0,,伍ノ型(ごのかた) 炎虎(えんこ)！
Dialogue: 0,0:00:20.35,0:00:22.77,Default#1,,0,0,0,,乱式(らんしき)！　ヒャハ～！
Dialogue: 0,0:00:25.86,0:00:27.52,Default#1,,0,0,0,,杏寿郎(きょうじゅろう)！
Dialogue: 0,0:00:29.48,0:00:30.32,Default#1,,0,0,0,,ううっ！
Dialogue: 0,0:00:34.16,0:00:35.07,Default#1,,0,0,0,,はあ！
Dialogue: 0,0:00:43.08,0:00:46.63,Default#1,,0,0,0,,（伊之助(いのすけ)）\Nおお… やったか？　勝ったのか！
Dialogue: 0,0:00:51.13,0:00:52.51,Default#1,,0,0,0,,（炭治郎(たんじろう)）あ…
Dialogue: 0,0:01:06.77,0:01:10.98,Default#1,,0,0,0,,（煉獄）ハア ハア ハア…
Dialogue: 0,0:01:15.95,0:01:22.75,Default#1,,0,0,0,,（荒い息遣い）
Dialogue: 0,0:01:27.54,0:01:29.75,Default#1,,0,0,0,,死ぬな 杏寿郎
Dialogue: 0,0:01:30.34,0:01:35.59,Default#1,,0,0,0,,（煉獄）ハア ハア…
Dialogue: 0,0:01:35.67,0:01:39.85,Default#1,,0,0,0,,（荒い息遣い）
Dialogue: 0,0:01:42.89,0:01:44.18,Default#1,,0,0,0,,（炭治郎）煉獄さん
Dialogue: 0,0:01:44.68,0:01:46.44,Default#1,,0,0,0,,煉獄さん…
Dialogue: 0,0:01:48.69,0:01:50.48,Default#1,,0,0,0,,煉獄さん！
Dialogue: 0,0:01:55.19,0:01:58.78,Default#1,,0,0,0,,（猗窩座）\N生身を削る思いで戦ったとしても
Dialogue: 0,0:01:58.86,0:02:01.58,Default#1,,0,0,0,,全てムダなんだよ 杏寿郎
Dialogue: 0,0:02:02.87,0:02:06.08,Default#1,,0,0,0,,お前が俺にくらわせた\Nすばらしい斬撃も
Dialogue: 0,0:02:06.58,0:02:08.71,Default#1,,0,0,0,,既に完治してしまった
Dialogue: 0,0:02:09.21,0:02:11.00,Default#1,,0,0,0,,だが お前は どうだ？
Dialogue: 0,0:02:11.50,0:02:16.42,Default#1,,0,0,0,,潰れた左目 砕けたあばら骨\N傷ついた内臓
Dialogue: 0,0:02:16.51,0:02:18.63,Default#1,,0,0,0,,もう取り返しがつかない
Dialogue: 0,0:02:18.72,0:02:21.64,Default#1,,0,0,0,,鬼であれば まばたきする間に治る
Dialogue: 0,0:02:22.14,0:02:25.10,Default#1,,0,0,0,,そんなもの鬼ならば かすり傷だ
Dialogue: 0,0:02:25.60,0:02:29.31,Default#1,,0,0,0,,どうあがいても\N人間では鬼に勝てない
Dialogue: 0,0:02:29.81,0:02:31.27,Default#1,,0,0,0,,（炭治郎）ぐっ…
Dialogue: 0,0:02:31.77,0:02:34.23,Default#1,,0,0,0,,（炭治郎）助けに入りたいのに…
Dialogue: 0,0:02:35.57,0:02:36.69,Default#1,,0,0,0,,うっ…
Dialogue: 0,0:02:36.78,0:02:39.28,Default#1,,0,0,0,,（炭治郎）手足に力が入らない
Dialogue: 0,0:02:39.78,0:02:44.45,Default#1,,0,0,0,,傷のせいでもあるだろうが\Nヒノカミ神楽を使うと こうなる
Dialogue: 0,0:02:47.87,0:02:49.87,Default#1,,0,0,0,,（息を吐く音）
Dialogue: 0,0:02:58.42,0:03:00.59,Default#1,,0,0,0,,杏寿郎 お前…
Dialogue: 0,0:03:02.51,0:03:03.18,Default#1,,0,0,0,,（煉獄）俺は…
Dialogue: 0,0:03:04.68,0:03:07.60,Default#1,,0,0,0,,俺の責務を全うする！
Dialogue: 0,0:03:07.68,0:03:10.81,Default#1,,0,0,0,,ここにいる者は誰も死なせない！
Dialogue: 0,0:03:12.56,0:03:16.07,Default#1,,0,0,0,,（煉獄）一瞬で多くの面積を\N根こそぎ えぐり斬る
Dialogue: 0,0:03:16.15,0:03:18.90,Default#1,,0,0,0,,炎の呼吸 奥義
Dialogue: 0,0:03:23.53,0:03:27.66,Default#1,,0,0,0,,ああ… すばらしい闘気だ
Dialogue: 0,0:03:27.75,0:03:32.58,Default#1,,0,0,0,,それほどの傷を負いながら\Nその気迫 その精神力
Dialogue: 0,0:03:32.67,0:03:35.17,Default#1,,0,0,0,,一分の隙もない構え
Dialogue: 0,0:03:35.67,0:03:37.55,Default#1,,0,0,0,,ハハハハ…
Dialogue: 0,0:03:38.05,0:03:41.59,Default#1,,0,0,0,,やはり お前は鬼になれ 杏寿郎！
Dialogue: 0,0:03:41.68,0:03:44.85,Default#1,,0,0,0,,俺と永遠に戦い続けよう！
Dialogue: 0,0:03:44.93,0:03:49.02,Default#1,,0,0,0,,（煉獄）\N心を燃やせ　限界を超えろ
Dialogue: 0,0:03:50.52,0:03:54.94,Default#1,,0,0,0,,俺は炎柱(えんばしら) 煉獄杏寿郎！
Dialogue: 0,0:03:55.44,0:03:59.07,Default#1,,0,0,0,,玖ノ型(くのかた) 煉獄！
Dialogue: 0,0:03:59.15,0:04:02.20,Default#1,,0,0,0,,破壊殺･滅式(めっしき)！
Dialogue: 0,0:04:04.07,0:04:04.74,Default#1,,0,0,0,,（煉獄）うおお！
Dialogue: 0,0:04:13.67,0:04:14.83,Default#1,,0,0,0,,ハハッ
Dialogue: 0,0:04:19.26,0:04:20.59,Default#1,,0,0,0,,煉獄さん！
Dialogue: 0,0:04:21.51,0:04:23.84,Default#1,,0,0,0,,（煉獄）うおおお！\N（猗窩座）はあああ！
Dialogue: 0,0:04:23.93,0:04:25.34,Default#1,,0,0,0,,ハハハッ
Dialogue: 0,0:04:32.56,0:04:34.48,Default#1,,0,0,0,,（煉獄）うおお～！
Dialogue: 0,0:04:37.19,0:04:37.98,Default#1,,0,0,0,,ううっ！
Dialogue: 0,0:04:39.07,0:04:41.49,Default#1,,0,0,0,,うおおおお！
Dialogue: 0,0:04:53.83,0:04:55.08,Default#1,,0,0,0,,（炭治郎）煉獄さん…
Dialogue: 0,0:05:00.30,0:05:01.67,Default#1,,0,0,0,,煉獄さん！
Dialogue: 0,0:05:04.26,0:05:05.01,Default#1,,0,0,0,,（炭治郎）見えた！
Dialogue: 0,0:05:05.51,0:05:07.89,Default#1,,0,0,0,,れん… ごく…
Dialogue: 0,0:05:12.43,0:05:13.10,Default#1,,0,0,0,,かはっ…
Dialogue: 0,0:05:14.64,0:05:16.52,Default#1,,0,0,0,,（炭治郎）あああ…
Dialogue: 0,0:05:16.60,0:05:18.23,Default#1,,0,0,0,,ああっ！
Dialogue: 0,0:05:18.73,0:05:22.11,Default#1,,0,0,0,,死ぬ… 死んでしまうぞ 杏寿郎！
Dialogue: 0,0:05:22.19,0:05:25.86,Default#1,,0,0,0,,鬼になれ　鬼になると言え！
Dialogue: 0,0:05:25.95,0:05:30.53,Default#1,,0,0,0,,お前は 選ばれし強き者なのだ！
Dialogue: 0,0:05:32.66,0:05:37.62,Default#1,,0,0,0,,（風鈴の音）
Dialogue: 0,0:05:43.51,0:05:44.63,Default#1,,0,0,0,,（瑠火(るか)）杏寿郎
Dialogue: 0,0:05:44.72,0:05:45.97,Default#1,,0,0,0,,（煉獄）はい！　母上
Dialogue: 0,0:05:47.47,0:05:51.89,Default#1,,0,0,0,,（瑠火）よく考えるのです\N母が今から聞くことを
Dialogue: 0,0:05:52.47,0:05:57.60,Default#1,,0,0,0,,なぜ自分が人よりも\N強く生まれたのか分かりますか？
Dialogue: 0,0:05:58.10,0:05:59.10,Default#1,,0,0,0,,（煉獄）うっ…
Dialogue: 0,0:06:02.02,0:06:03.28,Default#1,,0,0,0,,分かりません！
Dialogue: 0,0:06:04.07,0:06:06.36,Default#1,,0,0,0,,弱き人を助けるためです
Dialogue: 0,0:06:08.28,0:06:12.91,Default#1,,0,0,0,,生まれついて\N人よりも多くの才に恵まれた者は
Dialogue: 0,0:06:13.41,0:06:18.87,Default#1,,0,0,0,,その力を 世のため人のために\N使わねばなりません
Dialogue: 0,0:06:20.25,0:06:22.75,Default#1,,0,0,0,,天から賜りし力で
Dialogue: 0,0:06:22.84,0:06:27.63,Default#1,,0,0,0,,人を傷つけること\N私腹を肥やすことは許されません
Dialogue: 0,0:06:29.18,0:06:34.43,Default#1,,0,0,0,,弱き人を助けることは\N強く生まれた者の責務です
Dialogue: 0,0:06:35.02,0:06:39.10,Default#1,,0,0,0,,責任を持って果たさなければ\Nならない使命なのです
Dialogue: 0,0:06:39.90,0:06:43.36,Default#1,,0,0,0,,決して忘れることなきように
Dialogue: 0,0:06:43.86,0:06:44.86,Default#1,,0,0,0,,はい！
Dialogue: 0,0:06:51.41,0:06:52.45,Default#1,,0,0,0,,あ…
Dialogue: 0,0:07:01.67,0:07:05.75,Default#1,,0,0,0,,私はもう 長くは生きられません
Dialogue: 0,0:07:06.92,0:07:11.22,Default#1,,0,0,0,,強く優しい子の母になれて\N幸せでした
Dialogue: 0,0:07:13.64,0:07:15.35,Default#1,,0,0,0,,あとは頼みます
Dialogue: 0,0:07:18.35,0:07:19.77,Default#1,,0,0,0,,（風鈴の音）
Dialogue: 0,0:07:22.19,0:07:23.61,Default#1,,0,0,0,,うおお！
Dialogue: 0,0:07:23.69,0:07:24.48,Default#1,,0,0,0,,ぐっ…
Dialogue: 0,0:07:24.98,0:07:27.40,Default#1,,0,0,0,,うおおおお！
Dialogue: 0,0:07:28.61,0:07:31.16,Default#1,,0,0,0,,（煉獄）母上 俺のほうこそ
Dialogue: 0,0:07:31.66,0:07:35.24,Default#1,,0,0,0,,あなたのような人に産んでもらえて\N光栄だった！
Dialogue: 0,0:07:35.83,0:07:38.00,Default#1,,0,0,0,,うおおおお！
Dialogue: 0,0:07:39.75,0:07:40.41,Default#1,,0,0,0,,（猗窩座）ぐうっ…
Dialogue: 0,0:07:42.46,0:07:43.17,Default#1,,0,0,0,,ぐっ！
Dialogue: 0,0:07:43.25,0:07:44.29,Default#1,,0,0,0,,（猗窩座）止めた？
Dialogue: 0,0:07:44.38,0:07:47.05,Default#1,,0,0,0,,信じられない力だ
Dialogue: 0,0:07:47.13,0:07:50.30,Default#1,,0,0,0,,みぞおちに\N俺の右腕が貫通しているんだぞ！
Dialogue: 0,0:07:50.38,0:07:51.68,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:07:51.76,0:07:55.26,Default#1,,0,0,0,,しまった 夜明けが近い
Dialogue: 0,0:07:55.76,0:07:58.60,Default#1,,0,0,0,,早く殺して この場を去らなければ
Dialogue: 0,0:07:59.73,0:08:02.23,Default#1,,0,0,0,,腕が… 抜けん
Dialogue: 0,0:08:04.56,0:08:06.23,Default#1,,0,0,0,,（煉獄）逃がさない！
Dialogue: 0,0:08:06.73,0:08:08.40,Default#1,,0,0,0,,くっ… うう…
Dialogue: 0,0:08:08.48,0:08:10.49,Default#1,,0,0,0,,（煉獄）ぐおお！
Dialogue: 0,0:08:11.32,0:08:13.24,Default#1,,0,0,0,,ハア ハア…
Dialogue: 0,0:08:13.32,0:08:16.70,Default#1,,0,0,0,,（炭治郎）\N煉獄さんに なんと言われようと
Dialogue: 0,0:08:18.08,0:08:21.16,Default#1,,0,0,0,,ここで やらなきゃ
Dialogue: 0,0:08:21.79,0:08:26.79,Default#1,,0,0,0,,斬らなければ 鬼の首を… 早く！
Dialogue: 0,0:08:29.30,0:08:31.22,Default#1,,0,0,0,,（猗窩座）夜が明ける
Dialogue: 0,0:08:31.30,0:08:33.55,Default#1,,0,0,0,,ここには陽光がさす
Dialogue: 0,0:08:34.05,0:08:37.47,Default#1,,0,0,0,,逃げなければ… 逃げなければ！
Dialogue: 0,0:08:38.14,0:08:38.72,Default#1,,0,0,0,,（猗窩座）ぐっ…
Dialogue: 0,0:08:40.06,0:08:46.48,Default#1,,0,0,0,,ぬおおおお！
Dialogue: 0,0:08:46.56,0:08:48.98,Default#1,,0,0,0,,（煉獄）絶対に放さん
Dialogue: 0,0:08:49.07,0:08:51.99,Default#1,,0,0,0,,お前の首を斬り落とすまでは！
Dialogue: 0,0:08:52.07,0:08:54.78,Default#1,,0,0,0,,（煉獄）うああああ！\N（猗窩座）うおおおお！
Dialogue: 0,0:08:54.86,0:08:57.16,Default#1,,0,0,0,,どけえ～！
Dialogue: 0,0:08:57.24,0:08:59.58,Default#1,,0,0,0,,うおおおお！
Dialogue: 0,0:09:00.66,0:09:01.62,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:09:01.70,0:09:03.66,Default#1,,0,0,0,,（炭治郎）伊之助 動け～！
Dialogue: 0,0:09:04.16,0:09:06.58,Default#1,,0,0,0,,煉獄さんのために動け～！
Dialogue: 0,0:09:07.17,0:09:07.83,Default#1,,0,0,0,,ううっ！
Dialogue: 0,0:09:10.80,0:09:14.63,Default#1,,0,0,0,,獣(ケダモノ)の呼吸 壱ノ牙(いちのきば) 穿(うが)ち抜き！
Dialogue: 0,0:09:15.63,0:09:16.13,Default#1,,0,0,0,,（猗窩座）ぬっ…
Dialogue: 0,0:09:19.76,0:09:20.85,Default#1,,0,0,0,,ぐあっ…
Dialogue: 0,0:09:20.93,0:09:23.14,Default#1,,0,0,0,,（炭治郎）ぐう… あっ！
Dialogue: 0,0:09:25.23,0:09:26.02,Default#1,,0,0,0,,ハッ！
Dialogue: 0,0:09:28.40,0:09:30.32,Default#1,,0,0,0,,うう…
Dialogue: 0,0:09:33.24,0:09:34.65,Default#1,,0,0,0,,（猗窩座）あっ！
Dialogue: 0,0:09:36.24,0:09:39.20,Default#1,,0,0,0,,早く陽光の陰になる所へ
Dialogue: 0,0:09:40.53,0:09:42.04,Default#1,,0,0,0,,ハア ハア…
Dialogue: 0,0:09:47.46,0:09:48.21,Default#1,,0,0,0,,ううっ！
Dialogue: 0,0:09:49.46,0:09:53.05,Default#1,,0,0,0,,（猗窩座）\Nてこずった　早く太陽から距離を
Dialogue: 0,0:09:53.13,0:09:53.71,Default#1,,0,0,0,,ハッ…
Dialogue: 0,0:09:54.63,0:09:55.76,Default#1,,0,0,0,,どおっ…
Dialogue: 0,0:09:56.26,0:09:57.55,Default#1,,0,0,0,,ぐっ…
Dialogue: 0,0:10:00.26,0:10:02.01,Default#1,,0,0,0,,（炭治郎）逃げるな！
Dialogue: 0,0:10:02.51,0:10:04.27,Default#1,,0,0,0,,逃げるな 卑怯者(ひきょうもの)！
Dialogue: 0,0:10:04.35,0:10:06.39,Default#1,,0,0,0,,逃げるな～！
Dialogue: 0,0:10:08.48,0:10:11.98,Default#1,,0,0,0,,（猗窩座）\N何を言ってるんだ あのガキは
Dialogue: 0,0:10:12.07,0:10:14.65,Default#1,,0,0,0,,脳みそが頭に詰まってないのか？
Dialogue: 0,0:10:14.74,0:10:17.61,Default#1,,0,0,0,,俺は お前らから\N逃げてるんじゃない
Dialogue: 0,0:10:17.70,0:10:19.91,Default#1,,0,0,0,,太陽から逃げてるんだ！
Dialogue: 0,0:10:19.99,0:10:23.16,Default#1,,0,0,0,,それに\Nもう勝負はついてるだろうが
Dialogue: 0,0:10:23.24,0:10:26.83,Default#1,,0,0,0,,あいつは まもなく 力尽きて死ぬ！
Dialogue: 0,0:10:29.42,0:10:31.08,Default#1,,0,0,0,,（炭治郎）いつだって鬼殺隊(きさつたい)は
Dialogue: 0,0:10:31.17,0:10:35.63,Default#1,,0,0,0,,お前らに有利な夜の闇の中で\N戦っているんだ！
Dialogue: 0,0:10:35.71,0:10:37.76,Default#1,,0,0,0,,生身の人間がだ！
Dialogue: 0,0:10:37.84,0:10:40.18,Default#1,,0,0,0,,傷だって簡単には塞がらない
Dialogue: 0,0:10:40.26,0:10:43.01,Default#1,,0,0,0,,失った手足が戻ることもない！
Dialogue: 0,0:10:43.76,0:10:45.85,Default#1,,0,0,0,,逃げるな バカ野郎！
Dialogue: 0,0:10:45.93,0:10:49.60,Default#1,,0,0,0,,バカ野郎！　卑怯者！
Dialogue: 0,0:10:49.69,0:10:55.48,Default#1,,0,0,0,,お前なんかより煉獄さんのほうが\Nずっとすごいんだ！　強いんだ！
Dialogue: 0,0:10:55.98,0:10:59.70,Default#1,,0,0,0,,煉獄さんは負けてない\N誰も死なせなかった！
Dialogue: 0,0:11:00.20,0:11:03.33,Default#1,,0,0,0,,戦い抜いた！　守り抜いた！
Dialogue: 0,0:11:03.83,0:11:05.83,Default#1,,0,0,0,,お前の負けだ！
Dialogue: 0,0:11:05.91,0:11:09.92,Default#1,,0,0,0,,煉獄さんの勝ちだ～！
Dialogue: 0,0:11:10.58,0:11:14.84,Default#1,,0,0,0,,ハア ハア…
Dialogue: 0,0:11:16.25,0:11:17.34,Default#1,,0,0,0,,うう…
Dialogue: 0,0:11:19.34,0:11:23.51,Default#1,,0,0,0,,あああああ！
Dialogue: 0,0:11:24.85,0:11:27.77,Default#1,,0,0,0,,（炭治郎の叫び声）
Dialogue: 0,0:11:30.85,0:11:33.06,Default#1,,0,0,0,,（炭治郎）あああ…
Dialogue: 0,0:11:36.36,0:11:39.78,Default#1,,0,0,0,,（炭治郎のすすり泣き）
Dialogue: 0,0:11:44.12,0:11:46.49,Default#1,,0,0,0,,（煉獄）\Nもう そんなに叫ぶんじゃない
Dialogue: 0,0:11:52.50,0:11:56.88,Default#1,,0,0,0,,腹の傷が開く\N君も軽傷じゃないんだ
Dialogue: 0,0:11:58.00,0:12:02.80,Default#1,,0,0,0,,竈門(かまど)少年が死んでしまったら\N俺の負けになってしまうぞ
Dialogue: 0,0:12:05.05,0:12:06.43,Default#1,,0,0,0,,煉獄さん…
Dialogue: 0,0:12:07.51,0:12:11.89,Default#1,,0,0,0,,こっちにおいで\N最後に少し話をしよう
Dialogue: 0,0:12:40.01,0:12:41.88,Default#1,,0,0,0,,（煉獄）思い出したことがあるんだ
Dialogue: 0,0:12:43.55,0:12:45.59,Default#1,,0,0,0,,昔の夢を見たときに
Dialogue: 0,0:12:46.76,0:12:51.35,Default#1,,0,0,0,,俺の生家\N煉獄家に行ってみるといい
Dialogue: 0,0:12:52.93,0:12:56.31,Default#1,,0,0,0,,歴代の炎柱が残した手記が\Nあるはずだ
Dialogue: 0,0:12:58.02,0:13:00.32,Default#1,,0,0,0,,父は よく それを読んでいたが
Dialogue: 0,0:13:01.28,0:13:04.65,Default#1,,0,0,0,,俺は読まなかったから\N内容が分からない
Dialogue: 0,0:13:06.53,0:13:11.41,Default#1,,0,0,0,,君が言っていた\Nヒノカミ神楽について 何か…
Dialogue: 0,0:13:12.29,0:13:14.62,Default#1,,0,0,0,,記されているかもしれない
Dialogue: 0,0:13:16.04,0:13:19.84,Default#1,,0,0,0,,れ… 煉獄さん もういいですから
Dialogue: 0,0:13:19.92,0:13:21.67,Default#1,,0,0,0,,呼吸で止血してください
Dialogue: 0,0:13:22.17,0:13:24.47,Default#1,,0,0,0,,傷を塞ぐ方法はないですか？
Dialogue: 0,0:13:25.26,0:13:28.68,Default#1,,0,0,0,,ない　俺はもう すぐに死ぬ
Dialogue: 0,0:13:29.18,0:13:30.35,Default#1,,0,0,0,,あ…
Dialogue: 0,0:13:30.85,0:13:34.52,Default#1,,0,0,0,,（煉獄）しゃべれるうちに\Nしゃべってしまうから聞いてくれ
Dialogue: 0,0:13:36.35,0:13:41.40,Default#1,,0,0,0,,弟の千寿郎(せんじゅろう)には 自分の心のまま
Dialogue: 0,0:13:42.03,0:13:46.20,Default#1,,0,0,0,,正しいと思う道を進むように\N伝えてほしい
Dialogue: 0,0:13:48.87,0:13:53.45,Default#1,,0,0,0,,父には 体を大切にしてほしいと
Dialogue: 0,0:13:55.87,0:13:57.00,Default#1,,0,0,0,,それから…
Dialogue: 0,0:13:57.87,0:13:59.42,Default#1,,0,0,0,,竈門少年
Dialogue: 0,0:14:01.88,0:14:04.13,Default#1,,0,0,0,,俺は 君の妹を信じる
Dialogue: 0,0:14:05.01,0:14:07.72,Default#1,,0,0,0,,鬼殺隊の一員として認める
Dialogue: 0,0:14:10.26,0:14:15.52,Default#1,,0,0,0,,汽車の中で あの少女が\N血を流しながら人間を守るのを見た
Dialogue: 0,0:14:16.27,0:14:19.48,Default#1,,0,0,0,,命を懸けて鬼と戦い 人を守る者は
Dialogue: 0,0:14:20.40,0:14:24.03,Default#1,,0,0,0,,誰がなんと言おうと鬼殺隊の一員だ
Dialogue: 0,0:14:25.32,0:14:27.07,Default#1,,0,0,0,,胸を張って生きろ
Dialogue: 0,0:14:28.70,0:14:29.66,Default#1,,0,0,0,,うっ…
Dialogue: 0,0:14:32.37,0:14:37.21,Default#1,,0,0,0,,己の弱さや ふがいなさに\Nどれだけ打ちのめされようと
Dialogue: 0,0:14:39.50,0:14:40.92,Default#1,,0,0,0,,心を燃やせ
Dialogue: 0,0:14:43.88,0:14:46.38,Default#1,,0,0,0,,歯を食いしばって前を向け
Dialogue: 0,0:14:48.05,0:14:53.43,Default#1,,0,0,0,,君が足を止めて うずくまっても\N時間の流れは止まってくれない
Dialogue: 0,0:14:54.14,0:14:57.81,Default#1,,0,0,0,,共に寄り添って悲しんではくれない
Dialogue: 0,0:14:59.69,0:15:02.15,Default#1,,0,0,0,,俺が ここで死ぬことは気にするな
Dialogue: 0,0:15:02.98,0:15:07.15,Default#1,,0,0,0,,柱(はしら)ならば\N後輩の盾となるのは当然だ
Dialogue: 0,0:15:08.15,0:15:12.07,Default#1,,0,0,0,,柱ならば\N誰であっても同じことをする
Dialogue: 0,0:15:12.99,0:15:14.66,Default#1,,0,0,0,,若い芽は摘ませない
Dialogue: 0,0:15:16.41,0:15:17.62,Default#1,,0,0,0,,竈門少年
Dialogue: 0,0:15:19.41,0:15:20.83,Default#1,,0,0,0,,猪頭(いのがしら)少年
Dialogue: 0,0:15:21.67,0:15:23.34,Default#1,,0,0,0,,黄色い少年
Dialogue: 0,0:15:24.88,0:15:26.88,Default#1,,0,0,0,,もっともっと成長しろ
Dialogue: 0,0:15:27.71,0:15:32.97,Default#1,,0,0,0,,そして 今度は 君たちが\N鬼殺隊を支える柱となるのだ
Dialogue: 0,0:15:34.30,0:15:35.97,Default#1,,0,0,0,,俺は信じる
Dialogue: 0,0:15:36.81,0:15:39.27,Default#1,,0,0,0,,君たちを信じる
Dialogue: 0,0:15:44.06,0:15:44.90,Default#1,,0,0,0,,うっ…
Dialogue: 0,0:15:55.58,0:15:56.58,Default#1,,0,0,0,,（煉獄）ハッ…
Dialogue: 0,0:16:05.29,0:16:06.46,Default#1,,0,0,0,,母上
Dialogue: 0,0:16:11.34,0:16:13.39,Default#1,,0,0,0,,俺は ちゃんと やれただろうか
Dialogue: 0,0:16:15.80,0:16:21.69,Default#1,,0,0,0,,やるべきこと 果たすべきことを\N全うできましたか？
Dialogue: 0,0:16:31.15,0:16:33.28,Default#1,,0,0,0,,（瑠火）立派にできましたよ
Dialogue: 0,0:17:09.73,0:17:12.11,Default#1,,0,0,0,,（善逸(ぜんいつ)）汽車が脱線するとき
Dialogue: 0,0:17:12.65,0:17:15.20,Default#1,,0,0,0,,煉獄さんが\Nいっぱい技を出しててさ
Dialogue: 0,0:17:15.99,0:17:19.99,Default#1,,0,0,0,,車両の被害を最小限に\Nとどめてくれたんだよな
Dialogue: 0,0:17:22.20,0:17:23.66,Default#1,,0,0,0,,（炭治郎）そうだろうな
Dialogue: 0,0:17:25.25,0:17:27.38,Default#1,,0,0,0,,死んじゃうなんて そんな…
Dialogue: 0,0:17:28.25,0:17:30.42,Default#1,,0,0,0,,ホントに上弦(じょうげん)の鬼が来たのか？
Dialogue: 0,0:17:30.92,0:17:31.92,Default#1,,0,0,0,,（炭治郎）うん
Dialogue: 0,0:17:32.92,0:17:35.26,Default#1,,0,0,0,,なんで来んだよ 上弦なんか
Dialogue: 0,0:17:35.93,0:17:38.80,Default#1,,0,0,0,,そんな強いの？　そんなさ…
Dialogue: 0,0:17:39.43,0:17:40.31,Default#1,,0,0,0,,（炭治郎）うん…
Dialogue: 0,0:17:42.93,0:17:44.73,Default#1,,0,0,0,,悔しいなあ
Dialogue: 0,0:17:46.44,0:17:49.06,Default#1,,0,0,0,,何かひとつできるようになっても
Dialogue: 0,0:17:50.19,0:17:54.44,Default#1,,0,0,0,,また すぐ\N目の前に分厚い壁があるんだ
Dialogue: 0,0:17:55.95,0:18:01.03,Default#1,,0,0,0,,すごい人は もっとずっと\N先のところで戦っているのに
Dialogue: 0,0:18:02.04,0:18:04.41,Default#1,,0,0,0,,俺は まだ そこに行けない
Dialogue: 0,0:18:06.00,0:18:09.46,Default#1,,0,0,0,,こんなところで\Nつまずいているような俺は
Dialogue: 0,0:18:10.50,0:18:11.67,Default#1,,0,0,0,,俺は…
Dialogue: 0,0:18:14.46,0:18:18.51,Default#1,,0,0,0,,煉獄さんみたいになれるのかなあ
Dialogue: 0,0:18:19.01,0:18:23.39,Default#1,,0,0,0,,（炭治郎と善逸の泣き声）
Dialogue: 0,0:18:28.98,0:18:31.65,Default#1,,0,0,0,,（伊之助）\N弱気なこと言ってんじゃねえ！
Dialogue: 0,0:18:33.23,0:18:37.24,Default#1,,0,0,0,,なれるか なれねえかなんて\Nくだらねえこと言うんじゃねえ！
Dialogue: 0,0:18:37.74,0:18:39.61,Default#1,,0,0,0,,信じると言われたなら
Dialogue: 0,0:18:39.70,0:18:42.49,Default#1,,0,0,0,,それに応えること以外\N考えんじゃねえ！
Dialogue: 0,0:18:42.99,0:18:46.66,Default#1,,0,0,0,,死んだ生き物は\N土にかえるだけなんだよ！
Dialogue: 0,0:18:46.75,0:18:50.17,Default#1,,0,0,0,,べそべそしたって\N戻ってきやしねえんだよ！
Dialogue: 0,0:18:50.25,0:18:52.92,Default#1,,0,0,0,,悔しくても泣くんじゃねえ！
Dialogue: 0,0:18:53.00,0:18:56.42,Default#1,,0,0,0,,どんなに惨めでも恥ずかしくても
Dialogue: 0,0:18:56.51,0:18:58.51,Default#1,,0,0,0,,生きてかなきゃならねえんだぞ
Dialogue: 0,0:18:59.01,0:19:00.93,Default#1,,0,0,0,,お前も泣いてるじゃん
Dialogue: 0,0:19:01.43,0:19:04.72,Default#1,,0,0,0,,かぶり物から あふれるくらい\N涙出てるし
Dialogue: 0,0:19:04.81,0:19:05.31,Default#1,,0,0,0,,ぐっ…
Dialogue: 0,0:19:05.39,0:19:06.93,Default#1,,0,0,0,,（伊之助）俺は泣いてねえ！
Dialogue: 0,0:19:07.89,0:19:09.48,Default#1,,0,0,0,,ぐあっ…
Dialogue: 0,0:19:10.90,0:19:13.31,Default#1,,0,0,0,,ああっ… ああ！
Dialogue: 0,0:19:13.40,0:19:16.32,Default#1,,0,0,0,,（伊之助の泣き声）
Dialogue: 0,0:19:18.44,0:19:20.82,Default#1,,0,0,0,,（伊之助）こっち来い！　修行だ！
Dialogue: 0,0:19:20.91,0:19:27.16,Default#1,,0,0,0,,（３人の泣き声）
Dialogue: 0,0:19:32.13,0:19:34.63,Default#1,,0,0,0,,（伊之助）ああ！　ううっ…
Dialogue: 0,0:19:35.84,0:19:38.67,Default#1,,0,0,0,,（３人の泣き声）
Dialogue: 0,0:20:04.91,0:20:08.29,Default#1,,0,0,0,,（しのぶ）\Nそうですか 煉獄さんが…
Dialogue: 0,0:20:28.68,0:20:33.31,Default#1,,0,0,0,,（宇髄(うずい)）上弦の鬼には\N煉獄でさえ負けるのか
Dialogue: 0,0:20:38.73,0:20:40.40,Default#1,,0,0,0,,（伊黒(いぐろ)）俺は信じない
Dialogue: 0,0:20:47.53,0:20:49.49,Default#1,,0,0,0,,（悲鳴嶼(ひめじま)）南無阿弥陀仏(なむあみだぶつ)…
Dialogue: 0,0:20:55.71,0:21:00.21,Default#1,,0,0,0,,（不死川(しなずがわ)）\N醜い鬼どもは 俺が殲滅(せんめつ)する
Dialogue: 0,0:21:08.47,0:21:09.51,Default#1,,0,0,0,,（冨岡(とみおか)）そうか
Dialogue: 0,0:21:19.27,0:21:24.07,Default#1,,0,0,0,,（産屋敷(うぶやしき)）200人の乗客は\N一人として死ななかったか
Dialogue: 0,0:21:24.78,0:21:29.03,Default#1,,0,0,0,,杏寿郎は頑張ったんだね\Nすごい子だ
Dialogue: 0,0:21:30.74,0:21:35.92,Default#1,,0,0,0,,寂しくはないよ\N私も もう長くは生きられない
Dialogue: 0,0:21:38.84,0:21:41.88,Default#1,,0,0,0,,近いうちに\N杏寿郎や みんなのいる…
Dialogue: 0,0:21:42.59,0:21:45.30,Default#1,,0,0,0,,黄泉(よみ)の国へ行くだろうから
Dialogue: 0,0:21:54.89,0:21:55.94,Default#1,,0,0,0,,（炭治郎）うう…
Dialogue: 0,0:21:56.98,0:22:01.61,Default#1,,0,0,0,,煉獄さん… 煉獄さん
Dialogue: 0,0:22:04.03,0:22:06.11,Default#1,,0,0,0,,煉獄さん！
Dialogue: 0,0:22:10.07,0:22:12.08,Default#1,,0,0,0,,♪～
Dialogue: 0,0:26:26.91,0:26:28.92,Default#1,,0,0,0,,～♪
