Dialogue: 0,0:00:22.77,0:00:23.60,Default#1,,0,0,0,,あ…
Dialogue: 0,0:00:30.90,0:00:31.74,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:00:34.32,0:00:35.66,Default#1,,0,0,0,,うわっ！
Dialogue: 0,0:01:08.11,0:01:09.65,Default#1,,0,0,0,,（戸が開く音）
Dialogue: 0,0:01:14.11,0:01:15.28,Default#1,,0,0,0,,（煉獄(れんごく)）うまい！
Dialogue: 0,0:01:19.37,0:01:20.08,Default#1,,0,0,0,,うまい！
Dialogue: 0,0:01:23.16,0:01:23.70,Default#1,,0,0,0,,うまい！
Dialogue: 0,0:01:24.79,0:01:27.21,Default#1,,0,0,0,,（鬼殺(きさつ)隊士）炎柱(えんばしら)\Nお食事中 失礼します
Dialogue: 0,0:01:27.71,0:01:29.71,Default#1,,0,0,0,,（煉獄）ああ\Nここに座るがいい
Dialogue: 0,0:01:30.21,0:01:33.46,Default#1,,0,0,0,,おやじさん\Nこの若者にも同じものを
Dialogue: 0,0:01:35.72,0:01:37.01,Default#1,,0,0,0,,（隊士）よろしいのですか？
Dialogue: 0,0:01:37.09,0:01:38.09,Default#1,,0,0,0,,（煉獄）もちろんだ
Dialogue: 0,0:01:38.18,0:01:39.80,Default#1,,0,0,0,,俺も もう一杯もらおう
Dialogue: 0,0:01:43.56,0:01:46.77,Default#1,,0,0,0,,（蕎麦(そば)屋）あんた\N気持ちのいい食いっぷりだな
Dialogue: 0,0:01:47.27,0:01:48.40,Default#1,,0,0,0,,待ってな
Dialogue: 0,0:01:49.48,0:01:53.40,Default#1,,0,0,0,,（隊士）\N炎柱 昨夜のお働き 見事でした
Dialogue: 0,0:01:54.03,0:01:56.99,Default#1,,0,0,0,,（煉獄）いや\N鬼は逃走したあとだったからな
Dialogue: 0,0:01:57.49,0:01:59.24,Default#1,,0,0,0,,負傷したあの女性は どうなった？
Dialogue: 0,0:01:59.74,0:02:01.37,Default#1,,0,0,0,,（隊士）医師が言うには
Dialogue: 0,0:02:01.45,0:02:04.25,Default#1,,0,0,0,,傷痕も残らないで済む\Nということです
Dialogue: 0,0:02:04.83,0:02:07.33,Default#1,,0,0,0,,（煉獄）\Nそうか　処置が早くてよかった
Dialogue: 0,0:02:08.33,0:02:11.04,Default#1,,0,0,0,,（隊士）はい　ご活躍のおかげです
Dialogue: 0,0:02:20.05,0:02:21.26,Default#1,,0,0,0,,いただきます
Dialogue: 0,0:02:25.52,0:02:26.31,Default#1,,0,0,0,,（煉獄）これは？
Dialogue: 0,0:02:26.81,0:02:28.52,Default#1,,0,0,0,,（蕎麦屋）俺のおごりだよ
Dialogue: 0,0:02:29.02,0:02:30.27,Default#1,,0,0,0,,（煉獄）ありがたい
Dialogue: 0,0:02:32.40,0:02:33.69,Default#1,,0,0,0,,あ… うまい！
Dialogue: 0,0:02:33.77,0:02:37.82,Default#1,,0,0,0,,おやっさん 上野に店出したって\Nやっていけるよ これ
Dialogue: 0,0:02:38.32,0:02:40.32,Default#1,,0,0,0,,（蕎麦屋）大きなお世話だよ
Dialogue: 0,0:02:40.82,0:02:43.28,Default#1,,0,0,0,,（煉獄）\Nおやじさん 景気はどうかな？
Dialogue: 0,0:02:44.29,0:02:45.91,Default#1,,0,0,0,,（蕎麦屋）どう見えるね
Dialogue: 0,0:02:46.00,0:02:47.04,Default#1,,0,0,0,,（煉獄）悪いな
Dialogue: 0,0:02:47.54,0:02:49.25,Default#1,,0,0,0,,（蕎麦屋）この沿線で
Dialogue: 0,0:02:49.33,0:02:51.71,Default#1,,0,0,0,,切り裂き魔ってのが\N出てるせいで
Dialogue: 0,0:02:51.79,0:02:54.04,Default#1,,0,0,0,,めっきり\N人手が減っちまったよ
Dialogue: 0,0:02:55.05,0:02:58.05,Default#1,,0,0,0,,雇い人にも暇を出す始末さ
Dialogue: 0,0:02:58.80,0:03:02.30,Default#1,,0,0,0,,こないだは\N汽車の車掌がやられたってよ
Dialogue: 0,0:03:03.72,0:03:06.22,Default#1,,0,0,0,,それに\N無限列車(むげんれっしゃ)ってえのが
Dialogue: 0,0:03:06.31,0:03:08.68,Default#1,,0,0,0,,運行中止に\Nなっちまっただろ
Dialogue: 0,0:03:09.19,0:03:12.61,Default#1,,0,0,0,,噂(うわさ)じゃな 40人ばかり
Dialogue: 0,0:03:12.69,0:03:16.28,Default#1,,0,0,0,,乗客が神隠しに\N遭っちまったっていうぜ
Dialogue: 0,0:03:16.86,0:03:18.61,Default#1,,0,0,0,,（煉獄）ほう 物騒だな
Dialogue: 0,0:03:19.86,0:03:21.99,Default#1,,0,0,0,,炎柱 本題ですが
Dialogue: 0,0:03:22.07,0:03:24.70,Default#1,,0,0,0,,その無限列車 所在が分かりました
Dialogue: 0,0:03:25.20,0:03:29.71,Default#1,,0,0,0,,さる機関庫に 人目につかぬよう\N搬入されたとの情報が
Dialogue: 0,0:03:31.62,0:03:32.50,Default#1,,0,0,0,,（煉獄）そうか
Dialogue: 0,0:03:33.83,0:03:35.34,Default#1,,0,0,0,,うん うまい！
Dialogue: 0,0:03:36.34,0:03:38.13,Default#1,,0,0,0,,（隊士）無限列車に向かいますか？
Dialogue: 0,0:03:38.63,0:03:40.76,Default#1,,0,0,0,,（煉獄）うむ　だが その前に
Dialogue: 0,0:03:40.84,0:03:44.30,Default#1,,0,0,0,,車掌の遺体が発見された駅を\N検分してみよう
Dialogue: 0,0:03:52.65,0:03:53.69,Default#1,,0,0,0,,（ふく）ハア…
Dialogue: 0,0:03:54.69,0:03:59.15,Default#1,,0,0,0,,（トミ）ふく ため息ついてないで\Nあんパン お食べ
Dialogue: 0,0:03:59.65,0:04:00.61,Default#1,,0,0,0,,（ふく）うん
Dialogue: 0,0:04:02.28,0:04:05.32,Default#1,,0,0,0,,お客さん 今日も少なかったね
Dialogue: 0,0:04:06.49,0:04:08.83,Default#1,,0,0,0,,毎日 言ってるけど
Dialogue: 0,0:04:08.91,0:04:11.33,Default#1,,0,0,0,,暗い時間は危ないから
Dialogue: 0,0:04:11.41,0:04:14.58,Default#1,,0,0,0,,明日のお手伝いは\Nお昼からにしようね
Dialogue: 0,0:04:15.08,0:04:17.21,Default#1,,0,0,0,,夜は 鬼が出るから
Dialogue: 0,0:04:18.75,0:04:20.17,Default#1,,0,0,0,,（鎹鴉(かすがいがらす)）異状ナシ
Dialogue: 0,0:04:20.26,0:04:23.26,Default#1,,0,0,0,,（隊士）見て回りましたが\N変わったところはありませんね
Dialogue: 0,0:04:23.76,0:04:24.64,Default#1,,0,0,0,,（煉獄）うむ
Dialogue: 0,0:04:24.72,0:04:26.22,Default#1,,0,0,0,,（ふく）鬼なんか いないよ！
Dialogue: 0,0:04:26.89,0:04:28.97,Default#1,,0,0,0,,また鬼の話
Dialogue: 0,0:04:29.06,0:04:32.39,Default#1,,0,0,0,,ダメだよ 頑張って お弁当\N売らなきゃいけないんだから
Dialogue: 0,0:04:32.89,0:04:35.10,Default#1,,0,0,0,,お母さんは おなかが大きいし
Dialogue: 0,0:04:35.19,0:04:38.44,Default#1,,0,0,0,,お父さんは 食堂の経営\Nうまくいってないって…
Dialogue: 0,0:04:41.49,0:04:44.57,Default#1,,0,0,0,,そんな心配は 大人がすればいいの
Dialogue: 0,0:04:45.07,0:04:49.12,Default#1,,0,0,0,,早朝と夜は\Nおばあちゃん一人がやるから
Dialogue: 0,0:04:49.20,0:04:52.71,Default#1,,0,0,0,,おばあちゃんが切り裂き魔に\N襲われたら どうするの！
Dialogue: 0,0:04:53.33,0:04:56.29,Default#1,,0,0,0,,（トミ）\Nあんたの身代わりなら本望だよ
Dialogue: 0,0:04:56.38,0:04:57.58,Default#1,,0,0,0,,（ふく）おばあちゃん！
Dialogue: 0,0:05:00.25,0:05:03.47,Default#1,,0,0,0,,（煉獄）やあ こんばんは\N気持ちのいい月夜ですね
Dialogue: 0,0:05:04.17,0:05:08.01,Default#1,,0,0,0,,俺は鬼を捜している者\N鬼を見ていませんか？
Dialogue: 0,0:05:08.51,0:05:10.26,Default#1,,0,0,0,,ええっ！　いきなり？
Dialogue: 0,0:05:10.35,0:05:14.10,Default#1,,0,0,0,,（鎹鴉）\N杏寿郎(きょうじゅろう)様！　単刀直入ガ過ギマス！
Dialogue: 0,0:05:14.18,0:05:15.73,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:05:15.81,0:05:18.81,Default#1,,0,0,0,,（煉獄）\Nん？　ご婦人 何か ご存じか
Dialogue: 0,0:05:20.94,0:05:25.24,Default#1,,0,0,0,,お… 鬼？　変なこと言って\Nなっ なんですか
Dialogue: 0,0:05:25.32,0:05:26.82,Default#1,,0,0,0,,近づかないで！
Dialogue: 0,0:05:27.45,0:05:28.28,Default#1,,0,0,0,,（煉獄）危ない！
Dialogue: 0,0:05:29.12,0:05:30.95,Default#1,,0,0,0,,そんなに ぶるぶるしていたら
Dialogue: 0,0:05:31.04,0:05:33.29,Default#1,,0,0,0,,大切なあんパンを落としてしまうぞ
Dialogue: 0,0:05:33.37,0:05:34.96,Default#1,,0,0,0,,来ないで～！
Dialogue: 0,0:05:35.96,0:05:37.04,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:05:37.12,0:05:38.21,Default#1,,0,0,0,,（隊士）あっ！\N（鎹鴉）カアッ…
Dialogue: 0,0:05:42.50,0:05:43.76,Default#1,,0,0,0,,（煉獄）うまい！
Dialogue: 0,0:05:45.59,0:05:49.09,Default#1,,0,0,0,,鬼なんか知らない\Nいるわけないでしょ そんなの
Dialogue: 0,0:05:49.60,0:05:51.35,Default#1,,0,0,0,,（トミ）ふく おやめ
Dialogue: 0,0:05:52.43,0:05:56.02,Default#1,,0,0,0,,この人は きっと悪い人じゃないよ
Dialogue: 0,0:05:59.90,0:06:00.90,Default#1,,0,0,0,,（ふく）ごめんなさい
Dialogue: 0,0:06:01.40,0:06:05.61,Default#1,,0,0,0,,このところ切り裂き魔事件のせいで\N神経が張り詰めちゃって
Dialogue: 0,0:06:06.61,0:06:08.70,Default#1,,0,0,0,,今し方 耳にしたところによると
Dialogue: 0,0:06:09.20,0:06:11.99,Default#1,,0,0,0,,あなた方は暗いうちから準備して
Dialogue: 0,0:06:12.08,0:06:14.66,Default#1,,0,0,0,,我々のために\N弁当を売りに来てくれるのだな
Dialogue: 0,0:06:15.16,0:06:16.54,Default#1,,0,0,0,,実にありがたい
Dialogue: 0,0:06:17.12,0:06:19.67,Default#1,,0,0,0,,そのような人々が\N傷つけられることは
Dialogue: 0,0:06:19.75,0:06:21.13,Default#1,,0,0,0,,決してあってはならない
Dialogue: 0,0:06:21.63,0:06:25.38,Default#1,,0,0,0,,安心するがいい\N切り裂き魔は 俺が片づける
Dialogue: 0,0:06:26.72,0:06:28.76,Default#1,,0,0,0,,あなた 一体…
Dialogue: 0,0:06:29.26,0:06:30.09,Default#1,,0,0,0,,（煉獄）では
Dialogue: 0,0:06:30.64,0:06:32.93,Default#1,,0,0,0,,（ふく）あの！\N（煉獄）ん？
Dialogue: 0,0:06:33.85,0:06:39.10,Default#1,,0,0,0,,その… もし よければ\Nお弁当 買ってもらえませんか？
Dialogue: 0,0:06:39.60,0:06:41.94,Default#1,,0,0,0,,そうだな　では…
Dialogue: 0,0:06:42.02,0:06:43.77,Default#1,,0,0,0,,全部 頂こう！
Dialogue: 0,0:06:49.32,0:06:51.12,Default#1,,0,0,0,,あの 炎柱…
Dialogue: 0,0:06:51.62,0:06:53.78,Default#1,,0,0,0,,（煉獄）\N隊の皆に良い土産が出来たな
Dialogue: 0,0:06:54.33,0:06:57.12,Default#1,,0,0,0,,ここからは俺一人で行く\Nご苦労だった
Dialogue: 0,0:06:58.71,0:06:59.50,Default#1,,0,0,0,,では
Dialogue: 0,0:07:04.04,0:07:05.67,Default#1,,0,0,0,,なんだったのかしら
Dialogue: 0,0:07:06.17,0:07:08.88,Default#1,,0,0,0,,お弁当 買ってくれたのは\Nうれしいけど
Dialogue: 0,0:07:09.51,0:07:11.43,Default#1,,0,0,0,,（トミ）早く帰るよ ふく
Dialogue: 0,0:07:11.93,0:07:14.81,Default#1,,0,0,0,,どうも嫌な感じがしてきたわ
Dialogue: 0,0:07:14.89,0:07:17.27,Default#1,,0,0,0,,まるで背筋が凍るような…
Dialogue: 0,0:07:18.60,0:07:19.85,Default#1,,0,0,0,,おばあちゃん？
Dialogue: 0,0:07:21.40,0:07:23.40,Default#1,,0,0,0,,（汽笛）
Dialogue: 0,0:07:43.79,0:07:44.67,Default#1,,0,0,0,,（車掌補）あっ…
Dialogue: 0,0:07:46.55,0:07:48.46,Default#1,,0,0,0,,あああ…
Dialogue: 0,0:07:49.47,0:07:51.51,Default#1,,0,0,0,,だ… 誰だ！
Dialogue: 0,0:07:54.72,0:07:57.06,Default#1,,0,0,0,,（煉獄）\N俺は見てのとおりの弁当売りだ
Dialogue: 0,0:07:57.56,0:07:59.18,Default#1,,0,0,0,,決して怪しい者ではない！
Dialogue: 0,0:07:59.68,0:08:00.81,Default#1,,0,0,0,,（鎹鴉）カア～
Dialogue: 0,0:08:01.35,0:08:05.81,Default#1,,0,0,0,,（車掌補）いや しかし…\Nこの列車は車庫に入りますので
Dialogue: 0,0:08:06.32,0:08:08.07,Default#1,,0,0,0,,（煉獄）うむ 了解している
Dialogue: 0,0:08:08.57,0:08:12.15,Default#1,,0,0,0,,そこに無限列車があると聞いた\Nそれに用がある
Dialogue: 0,0:08:13.24,0:08:14.41,Default#1,,0,0,0,,無限列車…
Dialogue: 0,0:08:14.91,0:08:17.24,Default#1,,0,0,0,,それなら\Nもう車庫にはありませんよ
Dialogue: 0,0:08:17.33,0:08:18.62,Default#1,,0,0,0,,そうなのか
Dialogue: 0,0:08:19.12,0:08:23.54,Default#1,,0,0,0,,（車掌補）今朝方 設備の整った\N整備工場へ運ばれてゆきました
Dialogue: 0,0:08:24.54,0:08:25.83,Default#1,,0,0,0,,あそこですが…
Dialogue: 0,0:08:27.84,0:08:28.75,Default#1,,0,0,0,,（煉獄）ほう
Dialogue: 0,0:08:29.55,0:08:32.01,Default#1,,0,0,0,,では 俺は ここで降りるとしよう！
Dialogue: 0,0:08:32.09,0:08:32.93,Default#1,,0,0,0,,え？
Dialogue: 0,0:08:33.59,0:08:34.80,Default#1,,0,0,0,,降りるって…
Dialogue: 0,0:08:35.51,0:08:36.55,Default#1,,0,0,0,,ああっ！
Dialogue: 0,0:09:13.38,0:09:14.76,Default#1,,0,0,0,,無限列車…
Dialogue: 0,0:09:15.76,0:09:16.80,Default#1,,0,0,0,,なるほど
Dialogue: 0,0:09:17.34,0:09:19.72,Default#1,,0,0,0,,かすかだが\N鬼がいた気配が残っている
Dialogue: 0,0:09:22.72,0:09:24.48,Default#1,,0,0,0,,（親方）おい あんた！
Dialogue: 0,0:09:25.14,0:09:27.48,Default#1,,0,0,0,,ここは立ち入り禁止だぜ
Dialogue: 0,0:09:27.56,0:09:28.56,Default#1,,0,0,0,,（煉獄）やあ
Dialogue: 0,0:09:29.06,0:09:33.32,Default#1,,0,0,0,,俺は鉄道管理局から依頼されて\N弁当を差し入れに来た者だ
Dialogue: 0,0:09:33.90,0:09:35.36,Default#1,,0,0,0,,へえ…
Dialogue: 0,0:09:35.95,0:09:38.57,Default#1,,0,0,0,,おい！　弁当だってよ
Dialogue: 0,0:09:38.66,0:09:41.91,Default#1,,0,0,0,,（にぎやかな声）
Dialogue: 0,0:09:42.54,0:09:46.71,Default#1,,0,0,0,,おう 詰め所で休んでるタツ坊(ぼう)に\N１個持ってってやんな
Dialogue: 0,0:09:47.21,0:09:48.50,Default#1,,0,0,0,,（作業員）はい 親方
Dialogue: 0,0:09:49.42,0:09:52.13,Default#1,,0,0,0,,無限列車は\Nなぜ ここへ移されたんだ？
Dialogue: 0,0:09:52.96,0:09:56.63,Default#1,,0,0,0,,（親方）フン…\N車体に原因があるわけねえのに
Dialogue: 0,0:09:56.72,0:10:00.43,Default#1,,0,0,0,,巷(ちまた)では人食い列車だなんだと\N噂されて
Dialogue: 0,0:10:00.51,0:10:02.72,Default#1,,0,0,0,,それが 俺たちは悔しいんだ
Dialogue: 0,0:10:03.22,0:10:06.23,Default#1,,0,0,0,,だから 運行再開が決まった今
Dialogue: 0,0:10:06.31,0:10:10.31,Default#1,,0,0,0,,精いっぱいの整備をして\N送り出してやろうと思ってな
Dialogue: 0,0:10:10.94,0:10:12.32,Default#1,,0,0,0,,運行再開？
Dialogue: 0,0:10:12.40,0:10:15.15,Default#1,,0,0,0,,ああ 明日の夜だ
Dialogue: 0,0:10:15.24,0:10:16.40,Default#1,,0,0,0,,だから みんな こうやって…
Dialogue: 0,0:10:17.99,0:10:19.74,Default#1,,0,0,0,,（作業員）うわあ～！
Dialogue: 0,0:10:20.62,0:10:23.49,Default#1,,0,0,0,,鬼ダ！　鬼ガ イルゾ！
Dialogue: 0,0:10:25.20,0:10:26.54,Default#1,,0,0,0,,ああっ…
Dialogue: 0,0:10:32.38,0:10:33.80,Default#1,,0,0,0,,（鬼）おっと
Dialogue: 0,0:10:35.80,0:10:36.80,Default#1,,0,0,0,,（親方）タツ坊！
Dialogue: 0,0:10:37.30,0:10:38.43,Default#1,,0,0,0,,下がっていろ
Dialogue: 0,0:10:38.51,0:10:40.14,Default#1,,0,0,0,,（タツ坊）親方！
Dialogue: 0,0:10:40.22,0:10:42.72,Default#1,,0,0,0,,（親方）野郎…\N（煉獄）少年を放せ！
Dialogue: 0,0:10:43.22,0:10:46.06,Default#1,,0,0,0,,（鬼）鬼狩りか 偉そうに
Dialogue: 0,0:10:46.56,0:10:49.69,Default#1,,0,0,0,,オエッ… なんだ このにおいは
Dialogue: 0,0:10:53.27,0:10:55.28,Default#1,,0,0,0,,鬼になってから こっち
Dialogue: 0,0:10:55.36,0:10:58.70,Default#1,,0,0,0,,人間の食い物が\N気色悪くてしかたがねえや
Dialogue: 0,0:10:59.36,0:11:02.66,Default#1,,0,0,0,,俺の知る人々が\N真心込めて作った弁当だ
Dialogue: 0,0:11:03.16,0:11:04.53,Default#1,,0,0,0,,聞き捨てならんな
Dialogue: 0,0:11:04.62,0:11:07.50,Default#1,,0,0,0,,まずいったら まずいんだよ
Dialogue: 0,0:11:07.58,0:11:11.58,Default#1,,0,0,0,,お前も まずそうな\N血のにおいがするぜ 小僧
Dialogue: 0,0:11:11.67,0:11:12.83,Default#1,,0,0,0,,（タツ坊）うう…
Dialogue: 0,0:11:14.71,0:11:17.46,Default#1,,0,0,0,,（鬼）\Nうまそうな人間は食っちまうが
Dialogue: 0,0:11:17.55,0:11:21.09,Default#1,,0,0,0,,まずいヤツは\N夜通し傷つけて楽しむんだ
Dialogue: 0,0:11:21.68,0:11:23.55,Default#1,,0,0,0,,（煉獄）\Nそういうことか 切り裂き魔
Dialogue: 0,0:11:24.72,0:11:26.18,Default#1,,0,0,0,,不愉快なヤツだ
Dialogue: 0,0:11:26.68,0:11:28.89,Default#1,,0,0,0,,悔しかったら 俺を…
Dialogue: 0,0:11:30.73,0:11:32.48,Default#1,,0,0,0,,斬ってみろよ！
Dialogue: 0,0:11:32.56,0:11:33.52,Default#1,,0,0,0,,なっ…
Dialogue: 0,0:11:34.02,0:11:35.32,Default#1,,0,0,0,,なんなんだ
Dialogue: 0,0:11:37.07,0:11:39.86,Default#1,,0,0,0,,（鬼）俺を止められたヤツはいねえ
Dialogue: 0,0:11:40.36,0:11:43.28,Default#1,,0,0,0,,俺が 一番 速い！
Dialogue: 0,0:11:45.20,0:11:46.62,Default#1,,0,0,0,,わりいな
Dialogue: 0,0:11:46.70,0:11:49.83,Default#1,,0,0,0,,このガキを抱えてたら\N斬りにくいよなあ
Dialogue: 0,0:11:49.91,0:11:52.46,Default#1,,0,0,0,,ひと思いに\N殺(や)っちまったほうがいいか？
Dialogue: 0,0:11:53.46,0:11:55.46,Default#1,,0,0,0,,（タツ坊）た… 助けて
Dialogue: 0,0:11:55.54,0:11:59.30,Default#1,,0,0,0,,心配するな\N君は必ず俺が助ける
Dialogue: 0,0:11:59.80,0:12:02.55,Default#1,,0,0,0,,（鬼）\Nできっこねえ　昨日だって…
Dialogue: 0,0:12:03.14,0:12:05.60,Default#1,,0,0,0,,（女）あ… ああ…
Dialogue: 0,0:12:07.01,0:12:10.89,Default#1,,0,0,0,,（鬼）別の女を\N飽きるまで切り刻んでやったぜ
Dialogue: 0,0:12:12.81,0:12:15.69,Default#1,,0,0,0,,（煉獄）\Nなるほど お前の速さは分かった
Dialogue: 0,0:12:16.44,0:12:18.28,Default#1,,0,0,0,,しかし 過信しないことだ
Dialogue: 0,0:12:19.36,0:12:22.49,Default#1,,0,0,0,,昨夜は 俺たちが来たために\N逃げたのかと思ったが
Dialogue: 0,0:12:22.99,0:12:26.41,Default#1,,0,0,0,,どうやら お前は感知することも\Nできていなかった様子
Dialogue: 0,0:12:28.66,0:12:31.58,Default#1,,0,0,0,,逃げ足だけは 確かに速いようだな
Dialogue: 0,0:12:31.66,0:12:32.96,Default#1,,0,0,0,,（鬼）なに？
Dialogue: 0,0:12:33.54,0:12:37.42,Default#1,,0,0,0,,（煉獄）昨夜の被害者も\N俺たちが迅速に処置した
Dialogue: 0,0:12:40.05,0:12:42.34,Default#1,,0,0,0,,傷痕も残らず回復するだろう
Dialogue: 0,0:12:43.80,0:12:46.97,Default#1,,0,0,0,,ハッ たとえ生き延びたとして
Dialogue: 0,0:12:47.05,0:12:51.22,Default#1,,0,0,0,,そいつは恐怖に 一生支配されて\N生きていくだろうよ
Dialogue: 0,0:12:51.31,0:12:52.81,Default#1,,0,0,0,,そうはさせん！
Dialogue: 0,0:12:53.31,0:12:55.10,Default#1,,0,0,0,,癒やし難き心の傷も
Dialogue: 0,0:12:55.60,0:12:58.73,Default#1,,0,0,0,,我々が長い時間をかけて\N手当てしていく！
Dialogue: 0,0:12:59.32,0:13:00.40,Default#1,,0,0,0,,フッ…
Dialogue: 0,0:13:00.48,0:13:04.11,Default#1,,0,0,0,,その正義漢ヅラに\N目に物を見せてやろうか
Dialogue: 0,0:13:04.20,0:13:07.74,Default#1,,0,0,0,,その女 もう一度 殺しに行ってやる
Dialogue: 0,0:13:07.82,0:13:12.20,Default#1,,0,0,0,,（煉獄）彼女の身柄は隠している\Nお前に見つけ出すことは不可能だ
Dialogue: 0,0:13:12.70,0:13:13.46,Default#1,,0,0,0,,（鬼）くっ…
Dialogue: 0,0:13:13.54,0:13:14.46,Default#1,,0,0,0,,ん？
Dialogue: 0,0:13:16.17,0:13:19.46,Default#1,,0,0,0,,ハッ じゃあ その弁当売りだ
Dialogue: 0,0:13:19.54,0:13:21.67,Default#1,,0,0,0,,（煉獄）\N俺とは関係のない人たちだ！
Dialogue: 0,0:13:22.17,0:13:23.92,Default#1,,0,0,0,,（鬼）知り合いなんだろう？
Dialogue: 0,0:13:24.01,0:13:27.30,Default#1,,0,0,0,,それ駅弁だろ　どこか分かるぜ
Dialogue: 0,0:13:27.80,0:13:29.18,Default#1,,0,0,0,,動くな
Dialogue: 0,0:13:29.76,0:13:31.72,Default#1,,0,0,0,,（煉獄）やはり お前は不愉快だ
Dialogue: 0,0:13:32.22,0:13:34.18,Default#1,,0,0,0,,（鬼）こっちは愉快だぜ
Dialogue: 0,0:13:34.27,0:13:38.44,Default#1,,0,0,0,,助けたいなら\N俺より早く駅まで来ることだ
Dialogue: 0,0:13:38.52,0:13:40.61,Default#1,,0,0,0,,その前に こいつは
Dialogue: 0,0:13:41.27,0:13:43.11,Default#1,,0,0,0,,殺しておくとする！
Dialogue: 0,0:13:43.82,0:13:44.78,Default#1,,0,0,0,,（煉獄）ふん！
Dialogue: 0,0:13:49.32,0:13:50.12,Default#1,,0,0,0,,くっ…
Dialogue: 0,0:13:53.54,0:13:55.12,Default#1,,0,0,0,,うう…
Dialogue: 0,0:13:56.46,0:13:57.83,Default#1,,0,0,0,,チッ…
Dialogue: 0,0:14:01.25,0:14:03.96,Default#1,,0,0,0,,ヘッ お先に
Dialogue: 0,0:14:06.01,0:14:07.93,Default#1,,0,0,0,,ハ～ッハア！
Dialogue: 0,0:14:08.64,0:14:11.05,Default#1,,0,0,0,,（親方）頼む… 助けてくれ
Dialogue: 0,0:14:11.68,0:14:13.85,Default#1,,0,0,0,,大丈夫だ 俺に任せろ
Dialogue: 0,0:14:14.35,0:14:16.89,Default#1,,0,0,0,,それに 俺の仲間も\Nすぐに駆けつける
Dialogue: 0,0:14:17.94,0:14:20.40,Default#1,,0,0,0,,（鎹鴉）コッチダ！\Nコッチダ！　コッチダ！
Dialogue: 0,0:14:22.52,0:14:23.57,Default#1,,0,0,0,,（隊士）煉獄様
Dialogue: 0,0:14:24.07,0:14:25.28,Default#1,,0,0,0,,医療班は？
Dialogue: 0,0:14:25.36,0:14:26.24,Default#1,,0,0,0,,向かっています
Dialogue: 0,0:14:26.74,0:14:29.61,Default#1,,0,0,0,,（煉獄）\N応急処置は済ませた　あとは頼む
Dialogue: 0,0:14:34.79,0:14:36.20,Default#1,,0,0,0,,（息を吐く音）
Dialogue: 0,0:14:41.92,0:14:43.84,Default#1,,0,0,0,,（煉獄）全集中の呼吸
Dialogue: 0,0:15:01.15,0:15:04.82,Default#1,,0,0,0,,まったく ふくは きかん子だねえ
Dialogue: 0,0:15:05.40,0:15:08.19,Default#1,,0,0,0,,手伝うったら手伝うの
Dialogue: 0,0:15:10.70,0:15:12.16,Default#1,,0,0,0,,おはようございます！
Dialogue: 0,0:15:13.16,0:15:15.83,Default#1,,0,0,0,,（駅員）やあ 今日も早いね
Dialogue: 0,0:15:22.29,0:15:23.42,Default#1,,0,0,0,,（においを嗅ぐ音）
Dialogue: 0,0:15:24.29,0:15:25.46,Default#1,,0,0,0,,におうぜ
Dialogue: 0,0:15:31.22,0:15:32.14,Default#1,,0,0,0,,（ふく）よし！
Dialogue: 0,0:15:34.64,0:15:35.81,Default#1,,0,0,0,,ん？
Dialogue: 0,0:15:37.43,0:15:38.81,Default#1,,0,0,0,,駅員さん？
Dialogue: 0,0:15:39.94,0:15:42.31,Default#1,,0,0,0,,（鬼）におう… におうぜ
Dialogue: 0,0:15:43.36,0:15:44.48,Default#1,,0,0,0,,お前だな
Dialogue: 0,0:15:44.56,0:15:47.98,Default#1,,0,0,0,,臭(くせ)え弁当のにおいが\N染みついていやがる
Dialogue: 0,0:15:48.69,0:15:49.40,Default#1,,0,0,0,,あ…
Dialogue: 0,0:15:49.49,0:15:51.24,Default#1,,0,0,0,,（鬼）お前に恨みはねえが
Dialogue: 0,0:15:51.82,0:15:54.24,Default#1,,0,0,0,,あの鬼狩りが 全部悪いのさ
Dialogue: 0,0:15:55.08,0:15:55.58,Default#1,,0,0,0,,ぐっ…
Dialogue: 0,0:15:56.53,0:15:58.54,Default#1,,0,0,0,,オエエエ…
Dialogue: 0,0:15:59.04,0:16:01.12,Default#1,,0,0,0,,逃げるんだよ ふく！
Dialogue: 0,0:16:01.21,0:16:03.92,Default#1,,0,0,0,,（せきこみ）
Dialogue: 0,0:16:04.00,0:16:06.13,Default#1,,0,0,0,,（鬼）何しやがる ババア
Dialogue: 0,0:16:11.01,0:16:13.22,Default#1,,0,0,0,,（ふく）ハア ハア…
Dialogue: 0,0:16:13.30,0:16:14.14,Default#1,,0,0,0,,うっ！
Dialogue: 0,0:16:19.60,0:16:21.85,Default#1,,0,0,0,,あっ ああ…
Dialogue: 0,0:16:22.60,0:16:24.02,Default#1,,0,0,0,,へッ…
Dialogue: 0,0:16:36.32,0:16:38.41,Default#1,,0,0,0,,あっ あなたは…
Dialogue: 0,0:16:39.45,0:16:40.41,Default#1,,0,0,0,,大事はないか？
Dialogue: 0,0:16:41.58,0:16:43.58,Default#1,,0,0,0,,あっ… おばあちゃん！
Dialogue: 0,0:16:46.67,0:16:47.84,Default#1,,0,0,0,,クソ…
Dialogue: 0,0:16:48.71,0:16:50.09,Default#1,,0,0,0,,あっ…
Dialogue: 0,0:16:51.72,0:16:52.80,Default#1,,0,0,0,,ああっ…
Dialogue: 0,0:16:52.88,0:16:55.76,Default#1,,0,0,0,,（鬼）\Nやい ババア さっきは よくも…
Dialogue: 0,0:17:06.65,0:17:10.28,Default#1,,0,0,0,,一体…\Nどうやって追いつきやがった
Dialogue: 0,0:17:14.24,0:17:16.20,Default#1,,0,0,0,,（煉獄）過信するなと言ったはずだ
Dialogue: 0,0:17:21.95,0:17:24.29,Default#1,,0,0,0,,試してみようじゃねえか
Dialogue: 0,0:17:24.37,0:17:27.67,Default#1,,0,0,0,,俺が このババアの喉を\N切り裂くのが先か
Dialogue: 0,0:17:27.75,0:17:30.71,Default#1,,0,0,0,,お前が俺の首を取るのが先か
Dialogue: 0,0:17:31.80,0:17:33.42,Default#1,,0,0,0,,試すには及ばない
Dialogue: 0,0:17:34.26,0:17:35.13,Default#1,,0,0,0,,お前は…
Dialogue: 0,0:17:37.93,0:17:39.05,Default#1,,0,0,0,,遅い！
Dialogue: 0,0:17:40.72,0:17:44.56,Default#1,,0,0,0,,炎の呼吸 壱ノ型(いちのかた) 不知火(しらぬい)
Dialogue: 0,0:17:46.02,0:17:46.98,Default#1,,0,0,0,,ああ…
Dialogue: 0,0:18:16.26,0:18:17.43,Default#1,,0,0,0,,（息を吐く音）
Dialogue: 0,0:18:29.48,0:18:33.86,Default#1,,0,0,0,,あなたは…\N救ってくださったのですね
Dialogue: 0,0:18:34.57,0:18:35.94,Default#1,,0,0,0,,二度も…
Dialogue: 0,0:18:38.78,0:18:40.16,Default#1,,0,0,0,,（ふく）おばあちゃん？
Dialogue: 0,0:18:40.24,0:18:45.08,Default#1,,0,0,0,,忘れもしません そのお顔 羽織…
Dialogue: 0,0:18:45.16,0:18:51.00,Default#1,,0,0,0,,私と… ふくの母親は
Dialogue: 0,0:18:51.08,0:18:55.59,Default#1,,0,0,0,,20年前\Nあなたに助けていただきました
Dialogue: 0,0:18:56.92,0:18:59.59,Default#1,,0,0,0,,（ふく）\Nおばあちゃん 何言ってるの？
Dialogue: 0,0:19:02.85,0:19:05.60,Default#1,,0,0,0,,それは きっと俺の父でしょう
Dialogue: 0,0:19:08.18,0:19:11.52,Default#1,,0,0,0,,俺は父を継いで\N鬼を狩っているのです
Dialogue: 0,0:19:12.61,0:19:15.94,Default#1,,0,0,0,,父と同じように\Nあなたをお守りできたこと
Dialogue: 0,0:19:16.03,0:19:17.11,Default#1,,0,0,0,,光栄です
Dialogue: 0,0:19:19.36,0:19:20.74,Default#1,,0,0,0,,ハア…
Dialogue: 0,0:19:21.49,0:19:23.03,Default#1,,0,0,0,,まだ信じられない
Dialogue: 0,0:19:23.78,0:19:27.87,Default#1,,0,0,0,,ごめんなさい おばあちゃん\N鬼がいるわけないなんて言って
Dialogue: 0,0:19:28.37,0:19:30.87,Default#1,,0,0,0,,ハハハハハ！
Dialogue: 0,0:19:32.13,0:19:34.09,Default#1,,0,0,0,,いや それでいいんだ
Dialogue: 0,0:19:34.59,0:19:39.55,Default#1,,0,0,0,,鬼を知らず 遭遇もせず\Nそれで天寿を全うできるなら
Dialogue: 0,0:19:39.63,0:19:40.88,Default#1,,0,0,0,,それが 一番だ
Dialogue: 0,0:19:44.14,0:19:46.35,Default#1,,0,0,0,,（隊士）炎柱！　大丈夫ですか！
Dialogue: 0,0:19:47.43,0:19:49.81,Default#1,,0,0,0,,何 今 片づいたところだ
Dialogue: 0,0:19:49.89,0:19:51.39,Default#1,,0,0,0,,ご苦労さまでした
Dialogue: 0,0:19:51.48,0:19:52.31,Default#1,,0,0,0,,うむ
Dialogue: 0,0:19:52.40,0:19:54.69,Default#1,,0,0,0,,それで 無限列車は…
Dialogue: 0,0:19:55.19,0:19:59.82,Default#1,,0,0,0,,今夜中に整備を整え\N明日から運行再開とのことだ
Dialogue: 0,0:20:00.40,0:20:03.66,Default#1,,0,0,0,,それはよかった\Nこれで万事解決ですね
Dialogue: 0,0:20:04.74,0:20:06.74,Default#1,,0,0,0,,（煉獄）それは性急すぎるな
Dialogue: 0,0:20:07.24,0:20:11.04,Default#1,,0,0,0,,40人以上も食った鬼が\Nこの程度であろうはずがない
Dialogue: 0,0:20:11.54,0:20:12.58,Default#1,,0,0,0,,（隊士）ハッ…
Dialogue: 0,0:20:12.67,0:20:16.13,Default#1,,0,0,0,,では 切り裂き魔は\N我々の目を攪乱(かくらん)するための？
Dialogue: 0,0:20:16.63,0:20:18.46,Default#1,,0,0,0,,あるいは そうなのかもな
Dialogue: 0,0:20:18.96,0:20:22.51,Default#1,,0,0,0,,いずれにせよ\N無限列車の鬼は別にいる
Dialogue: 0,0:20:23.26,0:20:27.56,Default#1,,0,0,0,,もっと強力な 得体(えたい)の知れない鬼が\Nどこかに潜んでいる
Dialogue: 0,0:20:29.22,0:20:31.18,Default#1,,0,0,0,,では 明日 無限列車に…
Dialogue: 0,0:20:31.68,0:20:33.44,Default#1,,0,0,0,,無論 乗り込む！
Dialogue: 0,0:20:34.81,0:20:36.98,Default#1,,0,0,0,,もう 今日だがな
Dialogue: 0,0:20:42.49,0:20:44.45,Default#1,,0,0,0,,（客）\Nおい おやっさん 聞いたかい？
Dialogue: 0,0:20:44.95,0:20:45.53,Default#1,,0,0,0,,（蕎麦屋）あん？
Dialogue: 0,0:20:46.03,0:20:48.20,Default#1,,0,0,0,,（客）無限列車が再運行だってよ
Dialogue: 0,0:20:49.20,0:20:50.62,Default#1,,0,0,0,,（蕎麦屋）へえ…
Dialogue: 0,0:20:51.20,0:20:55.04,Default#1,,0,0,0,,また妙なことが起きなきゃいいがな
Dialogue: 0,0:21:06.55,0:21:07.51,Default#1,,0,0,0,,（ふく）あの！
Dialogue: 0,0:21:08.01,0:21:09.81,Default#1,,0,0,0,,（煉獄）\Nやあ 弁当屋さん
Dialogue: 0,0:21:10.31,0:21:13.48,Default#1,,0,0,0,,今朝のこと なんて\Nお礼を言っていいか…
Dialogue: 0,0:21:14.10,0:21:15.81,Default#1,,0,0,0,,これをどうぞ
Dialogue: 0,0:21:15.90,0:21:19.77,Default#1,,0,0,0,,私らには\Nこんなものしかないのですが
Dialogue: 0,0:21:19.86,0:21:23.61,Default#1,,0,0,0,,（煉獄）\Nおお！　実は昨夜 食べ損ねてな
Dialogue: 0,0:21:23.70,0:21:25.36,Default#1,,0,0,0,,これは何よりうれしい
Dialogue: 0,0:21:26.11,0:21:28.78,Default#1,,0,0,0,,しかし 代金は払おう
Dialogue: 0,0:21:28.87,0:21:30.99,Default#1,,0,0,0,,いえ お気持ちだけで
Dialogue: 0,0:21:31.08,0:21:33.50,Default#1,,0,0,0,,そうか　では これは頂くとし
Dialogue: 0,0:21:34.00,0:21:36.46,Default#1,,0,0,0,,そこにある分を 全部買おう！
Dialogue: 0,0:21:36.54,0:21:38.17,Default#1,,0,0,0,,（ふく）ええ～！
Dialogue: 0,0:21:41.46,0:21:42.88,Default#1,,0,0,0,,（トミ）お気をつけて
Dialogue: 0,0:21:42.96,0:21:45.63,Default#1,,0,0,0,,（ふく）近くに来たら\Nまた寄ってください
Dialogue: 0,0:21:46.26,0:21:47.68,Default#1,,0,0,0,,（煉獄）\Nあなた方のことは
Dialogue: 0,0:21:47.76,0:21:49.26,Default#1,,0,0,0,,父に必ず伝えます
Dialogue: 0,0:21:49.76,0:21:51.22,Default#1,,0,0,0,,喜ぶことでしょう
Dialogue: 0,0:21:52.43,0:21:54.89,Default#1,,0,0,0,,では お元気で\Nまた会いましょう
Dialogue: 0,0:22:17.12,0:22:20.04,Default#1,,0,0,0,,（伊之助(いのすけ)）な… なっ なっ…
Dialogue: 0,0:22:20.13,0:22:22.63,Default#1,,0,0,0,,なんだ この生き物は！
Dialogue: 0,0:22:23.71,0:22:27.22,Default#1,,0,0,0,,はい 上等弁当\N36銭になります
Dialogue: 0,0:22:27.30,0:22:29.84,Default#1,,0,0,0,,お茶 お酒 ご一緒に\Nいかがですか？
Dialogue: 0,0:22:29.93,0:22:32.06,Default#1,,0,0,0,,（伊之助）猪突猛進(ちょとつもうしん)！\N（ふく）ん？
Dialogue: 0,0:22:33.06,0:22:35.02,Default#1,,0,0,0,,（善逸(ぜんいつ)）\Nやめろ 恥ずかしい！
Dialogue: 0,0:22:35.77,0:22:37.14,Default#1,,0,0,0,,（ふく）何かしら…
Dialogue: 0,0:22:46.61,0:22:49.41,Default#1,,0,0,0,,（汽笛）
Dialogue: 0,0:22:49.91,0:22:51.12,Default#1,,0,0,0,,（せきこみ）
Dialogue: 0,0:23:08.34,0:23:09.13,Default#1,,0,0,0,,（煉獄）うむ
Dialogue: 0,0:23:09.76,0:23:11.51,Default#1,,0,0,0,,では ありがたく いただこう
Dialogue: 0,0:23:23.02,0:23:24.23,Default#1,,0,0,0,,うまい！
Dialogue: 0,0:23:42.50,0:23:44.42,Default#1,,0,0,0,,（炭治郎(たんじろう)）無限列車に乗車できたぞ
Dialogue: 0,0:23:44.50,0:23:47.46,Default#1,,0,0,0,,（善逸）炭治郎 炎柱に\N聞きたいことがあるんだろ？
Dialogue: 0,0:23:47.55,0:23:49.51,Default#1,,0,0,0,,（善逸）会えるといいな\N（炭治郎）うん
Dialogue: 0,0:23:49.59,0:23:52.14,Default#1,,0,0,0,,柱(はしら)だと？　俺と勝負だ！
Dialogue: 0,0:23:52.64,0:23:54.51,Default#1,,0,0,0,,うるさいから お前は黙ってろ！
Dialogue: 0,0:23:55.26,0:23:57.27,Default#1,,0,0,0,,（伊之助）んだと こら！\N（善逸）なんだよ！
Dialogue: 0,0:23:57.77,0:24:00.48,Default#1,,0,0,0,,もう分かったから\N伊之助 落ち着こう
Dialogue: 0,0:24:00.56,0:24:01.27,Default#1,,0,0,0,,（伊之助）フンッ！
Dialogue: 0,0:24:01.77,0:24:03.15,Default#1,,0,0,0,,バカ猪(いのしし)が
Dialogue: 0,0:24:03.73,0:24:06.69,Default#1,,0,0,0,,ところで炎柱って\Nどんな人なんだろうな
Dialogue: 0,0:24:06.77,0:24:09.15,Default#1,,0,0,0,,（炭治郎）それは…\N（鎹鴉）カア～！　カア～！
Dialogue: 0,0:24:09.24,0:24:10.86,Default#1,,0,0,0,,鎹鴉だ
Dialogue: 0,0:24:11.36,0:24:13.82,Default#1,,0,0,0,,（鎹鴉）カア～ カア～\N（炭治郎）うん… うんうん
Dialogue: 0,0:24:14.32,0:24:17.99,Default#1,,0,0,0,,鎹鴉が煉獄さんについて\N調べてきてくれたみたいだ
Dialogue: 0,0:24:18.08,0:24:20.29,Default#1,,0,0,0,,（善逸）えっ ホント？\N（鎹鴉）カア～
Dialogue: 0,0:24:20.37,0:24:25.13,Default#1,,0,0,0,,ほかの柱の皆さんが 煉獄さんを\Nどう思っているか… だって
Dialogue: 0,0:24:25.21,0:24:26.46,Default#1,,0,0,0,,（善逸）聞きたい 聞きたい！
Dialogue: 0,0:24:26.54,0:24:29.46,Default#1,,0,0,0,,柱！　全員 勝負だ！
Dialogue: 0,0:24:30.05,0:24:32.68,Default#1,,0,0,0,,はいはい　炭治郎 教えてよ
Dialogue: 0,0:24:32.76,0:24:34.84,Default#1,,0,0,0,,うん　じゃあ しのぶさんから
Dialogue: 0,0:24:35.34,0:24:37.01,Default#1,,0,0,0,,（しのぶ）いい人ですね
Dialogue: 0,0:24:37.51,0:24:40.43,Default#1,,0,0,0,,話が噛(か)み合わないときが\Nありますけれど
Dialogue: 0,0:24:40.52,0:24:44.02,Default#1,,0,0,0,,しのぶさ～ん！\Nいい匂いを思い出しちゃうよ
Dialogue: 0,0:24:44.10,0:24:45.35,Default#1,,0,0,0,,ほかの柱は？
Dialogue: 0,0:24:45.44,0:24:47.27,Default#1,,0,0,0,,じゃあ 一気にいくよ！
Dialogue: 0,0:24:47.86,0:24:50.53,Default#1,,0,0,0,,（宇髄(うずい)）派手！　俺より目立つな
Dialogue: 0,0:24:50.61,0:24:52.53,Default#1,,0,0,0,,でも めっちゃいいヤツだ
Dialogue: 0,0:24:53.20,0:24:54.78,Default#1,,0,0,0,,（無一郎(むいちろう)）フクロウみたい
Dialogue: 0,0:24:55.28,0:24:57.87,Default#1,,0,0,0,,快活な声が心地よい人
Dialogue: 0,0:24:58.37,0:25:00.70,Default#1,,0,0,0,,（蜜璃(みつり)）\Nかっこいい おにい様！
Dialogue: 0,0:25:00.79,0:25:03.12,Default#1,,0,0,0,,一緒に修行して楽しかった
Dialogue: 0,0:25:03.21,0:25:05.04,Default#1,,0,0,0,,かわいがってもらいました
Dialogue: 0,0:25:05.63,0:25:08.96,Default#1,,0,0,0,,（伊黒(いぐろ)）\Nよく話す　性格がいい
Dialogue: 0,0:25:11.80,0:25:13.09,Default#1,,0,0,0,,（不死川(しなずがわ)）いいヤツだ
Dialogue: 0,0:25:14.13,0:25:17.35,Default#1,,0,0,0,,（悲鳴嶼(ひめじま)）前向き　弱音を吐かない
Dialogue: 0,0:25:17.89,0:25:19.43,Default#1,,0,0,0,,正直
Dialogue: 0,0:25:20.14,0:25:22.06,Default#1,,0,0,0,,（炭治郎）と…\Nこんな感じみたいだ
Dialogue: 0,0:25:22.56,0:25:24.89,Default#1,,0,0,0,,（善逸）\Nあとは 冨岡(とみおか)さんは？
Dialogue: 0,0:25:24.98,0:25:27.65,Default#1,,0,0,0,,（炭治郎）そうだ 鎹鴉\N冨岡さんは？
Dialogue: 0,0:25:28.15,0:25:29.36,Default#1,,0,0,0,,（鎹鴉）カア～！
Dialogue: 0,0:25:29.86,0:25:31.73,Default#1,,0,0,0,,（炭治郎）\Nうんうん… 分かった
Dialogue: 0,0:25:32.24,0:25:34.45,Default#1,,0,0,0,,冨岡さんは\N煉獄さんについて…
Dialogue: 0,0:25:36.28,0:25:38.28,Default#1,,0,0,0,,（冨岡）よく話しかけてくれる
Dialogue: 0,0:25:38.95,0:25:39.83,Default#1,,0,0,0,,…だって
Dialogue: 0,0:25:40.33,0:25:44.29,Default#1,,0,0,0,,へえ～ 炎柱 みんなに\N好かれてるみたいだな
Dialogue: 0,0:25:44.37,0:25:46.87,Default#1,,0,0,0,,うん　早く会えるといいな
Dialogue: 0,0:25:46.96,0:25:48.38,Default#1,,0,0,0,,（伊之助）うおお～！
Dialogue: 0,0:25:48.96,0:25:51.13,Default#1,,0,0,0,,炎柱 勝負だ！
Dialogue: 0,0:25:51.21,0:25:51.92,Default#1,,0,0,0,,伊之助！
Dialogue: 0,0:25:52.00,0:25:53.80,Default#1,,0,0,0,,ムギ～！　おっ…
Dialogue: 0,0:25:53.88,0:25:56.51,Default#1,,0,0,0,,あっ 煉獄さん！
Dialogue: 0,0:25:57.01,0:26:01.18,Default#1,,0,0,0,,次回「鬼滅(きめつ)の刃」無限列車編\N第２話「深い眠り」
Dialogue: 0,0:26:01.68,0:26:03.85,Default#1,,0,0,0,,みんな 一緒に乗車しよう！
Dialogue: 0,0:26:03.93,0:26:05.56,Default#1,,0,0,0,,（３人）アニキ～！
