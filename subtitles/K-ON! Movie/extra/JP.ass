Dialogue: 0,1:25:41.71,1:25:42.27,JP,,0,0,0,,	うん
Dialogue: 0,1:25:42.63,1:25:44.92,JP,,0,0,0,,	でもまあ あたしたちの教室だからさ
Dialogue: 0,1:25:45.28,1:25:50.37,JP,,0,0,0,,	あたしたちでやろうかななんて思ってたりするんだけど
Dialogue: 0,1:25:51.57,1:25:52.93,JP,,0,0,0,,	そんなのダメです
Dialogue: 0,1:25:53.61,1:25:55.12,JP,,0,0,0,,	私もやりたいです
Dialogue: 0,1:25:55.35,1:25:56.29,JP,,0,0,0,,	本当に
Dialogue: 0,1:25:56.55,1:25:57.16,JP,,0,0,0,,	はい
Dialogue: 0,1:25:58.16,1:26:02.73,JP,,0,0,0,,	先輩たちとみんなの前で演奏できるなんて もう最後じゃないですか
Dialogue: 0,1:26:03.05,1:26:04.33,JP,,0,0,0,,	しかも学校で
Dialogue: 0,1:26:04.70,1:26:05.84,JP,,0,0,0,,	よかった
Dialogue: 0,1:26:05.84,1:26:07.76,JP,,0,0,0,,	ありがとう あずにゃん
Dialogue: 0,1:26:08.51,1:26:11.33,JP,,0,0,0,,	で いつやるんですか 律先輩
Dialogue: 0,1:26:16.98,1:26:18.96,JP,,0,0,0,,	来た 最後の登校日
Dialogue: 0,1:26:19.81,1:26:22.00,JP,,0,0,0,,	あの子たちは私が守ってみせる
Dialogue: 0,1:26:25.79,1:26:27.24,JP,,0,0,0,,	もう一列いりそうね
Dialogue: 0,1:26:27.61,1:26:28.97,JP,,0,0,0,,	あ いいの
Dialogue: 0,1:26:29.33,1:26:29.99,JP,,0,0,0,,	あら
Dialogue: 0,1:26:30.56,1:26:32.10,JP,,0,0,0,,	あとで拭けばいいじゃん
Dialogue: 0,1:26:58.97,1:27:00.40,JP,,0,0,0,,	永久保存だね
Dialogue: 0,1:27:05.96,1:27:07.81,JP,,0,0,0,,	山中先生 お早いですね
Dialogue: 0,1:27:08.01,1:27:10.59,JP,,0,0,0,,	ええ いろいろやる事がありまして
Dialogue: 0,1:27:11.68,1:27:13.34,JP,,0,0,0,,	おや この音は
Dialogue: 0,1:27:13.44,1:27:16.63,JP,,0,0,0,,	え あ 工事でもしてるのかしら
Dialogue: 0,1:27:16.68,1:27:19.39,JP,,0,0,0,,	そうですか 景気のいい工事ですね
Dialogue: 0,1:27:21.57,1:27:22.46,JP,,0,0,0,,	なんてな
Dialogue: 0,1:27:22.46,1:27:24.34,JP,,0,0,0,,	この間こそこそ話してたのはこれか
Dialogue: 0,1:27:24.34,1:27:25.44,JP,,0,0,0,,	 あやしいと思ってたが
Dialogue: 0,1:27:25.44,1:27:26.48,JP,,0,0,0,,	待ってください
Dialogue: 0,1:27:27.81,1:27:28.52,JP,,0,0,0,,	ライブ
Dialogue: 0,1:27:28.73,1:27:29.38,JP,,0,0,0,,	うん
Dialogue: 0,1:27:30.19,1:27:31.32,JP,,0,0,0,,	見たい 見たい
Dialogue: 0,1:27:35.51,1:27:38.96,JP,,0,0,0,,	えっと じゃあこれで最後にします
Dialogue: 0,1:27:51.20,1:27:52.12,JP,,0,0,0,,	おはよう
Dialogue: 0,1:27:52.23,1:27:53.31,JP,,0,0,0,,	こっち こっち
Dialogue: 0,1:27:53.31,1:27:54.30,JP,,0,0,0,,	始まるよ
Dialogue: 0,1:28:15.96,1:28:18.04,JP,,0,0,0,,	先生 待ってください
Dialogue: 0,1:28:19.00,1:28:20.82,JP,,0,0,0,,	問答無用
Dialogue: 0,1:28:20.82,1:28:23.48,JP,,0,0,0,,	ホームルームまでに終わらせますから
Dialogue: 0,1:28:58.07,1:28:59.70,JP,,0,0,0,,	あとで一枚ちょうだい
Dialogue: 0,1:28:59.86,1:29:00.57,JP,,0,0,0,,	オッケー
Dialogue: 0,1:29:59.63,1:30:04.02,JP,,0,0,0,,	まあ 昔のお前たちに比べたら かわいいもんだな
Dialogue: 0,1:31:11.71,1:31:15.73,JP,,0,0,0,,	さあ あとはあずにゃんの歌を完成させるだけだよ
Dialogue: 0,1:31:15.91,1:31:18.57,JP,,0,0,0,,	だけって それが一番大事なことだろう
Dialogue: 0,1:31:18.97,1:31:21.30,JP,,0,0,0,,	そうだメロディーができたの
Dialogue: 0,1:31:21.40,1:31:22.50,JP,,0,0,0,,	聞かせて
Dialogue: 0,1:31:50.64,1:31:52.88,JP,,0,0,0,,	ムギ すごいぞこれ
Dialogue: 0,1:31:54.75,1:31:57.09,JP,,0,0,0,,	あとは歌詞がまとまれば完成だね
Dialogue: 0,1:31:57.38,1:31:58.04,JP,,0,0,0,,	うん
Dialogue: 0,1:31:59.63,1:32:03.48,JP,,0,0,0,,	あずにゃんと一緒にいた時間がたくさんあって困っちゃうね
Dialogue: 0,1:32:04.56,1:32:06.60,JP,,0,0,0,,	私たちの宝物だよ
Dialogue: 0,1:32:07.08,1:32:08.26,JP,,0,0,0,,	それ使おうよ
Dialogue: 0,1:32:11.95,1:32:12.89,JP,,0,0,0,,	じゃあな
Dialogue: 0,1:32:12.89,1:32:13.71,JP,,0,0,0,,	またな
Dialogue: 0,1:32:27.88,1:32:29.01,JP,,0,0,0,,	姉ちゃん ご飯
Dialogue: 0,1:32:32.14,1:32:33.42,JP,,0,0,0,,	何よ
Dialogue: 0,1:32:33.89,1:32:35.14,JP,,0,0,0,,	背 伸びたじゃん
Dialogue: 0,1:32:35.14,1:32:36.07,JP,,0,0,0,,	マジ
Dialogue: 0,1:32:54.51,1:32:55.27,JP,,0,0,0,,	できた
Dialogue: 0,1:32:56.34,1:32:57.11,JP,,0,0,0,,	できた
Dialogue: 0,1:32:58.28,1:33:00.46,JP,,0,0,0,,	よかった 間に合った
Dialogue: 0,1:33:00.94,1:33:04.50,JP,,0,0,0,,	唯 まさかとは思うけど このことはまだ梓に
Dialogue: 0,1:33:04.50,1:33:05.68,JP,,0,0,0,,	大丈夫だよ
Dialogue: 0,1:33:05.80,1:33:07.54,JP,,0,0,0,,	憂にも内緒にしてるもん
Dialogue: 0,1:33:08.99,1:33:10.06,JP,,0,0,0,,	なんかさ
Dialogue: 0,1:33:10.88,1:33:11.80,JP,,0,0,0,,	どうした
Dialogue: 0,1:33:12.39,1:33:14.09,JP,,0,0,0,,	ずっと変なんだよね
Dialogue: 0,1:33:15.35,1:33:16.62,JP,,0,0,0,,	先輩たち
Dialogue: 0,1:33:17.05,1:33:18.92,JP,,0,0,0,,	ずっと変じゃん ちょっと
Dialogue: 0,1:33:23.23,1:33:24.74,JP,,0,0,0,,	そうじゃなくて
Dialogue: 0,1:33:25.84,1:33:27.24,JP,,0,0,0,,	どこが変なの
Dialogue: 0,1:33:28.21,1:33:32.26,JP,,0,0,0,,	私がいくとね 急に何かしてたことをやめたりするの
Dialogue: 0,1:33:33.55,1:33:35.00,JP,,0,0,0,,	そりゃおかしいね
Dialogue: 0,1:33:35.31,1:33:37.35,JP,,0,0,0,,	何してたんですかって聞くと
Dialogue: 0,1:33:37.54,1:33:38.85,JP,,0,0,0,,	急に目を逸らして
Dialogue: 0,1:33:39.35,1:33:42.09,JP,,0,0,0,,	宇宙と交信してんじゃない
Dialogue: 0,1:33:43.04,1:33:43.95,JP,,0,0,0,,	まさか
Dialogue: 0,1:33:43.95,1:33:48.16,JP,,0,0,0,,	もしくは梓に隠れてすんごいお菓子食べてるとかね
Dialogue: 0,1:33:48.64,1:33:52.46,JP,,0,0,0,,	それは ありえるな
Dialogue: 0,1:33:54.08,1:33:56.38,JP,,0,0,0,,	なんかずっと隠してる感じで
Dialogue: 0,1:33:58.76,1:33:59.70,JP,,0,0,0,,	ね 憂
Dialogue: 0,1:34:00.16,1:34:05.48,JP,,0,0,0,,	まさかとは思うけど 唯先輩留年してないよね
Dialogue: 0,1:34:06.51,1:34:07.54,JP,,0,0,0,,	それはないよ
Dialogue: 0,1:34:07.85,1:34:08.78,JP,,0,0,0,,	本当に
Dialogue: 0,1:34:09.18,1:34:11.98,JP,,0,0,0,,	卒業式用のタイツ買うって言ってたもん
Dialogue: 0,1:34:12.13,1:34:13.07,JP,,0,0,0,,	そっか
Dialogue: 0,1:34:14.15,1:34:15.19,JP,,0,0,0,,	じゃあさあ
Dialogue: 0,1:34:15.43,1:34:17.34,JP,,0,0,0,,	やっぱり大学落ちてたとか
Dialogue: 0,1:34:18.25,1:34:19.57,JP,,0,0,0,,	ちゃんと確かめた
Dialogue: 0,1:34:20.80,1:34:22.18,JP,,0,0,0,,	じゃあ なんだろう
Dialogue: 0,1:34:28.93,1:34:30.91,JP,,0,0,0,,	大丈夫だよ 梓ちゃん
Dialogue: 0,1:34:32.39,1:34:41.36,JP,,0,0,0,,	でもね 会えたよ すてきなキミに
Dialogue: 0,1:34:46.57,1:34:49.65,JP,,0,0,0,,	なんかここの歌詞 物足りないな
Dialogue: 0,1:34:54.86,1:34:56.35,JP,,0,0,0,,	お姉ちゃん 入るよ
Dialogue: 0,1:34:56.99,1:34:58.05,JP,,0,0,0,,	はーい
Dialogue: 0,1:35:01.22,1:35:03.20,JP,,0,0,0,,	まだだよ
Dialogue: 0,1:35:07.30,1:35:09.06,JP,,0,0,0,,	もういいよ
Dialogue: 0,1:35:15.02,1:35:18.10,JP,,0,0,0,,	お姉ちゃん また頬っぺに弦の跡がつくよ
Dialogue: 0,1:35:18.10,1:35:19.66,JP,,0,0,0,,	あ そうだね
Dialogue: 0,1:35:22.31,1:35:25.01,JP,,0,0,0,,	あ 明日の準備できてるよね
Dialogue: 0,1:35:27.97,1:35:29.80,JP,,0,0,0,,	とうとう卒業か
Dialogue: 0,1:35:30.39,1:35:31.63,JP,,0,0,0,,	早く寝てね
Dialogue: 0,1:35:31.98,1:35:32.96,JP,,0,0,0,,	憂もね
Dialogue: 0,1:35:32.96,1:35:33.51,JP,,0,0,0,,	うん
Dialogue: 0,1:35:35.15,1:35:36.00,JP,,0,0,0,,	憂
Dialogue: 0,1:35:36.18,1:35:36.97,JP,,0,0,0,,	なに
Dialogue: 0,1:35:39.18,1:35:40.22,JP,,0,0,0,,	ありがとね
Dialogue: 0,1:35:42.34,1:35:45.60,JP,,0,0,0,,	私 ちゃんと卒業できるよ
Dialogue: 0,1:35:49.56,1:35:50.22,JP,,0,0,0,,	うん
Dialogue: 0,1:35:51.98,1:35:53.22,JP,,0,0,0,,	じゃあ おやすみ
Dialogue: 0,1:35:53.53,1:35:54.55,JP,,0,0,0,,	おやすみ
Dialogue: 0,1:35:55.01,1:35:56.54,JP,,0,0,0,,	あ そうだ 憂
Dialogue: 0,1:35:57.84,1:35:58.50,JP,,0,0,0,,	なに
Dialogue: 0,1:35:58.92,1:36:02.45,JP,,0,0,0,,	大学行っても みんなでお茶できるよね
Dialogue: 0,1:36:04.79,1:36:07.14,JP,,0,0,0,,	もちろん できると思うよ
Dialogue: 0,1:36:07.55,1:36:09.99,JP,,0,0,0,,	そっか よかった
Dialogue: 0,1:36:10.75,1:36:12.52,JP,,0,0,0,,	じゃあ 今度こそ おやすみ
Dialogue: 0,1:36:16.02,1:36:17.11,JP,,0,0,0,,	おやすみ
Dialogue: 0,1:36:31.09,1:36:39.95,JP,,0,0,0,,	でもね 会えたよ すてきな子猫に
Dialogue: 0,1:36:52.02,1:36:53.85,JP,,0,0,0,,	ほら 急げ急げ
Dialogue: 0,1:36:53.85,1:36:55.13,JP,,0,0,0,,	待って
Dialogue: 0,1:36:55.22,1:36:56.35,JP,,0,0,0,,	早く
Dialogue: 0,1:37:19.83,1:37:20.48,JP,,0,0,0,,	唯
Dialogue: 0,1:37:23.18,1:37:24.15,JP,,0,0,0,,	開いてたぞ
Dialogue: 0,1:37:54.81,1:37:56.05,JP,,0,0,0,,	ね りっちゃん
Dialogue: 0,1:37:56.05,1:37:56.86,JP,,0,0,0,,	うん
Dialogue: 0,1:37:57.68,1:37:59.71,JP,,0,0,0,,	うまく演奏できるかな
Dialogue: 0,1:38:00.57,1:38:04.98,JP,,0,0,0,,	私 なんだかいままでで一番緊張してるよ
Dialogue: 0,1:38:08.40,1:38:09.36,JP,,0,0,0,,	私も
Dialogue: 0,1:38:11.60,1:38:13.84,JP,,0,0,0,,	すごく手が冷たいの
Dialogue: 0,1:38:15.72,1:38:16.80,JP,,0,0,0,,	本当だ
Dialogue: 0,1:38:18.91,1:38:21.27,JP,,0,0,0,,	緊張するよな
Dialogue: 0,1:38:23.17,1:38:24.64,JP,,0,0,0,,	ヤバイ
Dialogue: 0,1:38:25.37,1:38:26.51,JP,,0,0,0,,	やっぱりやめる
Dialogue: 0,1:38:26.51,1:38:27.39,JP,,0,0,0,,	ダメだ
Dialogue: 0,1:38:28.06,1:38:29.85,JP,,0,0,0,,	梓ちゃん どう思うかしら
Dialogue: 0,1:38:29.85,1:38:31.73,JP,,0,0,0,,	喜ぶに決まってる
Dialogue: 0,1:38:34.54,1:38:38.02,JP,,0,0,0,,	よかった みんなドキドキしてたんだね
Dialogue: 0,1:38:38.28,1:38:39.58,JP,,0,0,0,,	当たりまえだ
Dialogue: 0,1:39:17.57,1:39:23.39,JP,,0,0,0,,	私が初めて りっちゃんたちの演奏聞いたのって『翼をください』だったよね
Dialogue: 0,1:39:24.02,1:39:26.71,JP,,0,0,0,,	それで四人で軽音部が始まって
Dialogue: 0,1:39:27.59,1:39:29.26,JP,,0,0,0,,	梓が入ってきてくれて
Dialogue: 0,1:39:29.60,1:39:31.49,JP,,0,0,0,,	すごくパワーアップしたよね
Dialogue: 0,1:39:32.14,1:39:34.86,JP,,0,0,0,,	梓のカッティングで音が締まるんだよな
Dialogue: 0,1:39:35.21,1:39:37.42,JP,,0,0,0,,	唯は力技だもんな
Dialogue: 0,1:39:37.42,1:39:38.59,JP,,0,0,0,,	お前が言うなよ
Dialogue: 0,1:39:39.37,1:39:44.56,JP,,0,0,0,,	そっか 私に翼をくれたのはあずにゃんなんだ
Dialogue: 0,1:39:46.67,1:39:50.00,JP,,0,0,0,,	あずにゃんは私たちを幸せにしてくれた
Dialogue: 0,1:39:50.12,1:39:52.49,JP,,0,0,0,,	ちっちゃくてかわいい天使なんだよ
Dialogue: 0,1:39:52.92,1:39:54.08,JP,,0,0,0,,	天使って
Dialogue: 0,1:39:54.08,1:39:55.55,JP,,0,0,0,,	そうかもしれないな
Dialogue: 0,1:39:59.77,1:40:00.43,JP,,0,0,0,,	なに
Dialogue: 0,1:40:02.15,1:40:12.14,JP,,0,0,0,,	でもね 会えたよ すてきな天使に
Dialogue: 0,1:40:12.81,1:40:13.90,JP,,0,0,0,,	どうかな
Dialogue: 0,1:40:13.90,1:40:14.79,JP,,0,0,0,,	すてき
Dialogue: 0,1:40:15.15,1:40:17.03,JP,,0,0,0,,	ちょっと恥しくないか
Dialogue: 0,1:40:17.03,1:40:18.99,JP,,0,0,0,,	いや すごくいいと思う
Dialogue: 0,1:40:19.13,1:40:21.82,JP,,0,0,0,,	この曲もあずにゃんの羽根になるかな
Dialogue: 0,1:40:23.80,1:40:26.42,JP,,0,0,0,,	なる と思いたい
Dialogue: 0,1:40:26.92,1:40:29.68,JP,,0,0,0,,	なるわ たくさん詰め込んだもの
Dialogue: 0,1:40:30.02,1:40:33.09,JP,,0,0,0,,	梓ちゃんへの気持ち いーっぱい
Dialogue: 0,1:40:35.37,1:40:37.22,JP,,0,0,0,,	気に入ってくれるといいな
Dialogue: 0,1:40:42.00,1:40:43.37,JP,,0,0,0,,	お茶にしようか
Dialogue: 0,1:40:43.63,1:40:44.43,JP,,0,0,0,,	そうだな
Dialogue: 0,1:40:49.29,1:40:51.00,JP,,0,0,0,,	紅白饅頭食べよう
Dialogue: 0,1:40:51.14,1:40:52.66,JP,,0,0,0,,	これうまいよな
Dialogue: 0,1:40:53.45,1:40:55.54,JP,,0,0,0,,	あ これあずにゃんの分ね
Dialogue: 0,1:40:57.73,1:40:59.81,JP,,0,0,0,,	なあ どうやって切り出す
Dialogue: 0,1:41:01.25,1:41:02.88,JP,,0,0,0,,	まずは部長のあたしから
Dialogue: 0,1:41:03.32,1:41:06.14,JP,,0,0,0,,	梓 聞いてほしい曲があるんだ
Dialogue: 0,1:41:07.81,1:41:08.97,JP,,0,0,0,,	次はムギかな
Dialogue: 0,1:41:09.26,1:41:12.04,JP,,0,0,0,,	梓ちゃんのために作ったのよ とか
Dialogue: 0,1:41:12.27,1:41:14.22,JP,,0,0,0,,	え うまく言えるかな
Dialogue: 0,1:41:14.22,1:41:16.44,JP,,0,0,0,,	笑ったりしちゃだめだよ ムギちゃん
Dialogue: 0,1:41:18.41,1:41:19.48,JP,,0,0,0,,	ちょっと待って
Dialogue: 0,1:41:20.57,1:41:23.00,JP,,0,0,0,,	この話をしてる時って いつも
Dialogue: 0,1:41:24.76,1:41:26.01,JP,,0,0,0,,	あ 梓
Dialogue: 0,1:41:26.78,1:41:27.90,JP,,0,0,0,,	すみません
Dialogue: 0,1:41:28.93,1:41:32.08,JP,,0,0,0,,	今日くらいは お茶入れようと思ってたんですけど
Dialogue: 0,1:41:33.81,1:41:35.08,JP,,0,0,0,,	ダメよ
Dialogue: 0,1:41:46.00,1:41:48.14,JP,,0,0,0,,	あずにゃん こっちにおいで
Dialogue: 0,1:42:32.43,1:42:33.49,JP,,0,0,0,,	そっか
Dialogue: 0,1:45:15.74,1:45:18.25,JP,,0,0,0,,	あずにゃん 喜んでくれてたよね
Dialogue: 0,1:45:18.32,1:45:19.03,JP,,0,0,0,,	うん
Dialogue: 0,1:45:20.24,1:45:22.30,JP,,0,0,0,,	あれ 秋山さん泣いてんの
Dialogue: 0,1:45:22.30,1:45:24.86,JP,,0,0,0,,	ちょ ちょっと鼻が詰まっただけだ
Dialogue: 0,1:45:25.02,1:45:27.18,JP,,0,0,0,,	泣いてる暇なんてないわよ 澪ちゃん
Dialogue: 0,1:45:27.38,1:45:30.26,JP,,0,0,0,,	そうだよ 私たち大学生だよ
Dialogue: 0,1:45:30.49,1:45:32.64,JP,,0,0,0,,	ポジティブだな お前ら
Dialogue: 0,1:45:32.99,1:45:36.64,JP,,0,0,0,,	お茶の時に食べるお菓子もちょっと大人っぽくしなきゃ
Dialogue: 0,1:45:36.64,1:45:37.58,JP,,0,0,0,,	ホント
Dialogue: 0,1:45:37.58,1:45:38.64,JP,,0,0,0,,	そこかよ
Dialogue: 0,1:45:40.50,1:45:41.97,JP,,0,0,0,,	あ 立ち直った
Dialogue: 0,1:45:42.10,1:45:46.82,JP,,0,0,0,,	そういえば あのデスデビルの曲も後輩のために作った曲なんだって
Dialogue: 0,1:45:46.82,1:45:47.87,JP,,0,0,0,,	そうなの
Dialogue: 0,1:45:47.99,1:45:50.40,JP,,0,0,0,,	人の感性ってそれぞれだよな
