Dialogue: 0,0:00:21.41,0:00:22.31,JP,,0000,0000,0000,,	あんこ姫
Dialogue: 0,0:00:34.80,0:00:36.54,JP,,0000,0000,0000,,	この娘 たまこ
Dialogue: 0,0:00:36.84,0:00:38.04,JP,,0000,0000,0000,,	餅屋の娘
Dialogue: 0,0:00:39.57,0:00:41.94,JP,,0000,0000,0000,,	ひょんなことから旅の途中で出会い
Dialogue: 0,0:00:42.45,0:00:45.54,JP,,0000,0000,0000,,	俺はしばらくここにいてやることになった
Dialogue: 0,0:00:48.80,0:00:49.81,JP,,0000,0000,0000,,	おはよう
Dialogue: 0,0:00:50.44,0:00:51.23,JP,,0000,0000,0000,,	おはよう
Dialogue: 0,0:00:58.20,0:00:59.52,JP,,0000,0000,0000,,	しかたないな
Dialogue: 0,0:01:00.18,0:01:01.79,JP,,0000,0000,0000,,{\pos(640,80)}1個だけだよ
Dialogue: 0,0:01:00.42,0:01:02.05,JP,,0000,0000,0000,,	そして この娘
Dialogue: 0,0:01:02.68,0:01:05.31,JP,,0000,0000,0000,,	俺に 惚れてる
Dialogue: 0,0:02:47.04,0:02:47.64,JP,,0000,0000,0000,,	よし
Dialogue: 0,0:02:48.17,0:02:50.37,JP,,0000,0000,0000,,	そろそろ朝ごはんの支度しよっかな
Dialogue: 0,0:02:51.10,0:02:51.80,JP,,0000,0000,0000,,	たまこ
Dialogue: 0,0:02:52.20,0:02:52.75,JP,,0000,0000,0000,,	はい
Dialogue: 0,0:02:52.98,0:02:54.88,JP,,0000,0000,0000,,	一番右のやつだけどけとけ
Dialogue: 0,0:02:58.75,0:03:00.20,JP,,0000,0000,0000,,	お爺ちゃん
Dialogue: 0,0:03:14.38,0:03:15.19,JP,,0000,0000,0000,,	おいしい
Dialogue: 0,0:03:16.03,0:03:17.01,JP,,0000,0000,0000,,	あっ そうだ
Dialogue: 0,0:03:18.31,0:03:20.59,JP,,0000,0000,0000,,	餡子餅汁ハート型バージョン
Dialogue: 0,0:03:21.40,0:03:22.77,JP,,0000,0000,0000,,	おいしそうだな
Dialogue: 0,0:03:23.96,0:03:25.15,JP,,0000,0000,0000,,	お父さんにあげよう
Dialogue: 0,0:03:25.57,0:03:26.59,JP,,0000,0000,0000,,	寒い
Dialogue: 0,0:03:26.60,0:03:28.16,JP,,0000,0000,0000,,	あんこ おはよう
Dialogue: 0,0:03:28.42,0:03:29.58,JP,,0000,0000,0000,,	早く着替えなよ
Dialogue: 0,0:03:29.58,0:03:31.17,JP,,0000,0000,0000,,	寒い
Dialogue: 0,0:03:34.31,0:03:38.00,JP,,0000,0000,0000,,	お姉ちゃん 制服こたつの中に入れないでよ
Dialogue: 0,0:03:38.30,0:03:39.67,JP,,0000,0000,0000,,	あんこのも入れといたよ
Dialogue: 0,0:03:39.85,0:03:41.57,JP,,0000,0000,0000,,	熟れた果実
Dialogue: 0,0:03:46.43,0:03:48.05,JP,,0000,0000,0000,,	もうやだ この鳥
Dialogue: 0,0:03:48.62,0:03:49.78,JP,,0000,0000,0000,,	いつまでいるの
Dialogue: 0,0:03:49.93,0:03:51.97,JP,,0000,0000,0000,,	春になるまではいてやろうか
Dialogue: 0,0:03:52.14,0:03:52.97,JP,,0000,0000,0000,,	春
Dialogue: 0,0:03:53.38,0:03:54.81,JP,,0000,0000,0000,,	なんで春までなんだよ
Dialogue: 0,0:03:54.99,0:03:55.99,JP,,0000,0000,0000,,	寒いからだ
Dialogue: 0,0:03:58.15,0:03:59.85,JP,,0000,0000,0000,,	朝のお餅入れといたよ
Dialogue: 0,0:03:59.93,0:04:02.59,JP,,0000,0000,0000,,	それに 春になると花が咲き
Dialogue: 0,0:04:02.59,0:04:04.62,JP,,0000,0000,0000,,	乙女の心が浮き立つであろう
Dialogue: 0,0:04:05.00,0:04:07.00,JP,,0000,0000,0000,,	春になるとな
Dialogue: 0,0:04:07.39,0:04:10.56,JP,,0000,0000,0000,,	すると 王子のお妃探しやすくなる
Dialogue: 0,0:04:11.54,0:04:12.07,JP,,0000,0000,0000,,	多分
Dialogue: 0,0:04:12.15,0:04:12.88,JP,,0000,0000,0000,,	王子
Dialogue: 0,0:04:13.05,0:04:14.59,JP,,0000,0000,0000,,	わが一族の王子だ
Dialogue: 0,0:04:14.84,0:04:17.92,JP,,0000,0000,0000,,	私はそのお妃を探すために旅立ったのだ
Dialogue: 0,0:04:22.36,0:04:24.83,JP,,0000,0000,0000,,	王子の妃はどこにいるのやら
Dialogue: 0,0:04:25.50,0:04:27.67,JP,,0000,0000,0000,,	ペットショップに連れていってあげようか
Dialogue: 0,0:04:28.21,0:04:29.02,JP,,0000,0000,0000,,	何だ そりゃ
Dialogue: 0,0:04:29.42,0:04:30.56,JP,,0000,0000,0000,,	乙女の宝庫か
Dialogue: 0,0:04:31.41,0:04:33.12,JP,,0000,0000,0000,,	デラさん的には そうかもね
Dialogue: 0,0:04:34.61,0:04:36.10,JP,,0000,0000,0000,,	お父さんこれ見て
Dialogue: 0,0:04:36.81,0:04:37.69,JP,,0000,0000,0000,,	何だ こりゃ
Dialogue: 0,0:04:38.19,0:04:39.86,JP,,0000,0000,0000,,	バレンタイン餅だよ
Dialogue: 0,0:04:40.37,0:04:43.27,JP,,0000,0000,0000,,	まった お前は 駄目だな
Dialogue: 0,0:04:43.41,0:04:45.76,JP,,0000,0000,0000,,	そんな浮ついたもちなんか売れるか
Dialogue: 0,0:04:45.98,0:04:49.43,JP,,0000,0000,0000,,	もっとたくさんの人にお餅食べてもらいたいんだよ
Dialogue: 0,0:04:49.83,0:04:51.41,JP,,0000,0000,0000,,	餅は餅でいいんだ
Dialogue: 0,0:04:52.93,0:04:55.93,JP,,0000,0000,0000,,	このままじゃ 世の中からね 取り残されちゃうから
Dialogue: 0,0:04:57.48,0:04:58.64,JP,,0000,0000,0000,,	バレンタイン
Dialogue: 0,0:04:58.71,0:05:02.72,JP,,0000,0000,0000,,	ランチタイムにはチョコレートを買いに来る女性たちで賑わうそうです
Dialogue: 0,0:05:03.06,0:05:04.03,JP,,0000,0000,0000,,	なるほど
Dialogue: 0,0:05:03.12,0:05:06.05,JP,,0000,0000,0000,,{\pos(640,80)}みんなさんのチョコで恋の花が咲きますように
Dialogue: 0,0:05:05.02,0:05:06.18,JP,,0000,0000,0000,,	なるほど なるほど
Dialogue: 0,0:05:07.82,0:05:09.00,JP,,0000,0000,0000,,	みどり
Dialogue: 0,0:05:10.09,0:05:10.74,JP,,0000,0000,0000,,	おはよっ
Dialogue: 0,0:05:12.40,0:05:13.11,JP,,0000,0000,0000,,	どうしたの
Dialogue: 0,0:05:13.27,0:05:14.23,JP,,0000,0000,0000,,	振られた
Dialogue: 0,0:05:14.35,0:05:17.08,JP,,0000,0000,0000,,	ええ もう じゃなくて マジで
Dialogue: 0,0:05:17.25,0:05:19.96,JP,,0000,0000,0000,,	うん バレンタイン目前で
Dialogue: 0,0:05:20.58,0:05:23.47,JP,,0000,0000,0000,,	おっしゃー ケーキバイキング行こう
Dialogue: 0,0:05:25.11,0:05:26.45,JP,,0000,0000,0000,,	誰にあげるの
Dialogue: 0,0:05:26.78,0:05:27.77,JP,,0000,0000,0000,,	内緒だよ
Dialogue: 0,0:05:28.25,0:05:30.10,JP,,0000,0000,0000,,	手作り やるじゃん
Dialogue: 0,0:05:35.14,0:05:36.18,JP,,0000,0000,0000,,	おっはよう
Dialogue: 0,0:05:36.79,0:05:38.32,JP,,0000,0000,0000,,	みどりちゃん おはー
Dialogue: 0,0:05:39.54,0:05:40.60,JP,,0000,0000,0000,,	何描いてんの
Dialogue: 0,0:05:40.87,0:05:41.62,JP,,0000,0000,0000,,	お餅
Dialogue: 0,0:05:42.06,0:05:44.96,JP,,0000,0000,0000,,	ああ 新作餅のアイディア出しね
Dialogue: 0,0:05:45.33,0:05:45.98,JP,,0000,0000,0000,,	そう
Dialogue: 0,0:05:47.51,0:05:48.73,JP,,0000,0000,0000,,	チョコはいいな
Dialogue: 0,0:05:49.14,0:05:51.85,JP,,0000,0000,0000,,	チョコだけで可愛らしい本になってるんだもん
Dialogue: 0,0:05:52.87,0:05:54.66,JP,,0000,0000,0000,,	なのに お餅ときたら
Dialogue: 0,0:05:55.09,0:05:56.68,JP,,0000,0000,0000,,	世知辛いね
Dialogue: 0,0:05:56.94,0:05:58.17,JP,,0000,0000,0000,,	世知辛いよ
Dialogue: 0,0:05:58.62,0:06:00.02,JP,,0000,0000,0000,,	おはよう
Dialogue: 0,0:06:00.73,0:06:02.35,JP,,0000,0000,0000,,	かんなちゃん おはよう
Dialogue: 0,0:06:02.77,0:06:03.65,JP,,0000,0000,0000,,	はよぅ
