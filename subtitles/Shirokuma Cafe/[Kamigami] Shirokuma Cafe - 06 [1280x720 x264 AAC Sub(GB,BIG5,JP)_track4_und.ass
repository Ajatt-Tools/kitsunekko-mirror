Dialogue: 0,0:00:58.81,0:01:01.23,白熊中文OP,NTP,0000,0000,0000,,
Dialogue: 0,0:01:01.23,0:01:02.73,白熊中文OP,NTP,0000,0000,0000,,
Dialogue: 0,0:01:02.73,0:01:06.20,白熊中文OP,NTP,0000,0000,0000,,
Dialogue: 0,0:01:06.20,0:01:10.03,白熊中文OP,NTP,0000,0000,0000,,
