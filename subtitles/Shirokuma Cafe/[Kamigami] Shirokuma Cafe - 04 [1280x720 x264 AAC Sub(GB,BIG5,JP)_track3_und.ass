Dialogue: 0,0:00:58.90,0:01:01.19,*Default,NTP,0000,0000,0000,,
Dialogue: 0,0:01:01.19,0:01:03.29,*Default,NTP,0000,0000,0000,,
Dialogue: 0,0:01:03.29,0:01:06.16,*Default,NTP,0000,0000,0000,,
